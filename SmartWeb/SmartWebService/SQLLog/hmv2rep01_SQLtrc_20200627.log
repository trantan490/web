         , MWIPMATDEF B  
         , MWIPOPRDEF C  
     WHERE A.FACTORY LIKE '%'
       AND A.FACTORY = B.FACTORY  
       AND A.FACTORY = C.FACTORY 
       AND A.OPER = C.OPER    
       AND A.MAT_ID = B.MAT_ID  
       AND A.MAT_VER = 1 
       AND A.MAT_VER = B.MAT_VER 
       AND A.LOT_DEL_FLAG = ' '  
       AND A.LOT_TYPE = 'W'  
  GROUP BY B.MAT_GRP_1,  ' ', B.MAT_GRP_3,  ' ',  ' ',  ' ',  ' ',  ' ', B.MAT_CMF_7 
   )  

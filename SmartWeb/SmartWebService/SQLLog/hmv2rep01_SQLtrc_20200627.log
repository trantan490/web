                THEN QTY_1 
                ELSE 0 END) S4 
         , SUM(CASE WHEN A.FACTORY LIKE '%' AND C.OPER NOT IN ('00001', '00002') 
                THEN QTY_1 
                ELSE 0 END) TOTAL 

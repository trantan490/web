     , DECODE(LOT.OPER_GROUP, 'HMK3A', QTY_1, 0) QTY_17
     , TRUNC(TO_DATE('20200627013949', 'YYYYMMDDHH24MISS') - TO_DATE(LOT.LOT_CMF_14,'YYYYMMDDHH24MISS'), 2) AS TTL_TAT 

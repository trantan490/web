                                                             , SUM(DECODE(PLAN_WEEK, '202028', D0_QTY, 0)) AS D7_QTY  
                                                             , SUM(DECODE(PLAN_WEEK, '202028', D1_QTY, 0)) AS D8_QTY  
                                                             , SUM(DECODE(PLAN_WEEK, '202028', D2_QTY, 0)) AS D9_QTY  

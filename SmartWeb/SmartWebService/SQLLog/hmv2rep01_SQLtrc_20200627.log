                                                                WHEN CALENDAR_ID = 'SE' AND TO_CHAR(TO_DATE('20200627', 'YYYYMMDD'), 'D') = 7 THEN D0_QTY + D1_QTY + D2_QTY + D3_QTY + D4_QTY 

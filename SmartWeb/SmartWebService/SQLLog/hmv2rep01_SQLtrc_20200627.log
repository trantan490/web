SELECT COUNT(*)
FROM RWEBFUNLOG
WHERE FACTORY = :FACTORY  
AND USER_ID = :USER_ID 
AND FUNC_NAME = :FUNC_NAME 
AND FUNC_GROUP = :FUNC_GROUP
AND VIEW_TIME BETWEEN TO_CHAR(SYSDATE - 10/1440, 'YYYYMMDDHH24MISS') AND TO_CHAR(SYSDATE + 10/1440, 'YYYYMMDDHH24MISS')
-------------------------------------------------------------
SELECT (SELECT DATA_1 FROM MGCMTBLDAT@RPTTOMES WHERE FACTORY = 'HMVA2' AND TABLE_NAME = 'H_CUSTOMER' AND KEY_1 = C.MAT_GRP_1) AS CUSTOMER, C.MAT_GRP_9, C.MAT_GRP_3,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' '
     , SUM(NVL(A.ORI_PLAN,0)) AS ""
     , SUM(NVL(A.MON_PLAN,0)) AS "Rev"
     , SUM(NVL(A.MON_PLAN,0)) -SUM(NVL(A.ORI_PLAN,0)) AS " "
     , ROUND(((SUM(NVL(A.MON_PLAN,0)) * 90) / 100),0) AS TARGET_MON 
     , SUM(NVL(A.SHIP_MON,0)) 
     , DECODE(SUM(NVL(A.MON_PLAN,0)),0,0, ROUND((SUM(NVL(A.SHIP_MON,0))/SUM(NVL(A.MON_PLAN,0)))*100,1)) AS JINDO
     , SUM(NVL(A.SHIP_MON,0)) - ROUND(((SUM(NVL(A.MON_PLAN,0)) * 90) / 100),1) AS DEF   
     , ROUND(((SUM(NVL(A.MON_PLAN,0)) - SUM(NVL(A.SHIP_MON,0))) /4), 1) AS TARGET_DAY 
     , SUM(NVL(A.RCV0,0)), SUM(NVL(A.RCV1,0)), SUM(NVL(A.RCV2,0)) 
     , SUM(NVL(A.T_IN0,0)), SUM(NVL(A.T_IN1,0)), SUM(NVL(A.T_IN2,0)) 
     , SUM(NVL(A.T_OUT0,0)), SUM(NVL(A.T_OUT1,0)), SUM(NVL(A.T_OUT2,0)) 
     , SUM(NVL(A.SHP0,0)), SUM(NVL(A.SHP1,0)), SUM(NVL(A.SHP2,0)) 
     , SUM(NVL(B.V0,0)) AS STOCK
     , SUM(NVL(B.V1+B.V2+B.V4+B.V5+B.V6,0)) AS TEST 
     , SUM(NVL(B.V3,0)) AS HOLD 
     , SUM(NVL(B.V7+B.V8+B.V9+B.V10+B.V11,0)) AS TEST_END 
     , SUM(NVL(B.V12,0)) AS HMK4T 
     , SUM(NVL(B.V0,0)) + SUM(NVL(B.V1+B.V2+B.V4+B.V5+B.V6,0)) + SUM(NVL(B.V3,0)) + SUM(NVL(B.V7+B.V8+B.V9+B.V10+B.V11+B.V12,0)) AS TTL 
     , SUM(NVL(A.FGS,0)) AS FGS 
     , SUM(NVL(A.WEEK_PLAN,0)) AS WEEK_PLAN 
     , SUM(NVL(A.SHIP_WEEK,0)) AS SHIP_WEEK 
     , SUM(NVL(A.WEEK_PLAN,0) - NVL(A.SHIP_WEEK,0)) AS WEEK_DEF 
     , SUM(NVL(A.WEEK_PLAN,0) - NVL(A.SHIP_WEEK,0)) - SUM(NVL(B.V0,0) + NVL(B.V1+B.V2+B.V4+B.V5+B.V6,0) + NVL(B.V3,0) + NVL(B.V7+B.V8+B.V9+B.V10+B.V11+B.V12,0)) AS AO_DEF 
     , SUM(NVL(A.T_OUT_MONTH,0)) AS T_OUT_MONTH
     , SUM(NVL(A.T_OUT_WEEK,0)) AS T_OUT_WEEK
     , SUM(NVL(A.PLAN1,0)) AS PLAN1
     , SUM(NVL(A.PLAN2,0)) AS PLAN2
     , SUM(NVL(B.V7+B.V8+B.V9,0)) AS TR_WAIT 
     , SUM(NVL(B.V10,0)) AS TR_ING 
     , SUM(NVL(B.V11,0)) AS TR_END 
     , SUM(NVL(A.TR_OUT0,0)) AS TR_OUT0, SUM(NVL(A.TR_OUT1,0)) AS TR_OUT1, SUM(NVL(A.TR_OUT2,0)) AS TR_OUT2 
     , SUM(NVL(A.SHIP_MON_FGS,0)) AS SHIP_MON_FGS 
     , SUM(NVL(A.SHIP_DAY_FGS,0)) AS SHIP_DAY_FGS 
  FROM ( 
       SELECT MAT.MAT_GRP_1, MAT.MAT_GRP_2, MAT.MAT_GRP_3, MAT.MAT_GRP_4, MAT.MAT_GRP_5, MAT.MAT_GRP_6, MAT.MAT_GRP_7, MAT.MAT_GRP_8, MAT.MAT_CMF_10, MAT.MAT_ID, MAT.MAT_CMF_7 
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(PLAN.RESV_FIELD2)/MAT.MAT_CMF_13,0), SUM(PLAN.RESV_FIELD2)) AS MON_PLAN
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(PLAN.PLAN_QTY_TEST)/MAT.MAT_CMF_13,0), SUM(PLAN.PLAN_QTY_TEST)) AS ORI_PLAN
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(SHP.SHIP_MON)/MAT.MAT_CMF_13,0), SUM(SHP.SHIP_MON)) AS SHIP_MON
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(SHP.SHP0)/MAT.MAT_CMF_13,0), SUM(SHP.SHP0)) AS SHP0
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(SHP.SHP1)/MAT.MAT_CMF_13,0), SUM(SHP.SHP1)) AS SHP1
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(SHP.SHP2)/MAT.MAT_CMF_13,0), SUM(SHP.SHP2)) AS SHP2
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(T_INOUT.RCV0)/MAT.MAT_CMF_13,0), SUM(T_INOUT.RCV0)) AS RCV0
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(T_INOUT.RCV1)/MAT.MAT_CMF_13,0), SUM(T_INOUT.RCV1)) AS RCV1
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(T_INOUT.RCV2)/MAT.MAT_CMF_13,0), SUM(T_INOUT.RCV2)) AS RCV2
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(T_INOUT.T_IN0)/MAT.MAT_CMF_13,0), SUM(T_INOUT.T_IN0)) AS T_IN0
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(T_INOUT.T_IN1)/MAT.MAT_CMF_13,0), SUM(T_INOUT.T_IN1)) AS T_IN1
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(T_INOUT.T_IN2)/MAT.MAT_CMF_13,0), SUM(T_INOUT.T_IN2)) AS T_IN2
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(T_INOUT.T_OUT0)/MAT.MAT_CMF_13,0), SUM(T_INOUT.T_OUT0)) AS T_OUT0
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(T_INOUT.T_OUT1)/MAT.MAT_CMF_13,0), SUM(T_INOUT.T_OUT1)) AS T_OUT1
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(T_INOUT.T_OUT2)/MAT.MAT_CMF_13,0), SUM(T_INOUT.T_OUT2)) AS T_OUT2
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(T_INOUT.T_OUT_MONTH)/MAT.MAT_CMF_13,0), SUM(T_INOUT.T_OUT_MONTH)) AS T_OUT_MONTH
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(T_INOUT.T_OUT_WEEK)/MAT.MAT_CMF_13,0), SUM(T_INOUT.T_OUT_WEEK)) AS T_OUT_WEEK
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(FGS.FGS)/MAT.MAT_CMF_13,0), SUM(FGS.FGS)) AS FGS
            , CASE WHEN MAT.MAT_GRP_1 = 'SE' AND MAT.MAT_GRP_3 = 'COB' THEN ROUND(SUM(NVL(W_PLAN.WEEK_PLAN,0) + NVL(SHP.T_SHIP_WEEK,0))/MAT.MAT_CMF_13,0) 
                   WHEN MAT.MAT_GRP_1 = 'SE' THEN SUM(NVL(W_PLAN.WEEK_PLAN,0) + NVL(SHP.T_SHIP_WEEK,0))
                   ELSE SUM(NVL(W_PLAN.WEEK_PLAN,0))
              END AS WEEK_PLAN
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(SHP.SHIP_WEEK)/MAT.MAT_CMF_13,0), SUM(SHP.SHIP_WEEK)) AS SHIP_WEEK 
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(W_PLAN.PLAN1)/MAT.MAT_CMF_13,0), SUM(W_PLAN.PLAN1)) AS PLAN1 
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(W_PLAN.PLAN2)/MAT.MAT_CMF_13,0), SUM(W_PLAN.PLAN2)) AS PLAN2 
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(T_INOUT.TR_OUT0)/MAT.MAT_CMF_13,0), SUM(T_INOUT.TR_OUT0)) AS TR_OUT0
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(T_INOUT.TR_OUT1)/MAT.MAT_CMF_13,0), SUM(T_INOUT.TR_OUT1)) AS TR_OUT1
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(T_INOUT.TR_OUT2)/MAT.MAT_CMF_13,0), SUM(T_INOUT.TR_OUT2)) AS TR_OUT2
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(SHP.SHIP_MON_FGS)/MAT.MAT_CMF_13,0), SUM(SHP.SHIP_MON_FGS)) AS SHIP_MON_FGS 
            , DECODE(MAT.MAT_GRP_3,'COB',ROUND(SUM(SHP.SHIP_DAY_FGS)/MAT.MAT_CMF_13,0), SUM(SHP.SHIP_DAY_FGS)) AS SHIP_DAY_FGS 
         FROM ( 
               SELECT DISTINCT MAT_GRP_1, MAT_GRP_2, MAT_GRP_3, MAT_GRP_4, MAT_GRP_5, MAT_GRP_6, MAT_GRP_7, MAT_GRP_8, MAT_CMF_10, MAT_ID, MAT_CMF_7, TO_NUMBER(DECODE(MAT_CMF_13, ' ', 1, '-', 1, MAT_CMF_13)) AS MAT_CMF_13 
                 FROM MWIPMATDEF 
                WHERE 1=1 
                  AND FACTORY  = 'HMVT2' 
                  AND MAT_TYPE= 'FG' 
                  AND DELETE_FLAG <> 'Y' 
              ) MAT 
            , ( 
               SELECT MAT_ID,PLAN_QTY_TEST,PLAN_MONTH, RESV_FIELD2  
                 FROM (  
                       SELECT MAT_ID,SUM(PLAN_QTY_TEST) AS PLAN_QTY_TEST,PLAN_MONTH, SUM(RESV_FIELD2) AS RESV_FIELD2  
                         FROM ( 
                               SELECT MAT_ID, SUM(PLAN_QTY_TEST) AS PLAN_QTY_TEST, PLAN_MONTH, SUM(TO_NUMBER(DECODE(RESV_FIELD2,' ',0,RESV_FIELD2))) AS RESV_FIELD2 
                                 FROM CWIPPLNMON 
                                WHERE 1=1 
                                  AND FACTORY  = 'HMVT2' 
                                  AND PLAN_MONTH = '202006' 
                                GROUP BY MAT_ID, PLAN_MONTH 
                              ) 
                        GROUP BY MAT_ID,PLAN_MONTH  
                      ) 
              ) PLAN 
            , ( 
               SELECT MAT_ID 
                    , SUM(WW_QTY) AS WEEK_PLAN 
                    , 0 AS PLAN1 
                    , 0 AS PLAN2 
                 FROM RWIPPLNWEK 
                WHERE 1=1 
                  AND FACTORY = 'HMVT2' 
                  AND GUBUN = '3' 
                  AND MAT_ID NOT LIKE 'SE%' 
                  AND PLAN_WEEK = '202026' 
                GROUP BY MAT_ID 
                UNION ALL 
               SELECT MAT_ID 
                    , SUM(CASE WHEN PLAN_DAY BETWEEN '20200622' AND '20200628' THEN PLAN_QTY ELSE 0 END) AS WEEN_PLAN
                    , SUM(DECODE(PLAN_DAY, '20200627', PLAN_QTY, 0)) AS PLAN1
                    , SUM(DECODE(PLAN_DAY, '20200628', PLAN_QTY, 0)) AS PLAN2
                 FROM CWIPPLNDAY 
                WHERE 1=1 
                  AND FACTORY = 'HMVT2' 
                  AND PLAN_DAY BETWEEN '20200622' AND '20200705' 
                  AND IN_OUT_FLAG = 'IN'
                  AND CLASS = 'SLIS'
                GROUP BY MAT_ID 
              ) W_PLAN 
            , ( 
               SELECT MAT_ID 
                    , SUM(SHIP_MON) AS SHIP_MON 
                    , SUM(SHIP_WEEK) AS SHIP_WEEK 
                    , SUM(T_SHIP_WEEK) AS T_SHIP_WEEK 
                    , SUM(SHP0) AS SHP0 
                    , SUM(SHP1) AS SHP1 
                    , SUM(SHP2) AS SHP2 
                    , SUM(SHIP_MON_FGS) AS SHIP_MON_FGS 
                    , SUM(SHIP_DAY_FGS) AS SHIP_DAY_FGS 
                 FROM ( 
                       SELECT MAT_ID 
                            , SUM(CASE WHEN WORK_DATE BETWEEN '20200601' AND '20200627' THEN (S1_FAC_OUT_QTY_1+S2_FAC_OUT_QTY_1+S3_FAC_OUT_QTY_1) ELSE 0 END) SHIP_MON
                            , SUM(CASE WHEN WORK_DATE BETWEEN '20200622' AND '20200627' THEN (S1_FAC_OUT_QTY_1+S2_FAC_OUT_QTY_1+S3_FAC_OUT_QTY_1) ELSE 0 END) SHIP_WEEK
                            , SUM(CASE WHEN WORK_DATE BETWEEN '20200622' AND '20200626' THEN (S1_FAC_OUT_QTY_1+S2_FAC_OUT_QTY_1+S3_FAC_OUT_QTY_1) ELSE 0 END) T_SHIP_WEEK
                            , SUM(CASE WHEN WORK_DATE = '20200625' THEN (S1_FAC_OUT_QTY_1+S2_FAC_OUT_QTY_1+S3_FAC_OUT_QTY_1) ELSE 0 END) SHP0
                            , SUM(CASE WHEN WORK_DATE = '20200626' THEN (S1_FAC_OUT_QTY_1+S2_FAC_OUT_QTY_1+S3_FAC_OUT_QTY_1) ELSE 0 END) SHP1
                            , SUM(CASE WHEN WORK_DATE = '20200627' THEN (S1_FAC_OUT_QTY_1+S2_FAC_OUT_QTY_1+S3_FAC_OUT_QTY_1) ELSE 0 END) SHP2
                            , 0 AS SHIP_MON_FGS, 0 AS SHIP_DAY_FGS
                         FROM RSUMFACMOV 
                        WHERE 1=1 
                          AND WORK_DATE BETWEEN '20200531' AND '20200627'
                          AND FACTORY = 'HMVA2'
                          AND CM_KEY_1 = 'HMKE1'
                        GROUP BY MAT_ID 
                       ) 
                 GROUP BY MAT_ID 
               ) SHP 
             , ( 
                SELECT MAT_ID 
                     , SUM(CASE WHEN WORK_DATE = '20200625' AND OPER = 'T0000' THEN S1_OPER_IN_QTY_1 + S2_OPER_IN_QTY_1 + S3_OPER_IN_QTY_1 ELSE 0 END) RCV0
                     , SUM(CASE WHEN WORK_DATE = '20200626' AND OPER = 'T0000' THEN S1_OPER_IN_QTY_1 + S2_OPER_IN_QTY_1 + S3_OPER_IN_QTY_1 ELSE 0 END) RCV1
                     , SUM(CASE WHEN WORK_DATE = '20200627' AND OPER = 'T0000' THEN S1_OPER_IN_QTY_1 + S2_OPER_IN_QTY_1 + S3_OPER_IN_QTY_1 ELSE 0 END) RCV2
                     , SUM(CASE WHEN WORK_DATE = '20200625' AND OPER IN ('T0100','T0400') THEN S1_OPER_IN_QTY_1 + S2_OPER_IN_QTY_1 + S3_OPER_IN_QTY_1 ELSE 0 END) T_IN0 
                     , SUM(CASE WHEN WORK_DATE = '20200626' AND OPER IN ('T0100','T0400') THEN S1_OPER_IN_QTY_1 + S2_OPER_IN_QTY_1 + S3_OPER_IN_QTY_1 ELSE 0 END) T_IN1 
                     , SUM(CASE WHEN WORK_DATE = '20200627' AND OPER IN ('T0100','T0400') THEN S1_OPER_IN_QTY_1 + S2_OPER_IN_QTY_1 + S3_OPER_IN_QTY_1 ELSE 0 END) T_IN2 
                     , SUM(CASE WHEN WORK_DATE = '20200625' AND OPER IN ('T0100','T0400') THEN S1_END_QTY_1 + S2_END_QTY_1 + S3_END_QTY_1 + S1_END_RWK_QTY_1 + S2_END_RWK_QTY_1 + S3_END_RWK_QTY_1 ELSE 0 END) T_OUT0
                     , SUM(CASE WHEN WORK_DATE = '20200626' AND OPER IN ('T0100','T0400') THEN S1_END_QTY_1 + S2_END_QTY_1 + S3_END_QTY_1 + S1_END_RWK_QTY_1 + S2_END_RWK_QTY_1 + S3_END_RWK_QTY_1 ELSE 0 END) T_OUT1
                     , SUM(CASE WHEN WORK_DATE = '20200627' AND OPER IN ('T0100','T0400') THEN S1_END_QTY_1 + S2_END_QTY_1 + S3_END_QTY_1 + S1_END_RWK_QTY_1 + S2_END_RWK_QTY_1 + S3_END_RWK_QTY_1 ELSE 0 END) T_OUT2
                     , SUM(CASE WHEN WORK_DATE BETWEEN '20200601' AND '20200627' AND OPER IN ('T0100','T0400')
                           THEN S1_END_QTY_1 + S2_END_QTY_1 + S3_END_QTY_1 + S1_END_RWK_QTY_1 + S2_END_RWK_QTY_1 + S3_END_RWK_QTY_1 
                           ELSE 0 END) T_OUT_MONTH 
                     , SUM(CASE WHEN WORK_DATE BETWEEN '20200622' AND '20200627' AND OPER IN ('T0100','T0400')
                           THEN S1_END_QTY_1 + S2_END_QTY_1 + S3_END_QTY_1 + S1_END_RWK_QTY_1 + S2_END_RWK_QTY_1 + S3_END_RWK_QTY_1 
                           ELSE 0 END) T_OUT_WEEK 
                     , SUM(CASE WHEN WORK_DATE = '20200625' AND OPER = 'T1200' THEN S1_END_QTY_1 + S2_END_QTY_1 + S3_END_QTY_1 + S1_END_RWK_QTY_1 + S2_END_RWK_QTY_1 + S3_END_RWK_QTY_1 ELSE 0 END) TR_OUT0
                     , SUM(CASE WHEN WORK_DATE = '20200626' AND OPER = 'T1200' THEN S1_END_QTY_1 + S2_END_QTY_1 + S3_END_QTY_1 + S1_END_RWK_QTY_1 + S2_END_RWK_QTY_1 + S3_END_RWK_QTY_1 ELSE 0 END) TR_OUT1
                     , SUM(CASE WHEN WORK_DATE = '20200627' AND OPER = 'T1200' THEN S1_END_QTY_1 + S2_END_QTY_1 + S3_END_QTY_1 + S1_END_RWK_QTY_1 + S2_END_RWK_QTY_1 + S3_END_RWK_QTY_1 ELSE 0 END) TR_OUT2
                  FROM RSUMWIPMOV 
                 WHERE 1=1 
                   AND FACTORY = 'HMVT2' 
                   AND MAT_VER = 1 
                   AND OPER IN ('T0000','T0100','T0400','T1200') 
                   AND LOT_TYPE = 'W' 
                   AND CM_KEY_2 = 'PROD' 
                   AND WORK_DATE BETWEEN '20200531' AND '20200627'
                 GROUP BY MAT_ID 
               ) T_INOUT  
             , ( 
                SELECT MAT_ID 
                     , SUM(QTY_1) AS FGS 
                  FROM RWIPLOTSTS 
                 WHERE 1=1 
                   AND FACTORY = 'FGS' 
                   AND MAT_VER = 1 
                   AND LOT_DEL_FLAG = ' ' 
                 GROUP BY MAT_ID 
               ) FGS  
         WHERE 1 = 1 
           AND MAT.MAT_ID = PLAN.MAT_ID(+) 
           AND MAT.MAT_ID = W_PLAN.MAT_ID(+) 
           AND MAT.MAT_ID = SHP.MAT_ID(+) 
           AND MAT.MAT_ID = T_INOUT.MAT_ID(+) 
           AND MAT.MAT_ID = FGS.MAT_ID(+) 
      GROUP BY MAT.MAT_GRP_1, MAT.MAT_GRP_2, MAT.MAT_GRP_3, MAT.MAT_GRP_4, MAT.MAT_GRP_5, MAT.MAT_GRP_6, MAT.MAT_GRP_7, MAT.MAT_GRP_8, MAT.MAT_CMF_10, MAT.MAT_ID, MAT.MAT_CMF_7, MAT.MAT_CMF_13
     ) A  
   , ( 
      SELECT LOT.MAT_ID, MAT.MAT_GRP_3 
           , SUM(DECODE(OPER_GRP_1, 'HMK3T', DECODE(MAT.MAT_GRP_3,'COB',ROUND(QTY/TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)),0),QTY), 0)) V0 
           , SUM(DECODE(OPER_GRP_1, 'TEST', DECODE(LOT_STATUS, 'WAIT', DECODE(HOLD_FLAG, ' ', DECODE(MAT.MAT_GRP_3,'COB',ROUND(QTY/TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)),0),QTY),0),0), 0)) V1 
           , SUM(DECODE(OPER_GRP_1, 'TEST', DECODE(LOT_STATUS, 'PROC', DECODE(HOLD_FLAG, ' ', DECODE(MAT.MAT_GRP_3,'COB',ROUND(QTY/TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)),0),QTY),0),0), 0)) V2 
           , SUM(DECODE(OPER_GRP_1, 'TEST', DECODE(HOLD_FLAG, 'Y', DECODE(MAT.MAT_GRP_3,'COB',ROUND(QTY/TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)),0),QTY),0), 0)) V3 
           , SUM(DECODE(OPER_GRP_1, 'QA1', DECODE(MAT.MAT_GRP_3,'COB',ROUND(QTY/TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)),0),QTY), 0)) V4 
           , SUM(DECODE(OPER_GRP_1, 'CAS', DECODE(MAT.MAT_GRP_3,'COB',ROUND(QTY/TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)),0),QTY), 0)) V5 
           , SUM(DECODE(OPER_GRP_1, 'OS', DECODE(MAT.MAT_GRP_3,'COB',ROUND(QTY/TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)),0),QTY), 0)) V6 
           , SUM(DECODE(OPER_GRP_1, 'QA2', DECODE(MAT.MAT_GRP_3,'COB',ROUND(QTY/TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)),0),QTY), 0)) V7 
           , SUM(DECODE(OPER_GRP_1, 'BAKE', DECODE(MAT.MAT_GRP_3,'COB',ROUND(QTY/TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)),0),QTY), 0)) V8 
           , SUM(DECODE(OPER_GRP_1, 'V/I', DECODE(MAT.MAT_GRP_3,'COB',ROUND(QTY/TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)),0),QTY), 0)) V9 
           , SUM(DECODE(OPER_GRP_1, 'TnR', DECODE(MAT.MAT_GRP_3,'COB',ROUND(QTY/TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)),0),QTY), 0)) V10
           , SUM(DECODE(OPER_GRP_1, 'P/K', DECODE(MAT.MAT_GRP_3,'COB',ROUND(QTY/TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)),0),QTY), 0)) V11 
           , SUM(DECODE(OPER_GRP_1, 'HMK4T', DECODE(MAT.MAT_GRP_3,'COB',ROUND(QTY/TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)),0),QTY), 0)) V12 
       FROM(  
             SELECT A.FACTORY, A.MAT_ID, B.OPER_GRP_1, B.OPER_GRP_7, B.OPER_CMF_3, SUM(A.QTY_1) QTY, HOLD_FLAG, LOT_STATUS 
               FROM RWIPLOTSTS A 
                  , MWIPOPRDEF B 
              WHERE 1 = 1 
                AND A.FACTORY = 'HMVT2' 
                AND A.MAT_VER = 1 
                AND A.LOT_DEL_FLAG = ' ' 
                AND A.FACTORY = B.FACTORY 
                AND A.OPER = B.OPER 
              GROUP BY A.MAT_ID, A.FACTORY, B.OPER_GRP_1, B.OPER_GRP_7, B.OPER_CMF_3, HOLD_FLAG, LOT_STATUS 
           ) LOT 
           , MWIPMATDEF MAT 
      WHERE 1 = 1 
        AND LOT.FACTORY = MAT.FACTORY 
        AND LOT.MAT_ID = MAT.MAT_ID 
        AND MAT.DELETE_FLAG <> 'Y' 
        AND MAT.MAT_GRP_2 <> '-' 
      GROUP BY LOT.MAT_ID ,MAT.MAT_GRP_3  
     ) B 
     , MWIPMATDEF C 
 WHERE 1 = 1 
   AND C.FACTORY = 'HMVT2' 
   AND A.MAT_ID = B.MAT_ID(+)
   AND A.MAT_ID = C.MAT_ID(+)
   AND C.DELETE_FLAG = ' ' 
   AND C.MAT_GRP_3 <> 'COB' 
           AND C.MAT_ID LIKE '%' 
 GROUP BY C.MAT_GRP_1, C.MAT_GRP_9, C.MAT_GRP_3,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' '
HAVING (
         NVL(SUM(A.MON_PLAN), 0)+ 
         NVL(SUM(A.ORI_PLAN), 0)+ 
         NVL(SUM(A.SHIP_MON), 0)+ 
         NVL(SUM(A.SHP0), 0)+ NVL(SUM(A.SHP1), 0)+ NVL(SUM(A.SHP2), 0)+ 
         NVL(SUM(A.RCV0), 0)+ NVL(SUM(A.RCV1), 0)+ NVL(SUM(A.RCV2), 0)+ 
         NVL(SUM(A.T_IN0), 0)+ NVL(SUM(A.T_IN1), 0)+ NVL(SUM(A.T_IN2), 0)+ 
         NVL(SUM(A.T_OUT0), 0)+ NVL(SUM(A.T_OUT1), 0)+ NVL(SUM(A.T_OUT2), 0)+ 
         NVL(SUM(B.V0), 0)+ NVL(SUM(B.V1), 0)+ NVL(SUM(B.V2), 0)+ NVL(SUM(B.V3), 0)+ NVL(SUM(B.V4), 0)+ 
         NVL(SUM(B.V5), 0)+ NVL(SUM(B.V6), 0)+ NVL(SUM(B.V7), 0)+ NVL(SUM(B.V8), 0)+ NVL(SUM(B.V9), 0)+ 
         NVL(SUM(B.V10), 0)+ NVL(SUM(B.V11), 0)+ NVL(SUM(B.V12), 0)+ NVL(SUM(A.FGS), 0)+ NVL(SUM(A.WEEK_PLAN), 0)+ 
         NVL(SUM(A.SHIP_WEEK), 0)+ NVL(SUM(A.PLAN1), 0)+ NVL(SUM(A.PLAN2), 0) 
       ) <> 0
 ORDER BY DECODE(C.MAT_GRP_1, 'SE', 1, 'IM', 2, 'FC', 3, 'AB', 4, 5), CUSTOMER, C.MAT_GRP_9, C.MAT_GRP_3,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' '


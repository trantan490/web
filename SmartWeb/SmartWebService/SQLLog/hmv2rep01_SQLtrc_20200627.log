                 WHERE 1=1 
                   AND FACTORY = 'HMVA2' 
                   AND PLAN_DAY BETWEEN '20200622' AND '20200628'
                   AND IN_OUT_FLAG = 'OUT'
                   AND CLASS = 'ASSY'

                     , SUM(DECODE(OPER, 'A0608', DECODE(WORK_DATE,'20200409', QTY,0))) AS WB18
                     , SUM(DECODE(OPER, 'A0608', DECODE(WORK_DATE,'20200410', QTY,0))) AS WB28 

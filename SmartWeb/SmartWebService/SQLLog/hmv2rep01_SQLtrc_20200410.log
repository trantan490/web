                     , SUM(DECODE(OPER, 'A0607', DECODE(WORK_DATE,'20200409', QTY,0))) AS WB17
                     , SUM(DECODE(OPER, 'A0607', DECODE(WORK_DATE,'20200410', QTY,0))) AS WB27 

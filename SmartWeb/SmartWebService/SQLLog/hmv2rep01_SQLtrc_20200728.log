                '' AS TO_LOT_ID, 
                '' AS START_RES_ID,  
                '' AS END_RES_ID,  
                '' AS HOLD_TIME, 
                '' REALEASE_TIME, 
                '' AS TRAN_COMMENT  
        FROM    RWIPLOTLSM 
        WHERE   1=1  
                AND LOT_ID = 'GAHK79.1-1201'  
                AND HIST_DEL_FLAG = ' '  
        )  
        UNION 
        ( 
        SELECT  HIST_SEQ, 
                2 AS SORT, 
                BONUS_CODE_1, 
                TO_CHAR(TO_DATE(TRAN_TIME,'YYYYMMDDHH24MISS')) AS TRAN_TIME, 
                '' AS FROM_QTY, 
                '' AS TO_QTY, 
                TO_CHAR(BONUS_QTY_1) AS TRAN_QTY, 
                '' AS FROM_OPER,  
                '' AS FROM_OPER_DESC, 
                '' AS TO_OPER , 
                '' AS TO_OPER_DESC, 
                '' AS FROM_FACTORY,  
                '' AS TO_FACTORY, 
                '' AS USER_DESC,  
                '' AS FROM_LOT_ID, 

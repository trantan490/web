                DECODE(FROM_TO_FLAG,'T',TO_CHAR(FROM_TO_QTY_1),'') AS FROM_QTY, 
                DECODE(FROM_TO_FLAG,'F',TO_CHAR(FROM_TO_QTY_1),'') AS TO_QTY, 
                '' AS TRAN_QTY, 
                '' AS FROM_OPER,  
                '' AS FROM_OPER_DESC, 
                '' AS TO_OPER , 
                '' AS TO_OPER_DESC, 
                '' AS FROM_FACTORY,  
                '' AS TO_FACTORY, 
                '' AS USER_DESC,  
                DECODE(FROM_TO_FLAG,'T',FROM_TO_LOT_ID,'') AS FROM_LOT_ID, 
                DECODE(FROM_TO_FLAG,'F',FROM_TO_LOT_ID,'') AS TO_LOT_ID, 
                '' AS START_RES_ID,  
                '' AS END_RES_ID,  
                '' AS HOLD_TIME, 
                '' REALEASE_TIME, 
                '' AS TRAN_COMMENT  
        FROM    RWIPLOTSPL 
        WHERE   1=1 
                AND LOT_ID = 'GAHK79.1-1201' 
                AND HIST_DEL_FLAG = ' ' 
        ) 
        UNION 
        ( 
        SELECT  HIST_SEQ, 
                2 AS SORT, 
                'MERGE' AS TRAN_CODE, 
                TO_CHAR(TO_DATE(TRAN_TIME,'YYYYMMDDHH24MISS')) AS TRAN_TIME, 
                DECODE(FROM_TO_FLAG,'T',TO_CHAR(FROM_TO_QTY_1),'') AS FROM_QTY, 

                '' AS START_RES_ID,  
                '' AS END_RES_ID,  
                '' AS HOLD_TIME, 
                '' REALEASE_TIME, 
                '' AS TRAN_COMMENT  
        FROM    RWIPLOTMRG 
        WHERE   1=1 
                AND LOT_ID = 'GAHK79.1-1201' 
                AND HIST_DEL_FLAG = ' ' 
        ) 
        UNION 
        ( 
        SELECT  HIST_SEQ, 
                2 AS SORT, 
                'COMBINE' AS TRAN_CODE, 
                TO_CHAR(TO_DATE(TRAN_TIME,'YYYYMMDDHH24MISS')) AS TRAN_TIME, 
                DECODE(FROM_TO_FLAG,'T',TO_CHAR(FROM_TO_QTY_1),'') AS FROM_QTY, 

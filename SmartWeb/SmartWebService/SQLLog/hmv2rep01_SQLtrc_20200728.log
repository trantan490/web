             , A.LOT_ID AS LOT_ID 
             , TO_CHAR(TO_DATE(C.CREATE_TIME,'yyyymmddhh24miss')) AS IN_TIME 
             , TO_CHAR(TO_DATE(A.TRAN_TIME,'yyyymmddhh24miss')) AS SHIP_TIME 
             , ROUND((TO_DATE(A.TRAN_TIME,'yyyymmddhh24miss') - TO_DATE(C.CREATE_TIME,'yyyymmddhh24miss')),2) AS TAT 
             , A.QTY_1 AS OUT_QTY 
             , ( 
                SELECT NVL(SUM(TOTAL_LOSS_QTY),0) 
                  FROM RWIPLOTLOS  
                 WHERE LOT_ID=A.LOT_ID  
                   AND FACTORY = 'HMVT2'  
                   AND HIST_DEL_FLAG = ' '  
               ) AS LOSS_QTY  
          FROM (
                SELECT MAT_ID 
                     , LOT_ID 
                     , SHIP_QTY_1 AS QTY_1  
                     , TRAN_TIME 
                  FROM VWIPSHPLOT    
                 WHERE 1=1    
                   AND FROM_FACTORY = 'HMVT2'  
                   AND OWNER_CODE = 'PROD'  
                   AND LOT_TYPE = 'W' 
                   AND FROM_OPER = ''
                   AND TRAN_TIME BETWEEN '20200727220000' AND '20200728215959' 
                   AND LOT_ID = '60BUDG-05'
               ) A
             , MWIPMATDEF MAT
             , RWIPLOTSTS C 
         WHERE 1=1
           AND A.MAT_ID=MAT.MAT_ID 
           AND A.LOT_ID=C.LOT_ID 
           AND MAT.FACTORY = 'HMVT2' 
           AND C.CREATE_CODE != 'RETN' 
           AND A.LOT_ID NOT IN 
           ( 
           SELECT  LOT_ID 
           FROM    RWIPLOTHIS 
           WHERE   1=1 
                   AND FACTORY = 'HMVT2' 
                   AND TRAN_CODE = 'CREATE' 
                   AND OPER = ''
                   AND HIST_DEL_FLAG = ' '
           ) 
     ) SUMMERY
   , (
     SELECT LOT_ID
          , LOSS_CODE
          , SUM(LOSS_QTY) AS LOSS_QTY 
       FROM RWIPLOTLSM
      WHERE 1=1
        AND FACTORY = 'HMVT2'  
        AND HIST_DEL_FLAG = ' '
   GROUP BY LOT_ID,LOSS_CODE,LOSS_QTY

SELECT  HIST_SEQ, 
        TRAN_CODE, 
        TRAN_TIME, 
        FROM_QTY, 
        TO_QTY, 
        TRAN_QTY, 
        FROM_OPER, 
        FROM_OPER_DESC, 
        TO_OPER, 
        TO_OPER_DESC, 
        FROM_FACTORY, 
        TO_FACTORY, 
        USER_DESC, 
        FROM_LOT_ID, 
        TO_LOT_ID, 
        START_RES_ID, 
        END_RES_ID, 
        HOLD_TIME, 
        REALEASE_TIME, 
        TRAN_COMMENT 
FROM    ( 
        (  
        SELECT  HIS.HIST_SEQ, 
                1 AS SORT,  
                HIS.TRAN_CODE,  
                TO_CHAR(TO_DATE(HIS.TRAN_TIME,'YYYYMMDDHH24MISS')) AS TRAN_TIME, 
                TO_CHAR(HIS.OLD_QTY_1) AS FROM_QTY,  
                TO_CHAR(HIS.QTY_1) AS TO_QTY, 
                '' AS TRAN_QTY, 
                HIS.OLD_OPER AS FROM_OPER,  
                OPR1.OPER_DESC AS FROM_OPER_DESC, 
                HIS.OPER AS TO_OPER, 
                OPR2.OPER_DESC AS TO_OPER_DESC, 
                HIS.OLD_FACTORY AS FROM_FACTORY, 
                HIS.FACTORY AS TO_FACTORY,  
                TRIM(USR.USER_DESC) || '(' || TRIM(HIS.TRAN_USER_ID) || ')' AS USER_DESC, 
                '' AS FROM_LOT_ID, 
                '' AS TO_LOT_ID,  
                HIS.START_RES_ID,  
                HIS.END_RES_ID,  
                '' AS HOLD_TIME, 
                '' REALEASE_TIME, 
                HIS.TRAN_COMMENT  
        FROM    RWIPLOTHIS HIS,  
                MWIPOPRDEF OPR1, 
                MWIPOPRDEF OPR2, 
                RWEBUSRDEF USR  
        WHERE   1=1  
                AND HIS.LOT_ID ='GAHK79.1-1201'  
                AND HIS.OLD_OPER = OPR1.OPER  
                AND HIS.OPER = OPR2.OPER 
                AND HIS.OLD_FACTORY = OPR1.FACTORY 
                AND HIS.FACTORY = OPR2.FACTORY 
                AND HIS.TRAN_USER_ID = USR.USER_ID  
                AND HIS.HIST_DEL_FLAG = ' '   
        )  
        UNION  
        (  
        SELECT  HIST_SEQ,  
                2 AS SORT, 
                LOSS_CODE AS TRAN_CODE,  
                TO_CHAR(TO_DATE(TRAN_TIME,'YYYYMMDDHH24MISS')) AS TRAN_TIME, 
                '' AS FROM_QTY, 
                '' AS TO_QTY, 
                TO_CHAR(LOSS_QTY) AS TRAN_QTY,  
                '' AS FROM_OPER,  
                '' AS FROM_OPER_DESC, 
                '' AS TO_OPER , 

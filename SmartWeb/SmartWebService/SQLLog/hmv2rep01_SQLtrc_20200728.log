        AND MAT.MAT_ID = LSM.MAT_ID
        AND MAT.DELETE_FLAG = ' '
        AND MAT.FACTORY = 'HMVA2'
        AND LSM.TRAN_TIME BETWEEN '20200721220000' AND '20200727215959'
        AND LSM.HIST_DEL_FLAG = ' '

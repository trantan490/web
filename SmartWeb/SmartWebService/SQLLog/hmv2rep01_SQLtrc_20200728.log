     , ROUND(SUM(OUT_HMKA3) / 1000000, 0) AS OUT_HMKA3
     , ROUND(SUM(OUT_CLOSE) / 1000000, 0) AS OUT_CLOSE
     , ROUND(SUM(OUT_SIG) / 1000000, 0) AS OUT_SIG
     , ROUND(SUM(OUT_MK) / 1000000, 0) AS OUT_MK
     , ROUND(SUM(OUT_MOLD) / 1000000, 0) AS OUT_MOLD
     , ROUND(SUM(OUT_WB9) / 1000000, 0) AS OUT_WB9
     , ROUND(SUM(OUT_DA9) / 1000000, 0) AS OUT_DA9
     , ROUND(SUM(OUT_WB8) / 1000000, 0) AS OUT_WB8
     , ROUND(SUM(OUT_DA8) / 1000000, 0) AS OUT_DA8
     , ROUND(SUM(OUT_WB7) / 1000000, 0) AS OUT_WB7
     , ROUND(SUM(OUT_DA7) / 1000000, 0) AS OUT_DA7
     , ROUND(SUM(OUT_WB6) / 1000000, 0) AS OUT_WB6
     , ROUND(SUM(OUT_DA6) / 1000000, 0) AS OUT_DA6
     , ROUND(SUM(OUT_WB5) / 1000000, 0) AS OUT_WB5
     , ROUND(SUM(OUT_DA5) / 1000000, 0) AS OUT_DA5
     , ROUND(SUM(OUT_WB4) / 1000000, 0) AS OUT_WB4
     , ROUND(SUM(OUT_DA4) / 1000000, 0) AS OUT_DA4
     , ROUND(SUM(OUT_WB3) / 1000000, 0) AS OUT_WB3
     , ROUND(SUM(OUT_DA3) / 1000000, 0) AS OUT_DA3
     , ROUND(SUM(OUT_WB2) / 1000000, 0) AS OUT_WB2
     , ROUND(SUM(OUT_DA2) / 1000000, 0) AS OUT_DA2
     , ROUND(SUM(OUT_WB1) / 1000000, 0) AS OUT_WB1
     , ROUND(SUM(OUT_DA1) / 1000000, 0) AS OUT_DA1
     , ROUND(SUM(OUT_DDS) / 1000000, 0) AS OUT_DDS
     , ROUND(SUM(OUT_SAW) / 1000000, 0) AS OUT_SAW
     , ROUND(SUM(OUT_BG) / 1000000, 0) AS OUT_BG
     , ROUND(SUM(OUT_STEALTH) / 1000000, 0) AS OUT_STEALTH
     , ROUND(SUM(OUT_PRI) / 1000000, 0) AS OUT_PRI

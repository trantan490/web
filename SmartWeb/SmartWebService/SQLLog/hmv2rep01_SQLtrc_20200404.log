                     , SUM(DECODE(OPER, 'A0409', DECODE(WORK_DATE,'20200403', QTY,0))) AS DA19
                     , SUM(DECODE(OPER, 'A0409', DECODE(WORK_DATE,'20200404', QTY,0))) AS DA29
                     , SUM(DECODE(OPER, 'A0602', DECODE(WORK_DATE,'20200402', QTY,0))) AS WB02

                     , SUM(DECODE(OPER, 'A0609', DECODE(WORK_DATE,'20200403', QTY,0))) AS WB19
                     , SUM(DECODE(OPER, 'A0609', DECODE(WORK_DATE,'20200404', QTY,0))) AS WB29 
                  FROM ( 

     , TRUNC(TO_DATE('20200714220000', 'YYYYMMDDHH24MISS') - TO_DATE(LOT.LOT_CMF_14,'YYYYMMDDHH24MISS'), 2) AS TTL_TAT 
     , TRUNC(TO_DATE('20200714220000', 'YYYYMMDDHH24MISS') - TO_DATE(LOT.OPER_IN_TIME,'YYYYMMDDHH24MISS'), 2) AS OPER_TAT 
  FROM (
        SELECT LOT.FACTORY, LOT.MAT_ID, LOT.LOT_CMF_4, LOT.LOT_ID, LOT.LOT_CMF_14, LOT.OPER_IN_TIME, LOT.CREATE_QTY_1, OPR.OPER_GRP_1 AS OPER_GROUP, LOT.OPER, LOT.QTY_1 
          FROM RWIPLOTSTS_BOH LOT, 
               MWIPOPRDEF OPR 
         WHERE LOT.CUTOFF_DT = '2020071422'
           AND LOT.FACTORY = OPR.FACTORY 
           AND LOT.OPER = OPR.OPER 
           AND LOT.FACTORY = 'HMVA2' 
           AND LOT.LOT_DEL_FLAG = ' ' 
           AND LOT.MAT_VER = 1 
           AND LOT.LOT_TYPE = 'W' 
       ) LOT 

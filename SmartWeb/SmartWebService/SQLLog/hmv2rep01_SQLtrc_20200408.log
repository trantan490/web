             , SUM(NVL(A.ASSY_MON,0)) AS ASSY_MON 
             , SUM(NVL(A.ASSY_MON,0)) - ROUND(((SUM(NVL(A.MON_PLAN,0)) * 23.3) / 100),1) AS DEF   
             , SUM(NVL(A.AO0,0)) AS AO0 

             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D7' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D7_TIME 
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D8' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D8_TIME 
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D9' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D9_TIME 
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D1' AND MAINT_STEP = 'D_WAIT' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D1_WAIT_TIME 
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D2' AND MAINT_STEP = 'D_WAIT' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D2_WAIT_TIME 
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D3' AND MAINT_STEP = 'D_WAIT' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D3_WAIT_TIME  
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D4' AND MAINT_STEP = 'D_WAIT' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D4_WAIT_TIME  
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D5' AND MAINT_STEP = 'D_WAIT' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D5_WAIT_TIME  
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D6' AND MAINT_STEP = 'D_WAIT' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D6_WAIT_TIME  
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D7' AND MAINT_STEP = 'D_WAIT' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D7_WAIT_TIME  
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D8' AND MAINT_STEP = 'D_WAIT' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D8_WAIT_TIME  
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D9' AND MAINT_STEP = 'D_WAIT' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D9_WAIT_TIME  
          FROM (
                SELECT MNT.*, RES.RES_GRP_1, RES.RES_GRP_2, RES.RES_GRP_3
                     , SUBSTR(MNT.MAINT_CODE, 1, 2) AS GROUP_CODE
                  FROM CSUMRESMNT@RPTTOMES MNT
                     , MRASRESDEF RES
                 WHERE 1=1
                   AND MNT.FACTORY = RES.FACTORY

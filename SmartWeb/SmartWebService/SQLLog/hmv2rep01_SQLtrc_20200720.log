                                                                WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200720', 'YYYYMMDD'), 'D') = 4 THEN D4_QTY 
                                                                WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200720', 'YYYYMMDD'), 'D') = 5 THEN D5_QTY 

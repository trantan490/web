                                                             , SUM(DECODE(PLAN_WEEK, '202030', WW_QTY, 0)) AS W1_QTY  
                                                             , SUM(DECODE(PLAN_WEEK, '202031', WW_QTY, 0)) AS W2_QTY  
                                                          FROM RWIPPLNWEK  

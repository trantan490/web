     , DECODE(LOT.OPER_GROUP, 'GATE', QTY_1, 0) QTY_6
     , DECODE(LOT.OPER_GROUP, 'MOLD', QTY_1, 0) QTY_7
     , DECODE(LOT.OPER_GROUP, 'CURE', QTY_1, 0) QTY_8

                                                                WHEN CALENDAR_ID = 'SE' AND TO_CHAR(TO_DATE('20200810', 'YYYYMMDD'), 'D') = 2 THEN D2_QTY 
                                                                WHEN CALENDAR_ID = 'SE' AND TO_CHAR(TO_DATE('20200810', 'YYYYMMDD'), 'D') = 3 THEN D3_QTY 

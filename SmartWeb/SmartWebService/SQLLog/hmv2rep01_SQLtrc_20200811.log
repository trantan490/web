SELECT SYS_DATE     , SUBSTR(SYS_DATE, 5, 2)||' '||SUBSTR(SYS_DATE, 7, 2)||'' AS CONV     , TO_CHAR(TO_DATE('20200811', 'YYYYMMDD'), 'DY') AS DY     , TO_CHAR(TO_DATE(SYS_DATE, 'YYYYMMDD'), 'DY') AS DY2  FROM MWIPCALDEF WHERE CALENDAR_ID = 'OTD'   AND (PLAN_YEAR, PLAN_WEEK) = (SELECT PLAN_YEAR, PLAN_WEEK FROM MWIPCALDEF WHERE CALENDAR_ID = 'OTD' AND SYS_DATE = '20200811')  ORDER BY SYS_DATE

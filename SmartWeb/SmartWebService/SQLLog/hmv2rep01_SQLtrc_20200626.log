SELECT NVL((SELECT DATA_1 FROM MGCMTBLDAT WHERE TABLE_NAME = 'H_CUSTOMER' AND KEY_1 = MAT.MAT_GRP_1 AND ROWNUM=1), '-') AS CUSTOMER,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ', MAT.MAT_CMF_10 AS PIN_TYPE,  ' ',  ' ', MAT.MAT_ID AS PRODUCT,  ' '
     , SUBSTR(MAT.MAT_ID, LENGTH(MAT.MAT_ID) - 2) AS PKG_CODE
     , ROUND(SUM(NVL(MON_PLN.MON_PLAN,0)) / 1000,0) AS MON_PLAN
     , ROUND(SUM(NVL(MON_AO.SHP_MONTH,0)) / 1000,0) AS SHP_MONTH
     , ROUND(SUM(NVL(MON_PLN.MON_PLAN,0) - NVL(MON_AO.SHP_MONTH,0)) / 1000,0) AS MON_DEF
     , ROUND(DECODE(SUM(NVL(MON_PLN.MON_PLAN,0)), 0, 0, SUM(NVL(MON_AO.SHP_MONTH,0)) / SUM(NVL(MON_PLN.MON_PLAN,0)) * 100), 0) AS JINDO
     , ROUND(SUM(NVL(MON_PLN.MON_PLAN,0)) / 4 / 1000 ,0) AS TARGET
     , ROUND(SUM(NVL(WEEK_PLN.D0_PLAN, 0) + (NVL(WEEK_PLN.WEEK1_TTL, 0) - NVL(MON_AO.SHP_WEEK_TTL, 0))) / 1000, 0) AS PLAN_TODAY
     , ROUND(SUM(NVL(WEEK_PLN.D0_PLAN, 0) + (NVL(WEEK_PLN.WEEK1_TTL, 0) - NVL(MON_AO.SHP_WEEK_TTL, 0)) - NVL(MON_AO.SHP_TODAY, 0)) / 1000, 0) AS DEF_TODAY
     , ROUND(SUM(NVL(MON_AO.SHP_TODAY, 0)) / 1000, 0) AS SHP_TODAY
     , ROUND(SUM(NVL(WIP.A1000,0)) / 1000,0) AS A1000
     , ROUND(SUM(NVL(WIP.A1100,0)) / 1000,0) AS A1100
     , ROUND(SUM(NVL(WIP.A1150,0)) / 1000,0) AS A1150
     , ROUND(SUM(NVL(WIP.A2300,0)) / 1000,0) AS A2300
     , ROUND(SUM(NVL(WIP.WIP_TTL,0)) / 1000,0) AS WIP_TOTAL
     , '' AS ETC0     
     , ROUND(SUM(NVL(MON_AO.SHP_YESTERDAY,0)) / 1000,0) AS SHP_YESTERDAY
     , '' AS ETC1
     , ROUND(SUM(NVL(MON_PLN.MON_PLAN,0) - NVL(MON_AO.SHP_MONTH,0)) / 4 / 1000,0) AS GOAL
     , '' AS ETC2
     , '' AS ETC3
     , ROUND(SUM(NVL(WIP.HOLD_WIP,0)) / 1000,0) AS HOLD_WIP
     , ROUND(SUM(NVL(WIP.WIP_TTL,0) - NVL(WIP.HOLD_WIP,0)) / 1000,0) AS FORECAST_SHP
     , ROUND(SUM((NVL(WIP.WIP_TTL,0) - NVL(WIP.HOLD_WIP,0)) * NVL(SUBSTR(MAT.MAT_GRP_4, 3),1)) / 1000,0) AS CHIP
  FROM MWIPMATDEF MAT 
     , ( 
        SELECT MAT_ID, SUM(RESV_FIELD1) AS MON_PLAN 
          FROM CWIPPLNMON 
         WHERE 1=1 
           AND FACTORY = 'HMVA2' 
           AND PLAN_MONTH = '202006' 
         GROUP BY MAT_ID 
       ) MON_PLN  
     , ( 
        SELECT MAT_ID 
             , SUM(CASE WHEN TO_CHAR(TO_DATE('20200626', 'YYYYMMDD'), 'D') = 1 THEN D0_QTY
                        WHEN TO_CHAR(TO_DATE('20200626', 'YYYYMMDD'), 'D') = 2 THEN D0_QTY + D1_QTY
                        WHEN TO_CHAR(TO_DATE('20200626', 'YYYYMMDD'), 'D') = 3 THEN D0_QTY + D1_QTY + D2_QTY
                        WHEN TO_CHAR(TO_DATE('20200626', 'YYYYMMDD'), 'D') = 4 THEN D0_QTY + D1_QTY + D2_QTY + D3_QTY
                        WHEN TO_CHAR(TO_DATE('20200626', 'YYYYMMDD'), 'D') = 5 THEN D0_QTY + D1_QTY + D2_QTY + D3_QTY + D4_QTY
                        WHEN TO_CHAR(TO_DATE('20200626', 'YYYYMMDD'), 'D') = 6 THEN D0_QTY + D1_QTY + D2_QTY + D3_QTY + D4_QTY + D5_QTY
                        ELSE 0
                   END) AS WEEK1_TTL 
             , SUM(CASE WHEN TO_CHAR(TO_DATE('20200626', 'YYYYMMDD'), 'D') = 7 THEN D0_QTY
                        WHEN TO_CHAR(TO_DATE('20200626', 'YYYYMMDD'), 'D') = 1 THEN D1_QTY
                        WHEN TO_CHAR(TO_DATE('20200626', 'YYYYMMDD'), 'D') = 2 THEN D2_QTY
                        WHEN TO_CHAR(TO_DATE('20200626', 'YYYYMMDD'), 'D') = 3 THEN D3_QTY
                        WHEN TO_CHAR(TO_DATE('20200626', 'YYYYMMDD'), 'D') = 4 THEN D4_QTY
                        WHEN TO_CHAR(TO_DATE('20200626', 'YYYYMMDD'), 'D') = 5 THEN D5_QTY
                        WHEN TO_CHAR(TO_DATE('20200626', 'YYYYMMDD'), 'D') = 6 THEN D6_QTY
                        ELSE 0
                   END) AS D0_PLAN 
          FROM ( 
                SELECT MAT_ID 
                     , SUM(DECODE(PLAN_WEEK, '202026', D0_QTY, 0)) AS D0_QTY 
                     , SUM(DECODE(PLAN_WEEK, '202026', D1_QTY, 0)) AS D1_QTY 
                     , SUM(DECODE(PLAN_WEEK, '202026', D2_QTY, 0)) AS D2_QTY 
                     , SUM(DECODE(PLAN_WEEK, '202026', D3_QTY, 0)) AS D3_QTY 
                     , SUM(DECODE(PLAN_WEEK, '202026', D4_QTY, 0)) AS D4_QTY 
                     , SUM(DECODE(PLAN_WEEK, '202026', D5_QTY, 0)) AS D5_QTY 
                     , SUM(DECODE(PLAN_WEEK, '202026', D6_QTY, 0)) AS D6_QTY 
                  FROM RWIPPLNWEK 
                 WHERE 1=1 
                   AND FACTORY = 'HMVA2' 
                   AND GUBUN = '3' 
                   AND PLAN_WEEK = '202026'
                 GROUP BY MAT_ID 
               ) 
         GROUP BY MAT_ID 
       ) WEEK_PLN 
     , (       
        SELECT MAT_ID 
             , SUM(DECODE(WORK_DATE, '20200625', NVL(SHP_QTY_1, 0), 0)) AS SHP_YESTERDAY 
             , SUM(DECODE(WORK_DATE, '20200626', NVL(SHP_QTY_1, 0), 0)) AS SHP_TODAY 
             , SUM(CASE WHEN WORK_DATE BETWEEN '20200620' AND '20200625' THEN NVL(SHP_QTY_1, 0) END) AS SHP_WEEK_TTL 
             , SUM(CASE WHEN WORK_DATE BETWEEN '20200601' AND '20200626' THEN NVL(SHP_QTY_1, 0) END) AS SHP_MONTH 
          FROM VSUMWIPOUT
         WHERE 1=1 
           AND FACTORY  = 'HMVA2' 
           AND CM_KEY_2 = 'PROD' 
           AND LOT_TYPE = 'W' 
           AND MAT_ID NOT LIKE 'HX%' 
           AND WORK_DATE BETWEEN LEAST('20200620', '20200601', '20200625') AND '20200626' 
         GROUP BY MAT_ID 
         UNION ALL 
        SELECT MAT_ID 
             , SUM(DECODE(WORK_DATE, '20200625', NVL(SHP_QTY_1, 0), 0)) AS SHP_YESTERDAY 
             , SUM(DECODE(WORK_DATE, '20200626', NVL(SHP_QTY_1, 0), 0)) AS SHP_TODAY 
             , SUM(CASE WHEN WORK_DATE BETWEEN '20200620' AND '20200625' THEN NVL(SHP_QTY_1, 0) END) AS SHP_WEEK_TTL 
             , SUM(CASE WHEN WORK_DATE BETWEEN '20200601' AND '20200626' THEN NVL(SHP_QTY_1, 0) END) AS SHP_MONTH 
          FROM VSUMWIPOUT_06
         WHERE 1=1 
           AND FACTORY  = 'HMVA2' 
           AND CM_KEY_2 = 'PROD' 
           AND LOT_TYPE = 'W' 
           AND MAT_ID LIKE 'HX%' 
           AND WORK_DATE BETWEEN LEAST('20200620', '20200601', '20200625') AND '20200626' 
         GROUP BY MAT_ID 
       ) MON_AO        
     , (
        SELECT MAT_ID
             , SUM(DECODE(OPER, 'A1000', QTY_1, 0)) AS A1000
             , SUM(DECODE(OPER, 'A1100', QTY_1, 0)) AS A1100
             , SUM(DECODE(OPER, 'A1150', QTY_1, 0)) AS A1150
             , SUM(DECODE(OPER, 'A2300', QTY_1, 0)) AS A2300
             , SUM(QTY_1) AS WIP_TTL
             , SUM(DECODE(HOLD_FLAG, 'Y', QTY_1, 0)) AS HOLD_WIP
          FROM RWIPLOTSTS
         WHERE 1=1
           AND FACTORY = 'HMVA2'
           AND LOT_TYPE = 'W'
           AND LOT_DEL_FLAG = ' '
           AND OPER BETWEEN 'A0950' AND 'AZ010'
         GROUP BY MAT_ID
       ) WIP
 WHERE 1=1
   AND MAT.MAT_ID = MON_PLN.MAT_ID(+)
   AND MAT.MAT_ID = WEEK_PLN.MAT_ID(+)
   AND MAT.MAT_ID = MON_AO.MAT_ID(+)
   AND MAT.MAT_ID = WIP.MAT_ID(+)
   AND MAT.FACTORY = 'HMVA2' 
   AND MAT.MAT_TYPE= 'FG' 
   AND MAT.DELETE_FLAG <> 'Y' 
 GROUP BY MAT.MAT_GRP_1,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ', MAT.MAT_CMF_10,  ' ',  ' ', MAT.MAT_ID,  ' ', SUBSTR(MAT.MAT_ID, LENGTH(MAT.MAT_ID) - 2)
 HAVING (
         SUM(NVL(MON_AO.SHP_MONTH,0)) + SUM(NVL(MON_AO.SHP_WEEK_TTL,0)) + SUM(NVL(MON_AO.SHP_YESTERDAY,0)) + 
         SUM(NVL(WIP.WIP_TTL,0)) + 
         SUM(NVL(MON_PLN.MON_PLAN,0))  + SUM(NVL(WEEK_PLN.D0_PLAN,0)) + SUM(NVL(WEEK_PLN.WEEK1_TTL,0))
         ) <> 0
 ORDER BY DECODE(MAT.MAT_GRP_1, 'SE', 1, 'HX', 2, 3), MAT.MAT_GRP_1,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ', MAT.MAT_CMF_10,  ' ',  ' ', MAT.MAT_ID,  ' '


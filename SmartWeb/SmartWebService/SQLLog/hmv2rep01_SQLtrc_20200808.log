                                                                WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200807', 'YYYYMMDD'), 'D') = 4 THEN D5_QTY 
                                                                WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200807', 'YYYYMMDD'), 'D') = 5 THEN D6_QTY 
                                                                WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200807', 'YYYYMMDD'), 'D') = 6 THEN D7_QTY 

   AND C.MAT_GRP_6 = '12'
   AND C.MAT_CMF_11 = 'KN7'
   AND C.MAT_ID = 'DRET520E_A20_87CUNKN7'
 GROUP BY A.RES_MODEL 
 ORDER BY A.RES_MODEL 


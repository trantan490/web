   AND C.MAT_GRP_10 = 'FLGA'
   AND C.MAT_GRP_6 = '12'
   AND C.MAT_CMF_11 = 'KN7'
   AND C.MAT_ID = 'DRET520L_A20_87CNNKN7'
   AND C.MAT_GRP_4 = 'FU'
   AND C.MAT_GRP_5 = '-'
 GROUP BY A.RES_MODEL 
 ORDER BY A.RES_MODEL 


                                                   AND CM_KEY_2 = 'PROD'  
                                                   AND LOT_TYPE = 'W'  
                                                   AND CM_KEY_3 LIKE 'P%'
                                                   AND WORK_DATE BETWEEN LEAST('20200801', '20200808', '20200803') AND '20200808' 
                                                 GROUP BY MAT_ID  
                                               ) SHP 
                                         WHERE 1=1 
                                           AND MAT.FACTORY = 'HMVA2' 

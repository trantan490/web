        SELECT FACTORY, RES_STS_2 AS MAT_ID, RES_STS_8 AS OPER, RES_GRP_6 AS RES_MODEL, RES_GRP_7 AS UPEH_GRP, SUB_AREA_ID, COUNT(RES_ID) AS RES_CNT 
             , COUNT(DECODE(RES_PRI_STS,'PROC',RES_ID)) AS RUN_COUNT 

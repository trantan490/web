AND VIEW_TIME BETWEEN TO_CHAR(SYSDATE - 10/1440, 'YYYYMMDDHH24MISS') AND TO_CHAR(SYSDATE + 10/1440, 'YYYYMMDDHH24MISS')
-------------------------------------------------------------
SELECT NVL((SELECT DATA_1 FROM MGCMTBLDAT WHERE TABLE_NAME = 'H_CUSTOMER' AND KEY_1 = MAT_GRP_1 AND ROWNUM=1), '-') AS MAT_GRP_1,  ' ', MAT_GRP_6, MAT_GRP_3,  ' ',  ' ',  ' ',  ' ',  ' ', MAT_ID,  ' ', MATCODE
     , SUM(PLAN_QTY_ASSY) AS PLAN_QTY_ASSY
     , STEPID, DESCRIPT, UNIT, USAGE
     , ROUND(SUM(SO_QTY), 1) AS SO_QTY
     , ROUND(SUM(DECODE(MAT_TYPE, 'TE', TE_USE_QTY, M_USE_QTY_1)), 1) AS USE_QTY
     , ROUND(CASE WHEN SUM(NVL(SO_QTY,0)) = 0 THEN 0
                      WHEN SUM(NVL(SO_QTY,0)) - SUM(NVL(DECODE(MAT_TYPE, 'TE', TE_USE_QTY, M_USE_QTY_1),0)) < 0 THEN 0
                      ELSE SUM(NVL(SO_QTY,0)) - SUM(NVL(DECODE(MAT_TYPE, 'TE', TE_USE_QTY, M_USE_QTY_1),0)) 
                 END, 1) AS REMAIN_QTY
     , ROUND((CASE WHEN SUM(NVL(SO_QTY,0)) = 0 THEN 0
                       WHEN SUM(NVL(SO_QTY,0)) - SUM(NVL(DECODE(MAT_TYPE, 'TE', TE_USE_QTY, M_USE_QTY_1),0)) < 0 THEN 0
                       ELSE SUM(NVL(SO_QTY,0)) - SUM(NVL(DECODE(MAT_TYPE, 'TE', TE_USE_QTY, M_USE_QTY_1),0)) 
                  END / (SELECT LAST_DAY(TO_DATE('20200808', 'YYYYMMDD')) - TO_DATE('20200808', 'YYYYMMDD') LASTDAY FROM DUAL)), 1) AS DAY_QTY
     , ROUND(SUM(AVG_USE_QTY), 1) AS AVG_USE_QTY
     , ROUND(DECODE(ROUND((CASE WHEN SUM(NVL(SO_QTY,0)) = 0 THEN 0
                       WHEN SUM(NVL(SO_QTY,0)) - SUM(NVL(DECODE(MAT_TYPE, 'TE', TE_USE_QTY, M_USE_QTY_1),0)) < 0 THEN 0
                       ELSE SUM(NVL(SO_QTY,0)) - SUM(NVL(DECODE(MAT_TYPE, 'TE', TE_USE_QTY, M_USE_QTY_1),0)) 
                       END / (SELECT LAST_DAY(TO_DATE('20200808', 'YYYYMMDD')) - TO_DATE('20200808', 'YYYYMMDD') LASTDAY FROM DUAL)), 1),0,0, 
                   ROUND(INV_QTY + L_IN, 1) / ROUND((CASE WHEN SUM(NVL(SO_QTY,0)) = 0 THEN 0
                                                          WHEN SUM(NVL(SO_QTY,0)) - SUM(NVL(DECODE(MAT_TYPE, 'TE', TE_USE_QTY, M_USE_QTY_1),0)) < 0 THEN 0
                                                          ELSE SUM(NVL(SO_QTY,0)) - SUM(NVL(DECODE(MAT_TYPE, 'TE', TE_USE_QTY, M_USE_QTY_1),0)) 
                                                          END / (SELECT LAST_DAY(TO_DATE('20200808', 'YYYYMMDD')) - TO_DATE('20200808', 'YYYYMMDD') LASTDAY FROM DUAL)), 1)),1) AS EXPIRY_DAY
     , ROUND(INV_QTY, 1) AS INV_QTY
     , ROUND(L_IN, 1) AS L_IN 
     , ROUND(INV_QTY + L_IN, 1) AS TTL
  FROM (
        SELECT MAT.MAT_GRP_1,  ' ', MAT.MAT_GRP_6, MAT.MAT_GRP_3,  ' ',  ' ',  ' ',  ' ',  ' ', MAT.MAT_ID,  ' ', MAT.MATCODE
             , PLAN_QTY_ASSY, STEPID, DESCRIPT, UNIT
             , PLAN_QTY_ASSY * (CASE WHEN MAT_TYPE <> 'TE' THEN DECODE(UNIT_QTY, 0, 1, UNIT_QTY / PAR_BASE_QTY)
                                     ELSE DECODE(NVL(MAT_CMF_13, ' '), ' ', 1, TO_NUMBER(REPLACE(MAT_CMF_13,',','')))
                                END) AS SO_QTY
             , CASE WHEN MAT_TYPE <> 'TE' THEN UNIT_QTY / PAR_BASE_QTY
                    ELSE DECODE(NVL(MAT_CMF_13, ' '), ' ', 0, TO_NUMBER(REPLACE(MAT_CMF_13,',','')))
               END AS USAGE                    
             , MAT_CMF_13, MAT_TYPE
             , CASE WHEN MAT_TYPE <> 'TE' THEN 0
                    ELSE (BG_QTY / DECODE(NVL(MAT_CMF_13, ' '), ' ', 1, TO_NUMBER(REPLACE(MAT_CMF_13,',',''))))
               END AS TE_USE_QTY
             , M_USE_QTY_1, ROUND(AVG_USE_QTY,2) AS AVG_USE_QTY, INV_QTY, L_IN 
          FROM (  
                SELECT A.MAT_GRP_1, A.MAT_GRP_2, A.MAT_GRP_3, A.MAT_GRP_4, A.MAT_GRP_5, A.MAT_GRP_6, A.MAT_GRP_7, A.MAT_GRP_8, A.MAT_GRP_9, A.MAT_CMF_7, A.MAT_CMF_10, A.MAT_CMF_13, A.MAT_ID  
                     , B.STEPID, B.MATCODE, B.DESCRIPT, B.PAR_BASE_QTY, B.UNIT_QTY, B.UNIT, B.RESV_FIELD_2 AS MAT_TYPE 
                  FROM MWIPMATDEF@RPTTOMES A  
                     , CWIPBOMDEF B  
                 WHERE 1=1  
                   AND A.MAT_ID = B.PARTNUMBER(+)  
                   AND A.FACTORY = 'HMVA2'  
                   AND A.MAT_TYPE = 'FG' 
                   AND A.DELETE_FLAG = ' ' 
                   AND B.RESV_FLAG_1(+) = 'Y'  
                   AND B.RESV_FIELD_2(+) <> 'WW'           
               ) MAT
             , (   
                 SELECT FACTORY,MAT_ID,PLAN_QTY_ASSY,PLAN_MONTH   
                   FROM (   
                          SELECT FACTORY, MAT_ID, SUM(PLAN_QTY_ASSY) AS PLAN_QTY_ASSY, PLAN_MONTH   
                            FROM CWIPPLNMON   
                           WHERE 1=1   
                             AND FACTORY = 'HMVA2'   
                             AND MAT_ID NOT LIKE 'SES%'   
                           GROUP BY FACTORY, MAT_ID, PLAN_MONTH 
                          UNION ALL   
                          SELECT FACTORY, MAT_ID, SUM(PLAN_QTY) AS PLAN_QTY_ASSY , '202008' AS PLAN_MONTH   
                            FROM (   
                                   SELECT FACTORY, MAT_ID, SUM(NVL(PLAN_QTY, 0)) AS PLAN_QTY 
                                     FROM CWIPPLNDAY 
                                    WHERE 1=1 
                                      AND FACTORY = 'HMVA2' 
                                      AND PLAN_DAY BETWEEN TO_CHAR(TRUNC(TO_DATE('20200808', 'YYYYMMDD'), 'MM'), 'YYYYMMDD') AND TO_CHAR(TRUNC(LAST_DAY(TO_DATE('20200808', 'YYYYMMDD'))), 'YYYYMMDD')  
                                      AND IN_OUT_FLAG = 'OUT' 
                                      AND CLASS = 'ASSY' 
                                   GROUP BY FACTORY, MAT_ID 
                                   UNION ALL 
                                   SELECT CM_KEY_1 AS FACTORY, MAT_ID
                                        , SUM(S1_FAC_OUT_QTY_1 + S2_FAC_OUT_QTY_1 + S3_FAC_OUT_QTY_1 + S4_FAC_OUT_QTY_1) AS PLAN_QTY   
                                     FROM RSUMFACMOV 
                                    WHERE 1=1 
                                      AND WORK_DATE BETWEEN TO_CHAR(TRUNC(TO_DATE('20200808', 'YYYYMMDD'), 'MM'), 'YYYYMMDD') AND TO_CHAR(TRUNC(TO_DATE('20200808', 'YYYYMMDD'), 'IW') - 1, 'YYYYMMDD')
                                      AND LOT_TYPE = 'W'
                                      AND CM_KEY_1 = 'HMVA2' 
                                      AND CM_KEY_3 LIKE 'P%' 
                                      AND MAT_ID LIKE 'SES%' 
                                   GROUP BY CM_KEY_1, MAT_ID 
                                 )  
                           WHERE 1=1   
                          GROUP BY FACTORY, MAT_ID   
                        )  
                  WHERE PLAN_MONTH = '202008'  
                    AND PLAN_QTY_ASSY > 0  
               ) PLN  
             , (  
                SELECT P_MAT_ID, M_MAT_ID, OPER, SUM(M_USE_QTY_1) AS M_USE_QTY_1   
                     , SUM(CASE WHEN WORK_DATE BETWEEN TO_CHAR(TO_DATE('20200808', 'YYYYMMDD') - 3, 'YYYYMMDD') AND TO_CHAR(TO_DATE('20200808', 'YYYYMMDD') - 1, 'YYYYMMDD') THEN M_USE_QTY_1  
                                ELSE 0  
                           END) / 3 AS AVG_USE_QTY 
                  FROM RSUMMATMOV  
                 WHERE 1=1  
                   AND WORK_DATE BETWEEN  TO_CHAR(TRUNC(TO_DATE('20200808', 'YYYYMMDD'), 'MM'), 'YYYYMMDD') AND TO_CHAR(TRUNC(LAST_DAY(TO_DATE('20200808', 'YYYYMMDD'))), 'YYYYMMDD')  
                 GROUP BY P_MAT_ID, M_MAT_ID, OPER 
               ) BOM  
             , (  
                SELECT MAT_ID, SUM(INV_QTY) AS INV_QTY, SUM(L_IN) AS L_IN  
                  FROM (  
                        SELECT ITEM_CD AS MAT_ID, SUM(INV_QTY) AS INV_QTY, 0 AS L_IN  
                          FROM IERPWMSSTS@RPTTOMES 
                         WHERE 1=1 
                           AND WH_CD IN ('MH01', 'B10')             
                           AND INV_QTY > 0 
                         GROUP BY ITEM_CD 
                         UNION ALL  
                        SELECT MAT_ID, 0, SUM(QTY_1) AS QTY_1  
                          FROM RWIPLOTSTS  
                         WHERE 1=1  
                           AND FACTORY = 'HMVA2'  
                           AND OPER LIKE 'V%'     
                           AND LOT_DEL_FLAG = ' '  
                           AND LOT_TYPE <> 'W'  
                         GROUP BY MAT_ID  
                       )  
                 GROUP BY MAT_ID  
               ) WIP  
             , (
                SELECT MAT_ID
                     , SUM(S1_END_QTY_1 + S2_END_QTY_1 + S3_END_QTY_1) AS BG_QTY             
                  FROM RSUMWIPMOV
                 WHERE 1=1
                   AND WORK_DATE BETWEEN TO_CHAR(TRUNC(TO_DATE('20200808', 'YYYYMMDD'), 'MM'), 'YYYYMMDD') AND TO_CHAR(TRUNC(LAST_DAY(TO_DATE('20200808', 'YYYYMMDD'))), 'YYYYMMDD')
                   AND LOT_TYPE = 'W'
                   AND CM_KEY_1 = 'HMVA2'           
                   AND OPER = 'A0040'
                 GROUP BY MAT_ID
               ) BGE
         WHERE 1=1  
           AND MAT.MAT_ID = PLN.MAT_ID(+)
           AND MAT.MAT_ID = BOM.P_MAT_ID(+)  
           AND MAT.MATCODE = BOM.M_MAT_ID(+)  
           AND MAT.STEPID = BOM.OPER(+)  
           AND MAT.MATCODE = WIP.MAT_ID(+) 
           AND MAT.MAT_ID = BGE.MAT_ID(+)
           AND (MAT.MAT_TYPE <> 'TE' OR (MAT.MAT_TYPE = 'TE' AND MAT.STEPID = 'A0040'))
           AND MAT.MAT_TYPE = 'MC'
       )
 WHERE 1=1  
   AND NVL(PLAN_QTY_ASSY,0) + NVL(SO_QTY,0) + NVL(TE_USE_QTY,0) + NVL(M_USE_QTY_1,0) > 0
 GROUP BY MAT_GRP_1,  ' ', MAT_GRP_6, MAT_GRP_3,  ' ',  ' ',  ' ',  ' ',  ' ', MAT_ID,  ' ', MATCODE, STEPID, DESCRIPT, UNIT, USAGE, INV_QTY, L_IN 
 ORDER BY MAT_GRP_1,  ' ', MAT_GRP_6, MAT_GRP_3,  ' ',  ' ',  ' ',  ' ',  ' ', MAT_ID,  ' ', MATCODE, STEPID 


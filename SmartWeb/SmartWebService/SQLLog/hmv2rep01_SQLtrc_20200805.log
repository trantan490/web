SELECT (SELECT DATA_1 FROM MGCMTBLDAT WHERE TABLE_NAME = 'H_CUSTOMER' AND KEY_1 = MAT.MAT_GRP_1 AND ROWNUM=1) AS MAT_GRP_1,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ', MAT.MAT_CMF_10, MAT.MAT_ID, LOT_ID
     , RES_ID, PARA_CNT, TEST_TIME, OPER, START_TIME, TRAN_TIME 
     , SUM(LOT_SIZE) AS LOT_SIZE 
     , SUM(TEST_SIZE) AS TEST_SIZE
     , AVG(RETEST_PER) AS RETEST_PER
     , SUM(OUT_PRIME) AS OUT_PRIME
     , SUM(OUT_FT) AS OUT_FT
     , AVG(YIELD_PRIME) AS YIELD_PRIME
     , AVG(YIELD_FT) AS YIELD_FT
     , AVG(YIELD_RETEST) AS YIELD_RETEST
     , AVG(CNT) AS CNT
     , (SELECT DECODE(OPER, ' ', 'SHIP', OPER) FROM RWIPLOTSTS WHERE LOT_ID = DAT.LOT_ID) AS STATUS
     , ROUND((SELECT (TO_DATE(TRAN_TIME, 'YYYYMMDDHH24MISS') - TO_DATE(START_TIME, 'YYYYMMDDHH24MISS')) * 24 * 60 FROM CWIPLOTEND WHERE LOT_ID = DAT.LOT_ID AND OLD_OPER = 'T1080' AND RWK_FLAG = ' '  AND HIST_DEL_FLAG = ' '),0) AS BAKE_TIME
     , ROUND((SELECT (TO_DATE(TRAN_TIME, 'YYYYMMDDHH24MISS') - TO_DATE(START_TIME, 'YYYYMMDDHH24MISS')) * 24 * 60 FROM CWIPLOTEND WHERE LOT_ID = DAT.LOT_ID AND OLD_OPER IN ('T0100', 'T0400') AND RWK_FLAG = ' ' AND HIST_DEL_FLAG = ' '),0) AS FT_TIME
     , (SELECT LAST_COMMENT FROM CWIPLOTEND WHERE FACTORY = 'HMKT1' AND OLD_OPER IN ('T0100','T0400') AND LOT_DEL_FLAG = ' ' AND LOT_ID = DAT.LOT_ID AND ROWNUM = 1) AS LAST_COMMENT
     , (SELECT LOT_CMF_7 FROM RWIPLOTSTS WHERE LOT_ID = DAT.LOT_ID) AS ASSY_SITE
  FROM MWIPMATDEF MAT  
     , (  
        SELECT MAT_ID, LOT_ID, RES_ID, OPER, TRAN_TIME 
             , MAX(RETEST_INQTY) AS LOT_SIZE 
             , SUM(RETEST_INQTY) AS TEST_SIZE 
             , MAX(RETEST_GOODQTY) AS OUT_PRIME 
             , SUM(RETEST_GOODQTY) AS OUT_FT 
             , ROUND(SUM(RETEST_INQTY) / MAX(RETEST_INQTY), 2) AS RETEST_PER  
             , MIN(RETEST_YIELD) AS YIELD_PRIME 
             , MAX(RETEST_YIELD) AS YIELD_FT
             , MAX(RETEST_YIELD) - MIN(RETEST_YIELD) AS YIELD_RETEST 
             , COUNT(*) - 1 AS CNT 
             , ROW_NUMBER() OVER(PARTITION BY LOT_ID ORDER BY TRAN_TIME DESC) AS ROW_NUM  
             , ( 
                SELECT MAX(TRAN_TIME) 
                  FROM RWIPLOTHIS 
                 WHERE FACTORY = 'HMKT1' 
                   AND HIST_DEL_FLAG = ' ' 
                   AND TRAN_CODE = 'START' 
                   AND OPER IN ('T0100','T0400') 
                   AND LOT_ID = A.LOT_ID 
               ) AS START_TIME 
          FROM MWIPLOTRTT@RPTTOMES A
         WHERE 1=1 
           AND FACTORY = 'HMKT1'
           AND RES_ID LIKE '%'
           AND TRAN_TIME BETWEEN '20200804220000' AND '20200805215959'
           AND RETEST_YIELD > 0
           AND OPER IN ('T0100','T0400')
         GROUP BY MAT_ID, LOT_ID, RES_ID, OPER, TRAN_TIME 
       ) DAT 
     , (  
        SELECT * 
          FROM ( 
                SELECT ATTR_KEY AS MAT_ID 
                     , DECODE(ATTR_NAME, 'TEST_TIME_QUAD', '4', 'TEST_TIME_DUAL', '2', '1') AS PARA_CNT 
                     , ATTR_VALUE AS TEST_TIME 
                     , RANK() OVER(PARTITION BY ATTR_KEY ORDER BY ATTR_NAME DESC) AS RNK 
                  FROM MATRNAMSTS 
                 WHERE 1=1 
                   AND FACTORY = 'HMKT1'
                   AND ATTR_TYPE = 'MAT_TEST_PGM'
                   AND ATTR_NAME LIKE 'TEST_TIME%'
                   AND ATTR_VALUE <> ' '
               ) 
         WHERE RNK = 1 
       ) ATT 
 WHERE 1=1 
   AND DAT.MAT_ID = MAT.MAT_ID 
   AND DAT.MAT_ID = ATT.MAT_ID(+) 
   AND MAT.FACTORY = 'HMKT1' 
   AND MAT.MAT_ID LIKE '%' 
   AND DAT.ROW_NUM = '1' 
 GROUP BY MAT.MAT_GRP_1,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ', MAT.MAT_CMF_10, MAT.MAT_ID, LOT_ID, RES_ID, PARA_CNT, TEST_TIME, OPER, START_TIME, TRAN_TIME 
 ORDER BY MAT.MAT_GRP_1,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ', MAT.MAT_CMF_10, MAT.MAT_ID, LOT_ID, RES_ID, PARA_CNT, TEST_TIME, OPER, START_TIME, TRAN_TIME 


  FROM MWIPOPRDEF 
 WHERE 1=1 
   AND FACTORY='HMVA2'
   AND DECODE(OPER_CMF_2, ' ', 99999, TO_NUMBER(OPER_CMF_2)) BETWEEN (SELECT OPER_CMF_2 FROM MWIPOPRDEF WHERE OPER = 'A0005') AND (SELECT OPER_CMF_2 FROM MWIPOPRDEF WHERE OPER = 'A2350') 
   AND OPER_CMF_4 <> ' ' 

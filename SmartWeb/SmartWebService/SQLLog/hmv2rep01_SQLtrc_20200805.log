                                   AND LOT_TYPE = 'W'
                                   AND REGEXP_LIKE(OPER, 'A040*|A0033|A0040|A0230|A1000|A0950|A060*|A1750|A0030|A0200|A0000|A0020|AZ010|A0250|A050*|A053*|A055*|A1825')
                                   AND WORK_DATE BETWEEN '20200801' AND '20200831'
                                   AND SHIFT = '3'
                                   AND CM_KEY_3 LIKE 'P%' 
                               ) A
                             , (
                                SELECT *
                                  FROM (
                                        SELECT MAT_ID, MAT_GRP_2, MAT_GRP_4, MAT_GRP_5, MAT_GRP_6, MAT_CMF_11
                                             , CASE WHEN MAT_GRP_4 NOT IN ('-','FD','FU') AND MAT_GRP_5 <> '-'
                                                         THEN (CASE WHEN MAT_GRP_5 IN ('1st','Merge') OR MAT_GRP_5 LIKE 'Middle%' THEN 'O' ELSE '' END)
                                                    WHEN MAT_GRP_4 NOT IN ('-','FD','FU') AND MAT_GRP_5 = '-' THEN '' 
                                                    ELSE 'O'
                                               END GUBUN
                                             , (SELECT DATA_2 FROM MGCMTBLDAT WHERE FACTORY = 'HMVA2' AND TABLE_NAME = 'H_FAMILY' AND KEY_1 = MAT_GRP_2) AS DA_TYPE
                                             , COMP_CNT
                                          FROM VWIPMATDEF
                                         WHERE FACTORY = 'HMVA2'
                                           AND MAT_TYPE = 'FG'
                                           AND DELETE_FLAG = ' '
                                           AND MAT_GRP_2 <> '-'
                                       )
                               ) B
                             , (
                                SELECT MAT_ID, OPER, TCD_CMF_2 AS WIRE_CNT
                                  FROM CWIPTCDDEF@RPTTOMES
                                 WHERE FACTORY = 'HMVA2'
                                   AND OPER LIKE 'A060%'
                                   AND SET_FLAG = 'Y'
                                   AND TCD_CMF_2 <> ' '
                               ) C
                         WHERE A.MAT_ID = B.MAT_ID
                           AND A.MAT_ID = C.MAT_ID(+)
                           AND A.OPER = C.OPER(+)
                       )
                 GROUP BY MAT_ID, OPER_GRP
               ) WIP
             , (
                SELECT RES_STS_2 AS RES_MAT_ID
                     , OPER_GRP AS RES_OPER_GRP 
                     , SUM(RES_CNT) AS RES_TTL
                     , SUM(RUN_CNT) AS RUN_TTL
                     , SUM(DECODE(WORK_DATE, '20200801', RES_CNT, 0)) AS RES_0
                     , SUM(DECODE(WORK_DATE, '20200802', RES_CNT, 0)) AS RES_1
                     , SUM(DECODE(WORK_DATE, '20200803', RES_CNT, 0)) AS RES_2
                     , SUM(DECODE(WORK_DATE, '20200804', RES_CNT, 0)) AS RES_3
                     , SUM(DECODE(WORK_DATE, '20200805', RES_CNT, 0)) AS RES_4
                     , SUM(DECODE(WORK_DATE, '20200806', RES_CNT, 0)) AS RES_5
                     , SUM(DECODE(WORK_DATE, '20200807', RES_CNT, 0)) AS RES_6
                     , SUM(DECODE(WORK_DATE, '20200808', RES_CNT, 0)) AS RES_7
                     , SUM(DECODE(WORK_DATE, '20200809', RES_CNT, 0)) AS RES_8
                     , SUM(DECODE(WORK_DATE, '20200810', RES_CNT, 0)) AS RES_9
                     , SUM(DECODE(WORK_DATE, '20200811', RES_CNT, 0)) AS RES_10
                     , SUM(DECODE(WORK_DATE, '20200812', RES_CNT, 0)) AS RES_11
                     , SUM(DECODE(WORK_DATE, '20200813', RES_CNT, 0)) AS RES_12
                     , SUM(DECODE(WORK_DATE, '20200814', RES_CNT, 0)) AS RES_13
                     , SUM(DECODE(WORK_DATE, '20200815', RES_CNT, 0)) AS RES_14
                     , SUM(DECODE(WORK_DATE, '20200816', RES_CNT, 0)) AS RES_15
                     , SUM(DECODE(WORK_DATE, '20200817', RES_CNT, 0)) AS RES_16
                     , SUM(DECODE(WORK_DATE, '20200818', RES_CNT, 0)) AS RES_17
                     , SUM(DECODE(WORK_DATE, '20200819', RES_CNT, 0)) AS RES_18
                     , SUM(DECODE(WORK_DATE, '20200820', RES_CNT, 0)) AS RES_19
                     , SUM(DECODE(WORK_DATE, '20200821', RES_CNT, 0)) AS RES_20
                     , SUM(DECODE(WORK_DATE, '20200822', RES_CNT, 0)) AS RES_21
                     , SUM(DECODE(WORK_DATE, '20200823', RES_CNT, 0)) AS RES_22
                     , SUM(DECODE(WORK_DATE, '20200824', RES_CNT, 0)) AS RES_23
                     , SUM(DECODE(WORK_DATE, '20200825', RES_CNT, 0)) AS RES_24
                     , SUM(DECODE(WORK_DATE, '20200826', RES_CNT, 0)) AS RES_25
                     , SUM(DECODE(WORK_DATE, '20200827', RES_CNT, 0)) AS RES_26
                     , SUM(DECODE(WORK_DATE, '20200828', RES_CNT, 0)) AS RES_27
                     , SUM(DECODE(WORK_DATE, '20200829', RES_CNT, 0)) AS RES_28
                     , SUM(DECODE(WORK_DATE, '20200830', RES_CNT, 0)) AS RES_29
                     , SUM(DECODE(WORK_DATE, '20200831', RES_CNT, 0)) AS RES_30
                     , SUM(DECODE(WORK_DATE, '20200801', RUN_CNT, 0)) AS RUN_0
                     , SUM(DECODE(WORK_DATE, '20200802', RUN_CNT, 0)) AS RUN_1
                     , SUM(DECODE(WORK_DATE, '20200803', RUN_CNT, 0)) AS RUN_2
                     , SUM(DECODE(WORK_DATE, '20200804', RUN_CNT, 0)) AS RUN_3
                     , SUM(DECODE(WORK_DATE, '20200805', RUN_CNT, 0)) AS RUN_4
                     , SUM(DECODE(WORK_DATE, '20200806', RUN_CNT, 0)) AS RUN_5
                     , SUM(DECODE(WORK_DATE, '20200807', RUN_CNT, 0)) AS RUN_6
                     , SUM(DECODE(WORK_DATE, '20200808', RUN_CNT, 0)) AS RUN_7
                     , SUM(DECODE(WORK_DATE, '20200809', RUN_CNT, 0)) AS RUN_8
                     , SUM(DECODE(WORK_DATE, '20200810', RUN_CNT, 0)) AS RUN_9
                     , SUM(DECODE(WORK_DATE, '20200811', RUN_CNT, 0)) AS RUN_10
                     , SUM(DECODE(WORK_DATE, '20200812', RUN_CNT, 0)) AS RUN_11
                     , SUM(DECODE(WORK_DATE, '20200813', RUN_CNT, 0)) AS RUN_12
                     , SUM(DECODE(WORK_DATE, '20200814', RUN_CNT, 0)) AS RUN_13
                     , SUM(DECODE(WORK_DATE, '20200815', RUN_CNT, 0)) AS RUN_14
                     , SUM(DECODE(WORK_DATE, '20200816', RUN_CNT, 0)) AS RUN_15
                     , SUM(DECODE(WORK_DATE, '20200817', RUN_CNT, 0)) AS RUN_16
                     , SUM(DECODE(WORK_DATE, '20200818', RUN_CNT, 0)) AS RUN_17
                     , SUM(DECODE(WORK_DATE, '20200819', RUN_CNT, 0)) AS RUN_18
                     , SUM(DECODE(WORK_DATE, '20200820', RUN_CNT, 0)) AS RUN_19
                     , SUM(DECODE(WORK_DATE, '20200821', RUN_CNT, 0)) AS RUN_20
                     , SUM(DECODE(WORK_DATE, '20200822', RUN_CNT, 0)) AS RUN_21
                     , SUM(DECODE(WORK_DATE, '20200823', RUN_CNT, 0)) AS RUN_22
                     , SUM(DECODE(WORK_DATE, '20200824', RUN_CNT, 0)) AS RUN_23
                     , SUM(DECODE(WORK_DATE, '20200825', RUN_CNT, 0)) AS RUN_24
                     , SUM(DECODE(WORK_DATE, '20200826', RUN_CNT, 0)) AS RUN_25
                     , SUM(DECODE(WORK_DATE, '20200827', RUN_CNT, 0)) AS RUN_26
                     , SUM(DECODE(WORK_DATE, '20200828', RUN_CNT, 0)) AS RUN_27
                     , SUM(DECODE(WORK_DATE, '20200829', RUN_CNT, 0)) AS RUN_28
                     , SUM(DECODE(WORK_DATE, '20200830', RUN_CNT, 0)) AS RUN_29
                     , SUM(DECODE(WORK_DATE, '20200831', RUN_CNT, 0)) AS RUN_30
                  FROM (
                        SELECT RES_STS_2
                             , WORK_DATE
                             , CASE WHEN A.RES_GRP_3 = 'SDBG' THEN 'Stealth Saw'
                                    WHEN A.RES_GRP_3 = 'BACK LAP' THEN 'BG'
                                    WHEN A.RES_GRP_3 = 'WAFER EXPANDING' THEN 'DDS'
                                    WHEN A.RES_GRP_3 = 'MOLD' THEN 'C-MOLD'
                                    WHEN A.RES_GRP_3 = 'WIRE BOND' THEN 'WB'
                                    WHEN A.RES_GRP_3 = 'SAW SORTER' THEN 'SST'
                                    WHEN A.RES_GRP_3 = 'SAW' THEN 'SAW'
                                    WHEN A.RES_GRP_3 = 'DIE ATTACH' AND B.MAT_GRP_2 = 'DFN' THEN 'DA(DFN)'
                                    WHEN A.RES_GRP_3 = 'DIE ATTACH' AND REGEXP_LIKE(A.RES_GRP_6, 'ESEC2100SD|SDB-30UST|^DB*|^SPA*') THEN 'DA(Epoxy)'
                                    WHEN A.RES_GRP_3 = 'LAMINATION' THEN 'LAMI'
                                    ELSE 'DA'
                               END OPER_GRP
                             , RES_CNT
                             , RUN_CNT
                          FROM (
                                SELECT RES_STS_2
                                     , SUBSTR(CUTOFF_DT, 1, 8) AS WORK_DATE
                                     , RES_GRP_3
                                     , RES_GRP_6
                                     , COUNT(RES_ID) AS RES_CNT
                                     , SUM(DECODE(RES_UP_DOWN_FLAG, 'U', 1, 0)) AS RUN_CNT
                                  FROM MRASRESDEF_BOH
                                 WHERE FACTORY = 'HMVA2'
                                   AND CUTOFF_DT BETWEEN '2020080122' AND '2020083122'
                                   AND SUBSTR(CUTOFF_DT, -2) = '22'
                                   AND RES_CMF_9 = 'Y'
                                   AND RES_CMF_7 = 'Y'
                                   AND (RES_STS_1 NOT IN ('C200', 'B199') OR RES_UP_DOWN_FLAG = 'U') 
                                   AND DELETE_FLAG  = ' '
                                   AND RES_TYPE  = 'EQUIPMENT'
                                   AND (RES_GRP_3 IN ('SDBG','BACK LAP','WAFER EXPANDING','DIE ATTACH', 'WIRE BOND', 'SAW SORTER', 'SAW', 'LAMINATION') OR RES_ID IN ('M-A57','M-A59','M-A60'))
                                 GROUP BY RES_STS_2, SUBSTR(CUTOFF_DT, 1, 8), RES_GRP_3, RES_GRP_6
                               ) A
                             , (
                                SELECT MAT_ID, MAT_GRP_2
                                     , (SELECT DATA_2 FROM MGCMTBLDAT WHERE FACTORY = 'HMVA2' AND TABLE_NAME = 'H_FAMILY' AND KEY_1 = MAT_GRP_2) AS DA_TYPE
                                  FROM MWIPMATDEF
                                 WHERE FACTORY = 'HMVA2'
                                   AND MAT_TYPE = 'FG'
                               ) B
                 WHERE A.RES_STS_2 = B.MAT_ID(+)
                       )
                 GROUP BY RES_STS_2, OPER_GRP
               ) RES
         WHERE 1=1
           AND MAT.OPER_GRP = SHP.END_OPER_GRP(+)
           AND MAT.OPER_GRP = WIP.WIP_OPER_GRP(+)
           AND MAT.OPER_GRP = RES.RES_OPER_GRP(+)
           AND MAT.MAT_ID = SHP.END_MAT_ID(+)
           AND MAT.MAT_ID = WIP.WIP_MAT_ID(+)
           AND MAT.MAT_ID = RES.RES_MAT_ID(+)
           AND NVL(SHP.END_TTL,0) + NVL(WIP.WIP_TTL,0) + NVL(RES.RES_TTL,0) > 0
       ) A
     , (SELECT DECODE(LEVEL, 1, '재공', 2, '실적', 3, '예상실적', 4, '설비대수', 5, 'RUN') AS GUBUN FROM DUAL CONNECT BY LEVEL <= 5 ) B
 GROUP BY A.OPER_GRP, A.OPER_GRP,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ', B.GUBUN 
 ORDER BY DECODE(A.OPER_GRP, 'STOCK', 1, 'LAMI', 2, 'Stealth Saw', 3, 'BG', 4, 'DDS', 5, 'SAW', 6, 'DA', 7, 'DA(Epoxy)', 8, 'DA(DFN)', 9, 'WB', 10, 'C-MOLD', 11, 'SST', 12, 13), A.OPER_GRP,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ', DECODE(B.GUBUN, '재공', 1, '실적', 2, '예상실적', 3, '설비대수', 4, 5) 


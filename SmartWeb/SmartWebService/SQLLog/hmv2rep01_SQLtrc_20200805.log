             , ROUND(SUM(DECODE(OPER_GRP_1, 'HMK2A', DECODE(SHIP_QTY, 0, 0, TAT_QTY/SHIP_QTY), 0)), 2) AS C0
             , ROUND(SUM(DECODE(OPER_GRP_1, 'HMK2A', TAT_QTY, 0)), 2) AS CTAT_QTY0

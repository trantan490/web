                                                                WHEN CALENDAR_ID = 'SE' AND TO_CHAR(TO_DATE('20200805', 'YYYYMMDD'), 'D') = 1 THEN D0_QTY + D1_QTY + D2_QTY + D3_QTY + D4_QTY + D5_QTY 
                                                                ELSE 0 

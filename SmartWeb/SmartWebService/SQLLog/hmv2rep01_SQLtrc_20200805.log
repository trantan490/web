     , (SELECT KEY_1 || ' ' || DATA_1 FROM MGCMTBLDAT WHERE FACTORY = 'HMVA2' AND TABLE_NAME = 'VENDOR' AND KEY_1 = REGEXP_SUBSTR(B.RESV_FIELD_5, '[^,]+', 21)) AS VENDOR4
     , (SELECT KEY_1 || ' ' || DATA_1 FROM MGCMTBLDAT WHERE FACTORY = 'HMVA2' AND TABLE_NAME = 'VENDOR' AND KEY_1 = REGEXP_SUBSTR(B.RESV_FIELD_5, '[^,]+', 28)) AS VENDOR5

                        WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200803', 'YYYYMMDD'), 'D') = 3 THEN D7_QTY
                        WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200803', 'YYYYMMDD'), 'D') = 4 THEN D8_QTY

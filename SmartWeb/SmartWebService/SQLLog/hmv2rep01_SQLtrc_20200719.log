                        WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200718', 'YYYYMMDD'), 'D') = 2 THEN D6_QTY
                        WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200718', 'YYYYMMDD'), 'D') = 3 THEN D7_QTY

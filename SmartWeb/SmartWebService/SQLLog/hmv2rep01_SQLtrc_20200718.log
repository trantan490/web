                        WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200718', 'YYYYMMDD'), 'D') = 1 THEN D3_QTY
                        WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200718', 'YYYYMMDD'), 'D') = 2 THEN D4_QTY

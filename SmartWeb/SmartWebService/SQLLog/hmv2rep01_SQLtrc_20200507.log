                                    WHEN A.OPER LIKE 'A060%' AND B.GUBUN = 'O' THEN WIP_QTY * (CASE WHEN C.WIRE_CNT IS NOT NULL THEN C.WIRE_CNT
                                                                                                    WHEN B.MAT_GRP_6 NOT IN ('-','0') THEN B.MAT_GRP_6
                                                                                                    ELSE '1' END)
                                    WHEN B.GUBUN = 'O' THEN WIP_QTY
                                    ELSE 0
                               END AS CONV_WIP_QTY
                          FROM (
                                SELECT MAT_ID
                                     , WORK_DATE
                                     , OPER
                                     , EOH_QTY_1 AS WIP_QTY
                                     , EOH_QTY_2 AS WIP_QTY_2
                                  FROM RSUMWIPEOH
                                 WHERE FACTORY = 'HMVA2'
                                   AND LOT_TYPE = 'W'
                                   AND REGEXP_LIKE(OPER, 'A040*|A0033|A0040|A0230|A1000|A0950|A060*|A1750|A0030|A0200|A0000|A0020|AZ010|A0250|A050*|A053*|A055*|A1825')
                                   AND WORK_DATE BETWEEN '20200501' AND '20200531'
                                   AND SHIFT = '3'
                                   AND CM_KEY_3 LIKE 'P%' 
                               ) A
                             , (
                                SELECT *
                                  FROM (
                                        SELECT MAT_ID, MAT_GRP_2, MAT_GRP_4, MAT_GRP_5, MAT_GRP_6, MAT_CMF_11
                                             , CASE WHEN MAT_GRP_4 NOT IN ('-','FD','FU') AND MAT_GRP_5 <> '-'
                                                         THEN (CASE WHEN MAT_GRP_5 IN ('1st','Merge') OR MAT_GRP_5 LIKE 'Middle%' THEN 'O' ELSE '' END)
                                                    WHEN MAT_GRP_4 NOT IN ('-','FD','FU') AND MAT_GRP_5 = '-' THEN '' 
                                                    ELSE 'O'
                                               END GUBUN
                                             , (SELECT DATA_2 FROM MGCMTBLDAT WHERE FACTORY = 'HMVA2' AND TABLE_NAME = 'H_FAMILY' AND KEY_1 = MAT_GRP_2) AS DA_TYPE
                                             , COMP_CNT
                                          FROM VWIPMATDEF
                                         WHERE FACTORY = 'HMVA2'
                                           AND MAT_TYPE = 'FG'
                                           AND DELETE_FLAG = ' '
                                           AND MAT_GRP_2 <> '-'
                                       )
                               ) B
                             , (
                                SELECT MAT_ID, OPER, TCD_CMF_2 AS WIRE_CNT
                                  FROM CWIPTCDDEF@RPTTOMES
                                 WHERE FACTORY = 'HMVA2'
                                   AND OPER LIKE 'A060%'
                                   AND SET_FLAG = 'Y'
                                   AND TCD_CMF_2 <> ' '
                               ) C
                         WHERE A.MAT_ID = B.MAT_ID
                           AND A.MAT_ID = C.MAT_ID(+)
                           AND A.OPER = C.OPER(+)
                       )
                 GROUP BY MAT_ID, OPER_GRP
               ) WIP
             , (
                SELECT RES_STS_2 AS RES_MAT_ID
                     , OPER_GRP AS RES_OPER_GRP 
                     , SUM(RES_CNT) AS RES_TTL
                     , SUM(RUN_CNT) AS RUN_TTL
                     , SUM(DECODE(WORK_DATE, '20200501', RES_CNT, 0)) AS RES_0
                     , SUM(DECODE(WORK_DATE, '20200502', RES_CNT, 0)) AS RES_1
                     , SUM(DECODE(WORK_DATE, '20200503', RES_CNT, 0)) AS RES_2
                     , SUM(DECODE(WORK_DATE, '20200504', RES_CNT, 0)) AS RES_3
                     , SUM(DECODE(WORK_DATE, '20200505', RES_CNT, 0)) AS RES_4
                     , SUM(DECODE(WORK_DATE, '20200506', RES_CNT, 0)) AS RES_5
                     , SUM(DECODE(WORK_DATE, '20200507', RES_CNT, 0)) AS RES_6
                     , SUM(DECODE(WORK_DATE, '20200508', RES_CNT, 0)) AS RES_7
                     , SUM(DECODE(WORK_DATE, '20200509', RES_CNT, 0)) AS RES_8
                     , SUM(DECODE(WORK_DATE, '20200510', RES_CNT, 0)) AS RES_9
                     , SUM(DECODE(WORK_DATE, '20200511', RES_CNT, 0)) AS RES_10
                     , SUM(DECODE(WORK_DATE, '20200512', RES_CNT, 0)) AS RES_11
                     , SUM(DECODE(WORK_DATE, '20200513', RES_CNT, 0)) AS RES_12
                     , SUM(DECODE(WORK_DATE, '20200514', RES_CNT, 0)) AS RES_13
                     , SUM(DECODE(WORK_DATE, '20200515', RES_CNT, 0)) AS RES_14
                     , SUM(DECODE(WORK_DATE, '20200516', RES_CNT, 0)) AS RES_15
                     , SUM(DECODE(WORK_DATE, '20200517', RES_CNT, 0)) AS RES_16
                     , SUM(DECODE(WORK_DATE, '20200518', RES_CNT, 0)) AS RES_17
                     , SUM(DECODE(WORK_DATE, '20200519', RES_CNT, 0)) AS RES_18
                     , SUM(DECODE(WORK_DATE, '20200520', RES_CNT, 0)) AS RES_19
                     , SUM(DECODE(WORK_DATE, '20200521', RES_CNT, 0)) AS RES_20
                     , SUM(DECODE(WORK_DATE, '20200522', RES_CNT, 0)) AS RES_21
                     , SUM(DECODE(WORK_DATE, '20200523', RES_CNT, 0)) AS RES_22
                     , SUM(DECODE(WORK_DATE, '20200524', RES_CNT, 0)) AS RES_23
                     , SUM(DECODE(WORK_DATE, '20200525', RES_CNT, 0)) AS RES_24
                     , SUM(DECODE(WORK_DATE, '20200526', RES_CNT, 0)) AS RES_25
                     , SUM(DECODE(WORK_DATE, '20200527', RES_CNT, 0)) AS RES_26
                     , SUM(DECODE(WORK_DATE, '20200528', RES_CNT, 0)) AS RES_27
                     , SUM(DECODE(WORK_DATE, '20200529', RES_CNT, 0)) AS RES_28
                     , SUM(DECODE(WORK_DATE, '20200530', RES_CNT, 0)) AS RES_29
                     , SUM(DECODE(WORK_DATE, '20200531', RES_CNT, 0)) AS RES_30
                     , SUM(DECODE(WORK_DATE, '20200501', RUN_CNT, 0)) AS RUN_0
                     , SUM(DECODE(WORK_DATE, '20200502', RUN_CNT, 0)) AS RUN_1
                     , SUM(DECODE(WORK_DATE, '20200503', RUN_CNT, 0)) AS RUN_2
                     , SUM(DECODE(WORK_DATE, '20200504', RUN_CNT, 0)) AS RUN_3
                     , SUM(DECODE(WORK_DATE, '20200505', RUN_CNT, 0)) AS RUN_4
                     , SUM(DECODE(WORK_DATE, '20200506', RUN_CNT, 0)) AS RUN_5
                     , SUM(DECODE(WORK_DATE, '20200507', RUN_CNT, 0)) AS RUN_6
                     , SUM(DECODE(WORK_DATE, '20200508', RUN_CNT, 0)) AS RUN_7
                     , SUM(DECODE(WORK_DATE, '20200509', RUN_CNT, 0)) AS RUN_8
                     , SUM(DECODE(WORK_DATE, '20200510', RUN_CNT, 0)) AS RUN_9
                     , SUM(DECODE(WORK_DATE, '20200511', RUN_CNT, 0)) AS RUN_10
                     , SUM(DECODE(WORK_DATE, '20200512', RUN_CNT, 0)) AS RUN_11
                     , SUM(DECODE(WORK_DATE, '20200513', RUN_CNT, 0)) AS RUN_12
                     , SUM(DECODE(WORK_DATE, '20200514', RUN_CNT, 0)) AS RUN_13
                     , SUM(DECODE(WORK_DATE, '20200515', RUN_CNT, 0)) AS RUN_14
                     , SUM(DECODE(WORK_DATE, '20200516', RUN_CNT, 0)) AS RUN_15
                     , SUM(DECODE(WORK_DATE, '20200517', RUN_CNT, 0)) AS RUN_16
                     , SUM(DECODE(WORK_DATE, '20200518', RUN_CNT, 0)) AS RUN_17
                     , SUM(DECODE(WORK_DATE, '20200519', RUN_CNT, 0)) AS RUN_18
                     , SUM(DECODE(WORK_DATE, '20200520', RUN_CNT, 0)) AS RUN_19
                     , SUM(DECODE(WORK_DATE, '20200521', RUN_CNT, 0)) AS RUN_20
                     , SUM(DECODE(WORK_DATE, '20200522', RUN_CNT, 0)) AS RUN_21
                     , SUM(DECODE(WORK_DATE, '20200523', RUN_CNT, 0)) AS RUN_22
                     , SUM(DECODE(WORK_DATE, '20200524', RUN_CNT, 0)) AS RUN_23
                     , SUM(DECODE(WORK_DATE, '20200525', RUN_CNT, 0)) AS RUN_24
                     , SUM(DECODE(WORK_DATE, '20200526', RUN_CNT, 0)) AS RUN_25
                     , SUM(DECODE(WORK_DATE, '20200527', RUN_CNT, 0)) AS RUN_26
                     , SUM(DECODE(WORK_DATE, '20200528', RUN_CNT, 0)) AS RUN_27
                     , SUM(DECODE(WORK_DATE, '20200529', RUN_CNT, 0)) AS RUN_28
                     , SUM(DECODE(WORK_DATE, '20200530', RUN_CNT, 0)) AS RUN_29
                     , SUM(DECODE(WORK_DATE, '20200531', RUN_CNT, 0)) AS RUN_30
                  FROM (
                        SELECT RES_STS_2
                             , WORK_DATE
                             , CASE WHEN A.RES_GRP_3 = 'SDBG' THEN 'Stealth Saw'
                                    WHEN A.RES_GRP_3 = 'BACK LAP' THEN 'BG'
                                    WHEN A.RES_GRP_3 = 'WAFER EXPANDING' THEN 'DDS'
                                    WHEN A.RES_GRP_3 = 'MOLD' THEN 'C-MOLD'
                                    WHEN A.RES_GRP_3 = 'WIRE BOND' THEN 'WB'
                                    WHEN A.RES_GRP_3 = 'SAW SORTER' THEN 'SST'
                                    WHEN A.RES_GRP_3 = 'SAW' THEN 'SAW'
                                    WHEN A.RES_GRP_3 = 'DIE ATTACH' AND B.MAT_GRP_2 = 'DFN' THEN 'DA(DFN)'
                                    WHEN A.RES_GRP_3 = 'DIE ATTACH' AND REGEXP_LIKE(A.RES_GRP_6, 'ESEC2100SD|SDB-30UST|^DB*|^SPA*') THEN 'DA(Epoxy)'
                                    WHEN A.RES_GRP_3 = 'LAMINATION' THEN 'LAMI'
                                    ELSE 'DA'
                               END OPER_GRP
                             , RES_CNT
                             , RUN_CNT
                          FROM (
                                SELECT RES_STS_2
                                     , SUBSTR(CUTOFF_DT, 1, 8) AS WORK_DATE
                                     , RES_GRP_3
                                     , RES_GRP_6
                                     , COUNT(RES_ID) AS RES_CNT
                                     , SUM(DECODE(RES_UP_DOWN_FLAG, 'U', 1, 0)) AS RUN_CNT
                                  FROM MRASRESDEF_BOH
                                 WHERE FACTORY = 'HMVA2'
                                   AND CUTOFF_DT BETWEEN '2020050122' AND '2020053122'
                                   AND SUBSTR(CUTOFF_DT, -2) = '22'
                                   AND RES_CMF_9 = 'Y'
                                   AND RES_CMF_7 = 'Y'
                                   AND (RES_STS_1 NOT IN ('C200', 'B199') OR RES_UP_DOWN_FLAG = 'U') 
                                   AND DELETE_FLAG  = ' '
                                   AND RES_TYPE  = 'EQUIPMENT'
                                   AND (RES_GRP_3 IN ('SDBG','BACK LAP','WAFER EXPANDING','DIE ATTACH', 'WIRE BOND', 'SAW SORTER', 'SAW', 'LAMINATION') OR RES_ID IN ('M-A57','M-A59','M-A60'))
                                 GROUP BY RES_STS_2, SUBSTR(CUTOFF_DT, 1, 8), RES_GRP_3, RES_GRP_6
                               ) A
                             , (
                                SELECT MAT_ID, MAT_GRP_2
                                     , (SELECT DATA_2 FROM MGCMTBLDAT WHERE FACTORY = 'HMVA2' AND TABLE_NAME = 'H_FAMILY' AND KEY_1 = MAT_GRP_2) AS DA_TYPE
                                  FROM MWIPMATDEF
                                 WHERE FACTORY = 'HMVA2'
                                   AND MAT_TYPE = 'FG'
                               ) B
                 WHERE A.RES_STS_2 = B.MAT_ID(+)
                       )
                 GROUP BY RES_STS_2, OPER_GRP
               ) RES
         WHERE 1=1
           AND MAT.OPER_GRP = SHP.END_OPER_GRP(+)
           AND MAT.OPER_GRP = WIP.WIP_OPER_GRP(+)
           AND MAT.OPER_GRP = RES.RES_OPER_GRP(+)
           AND MAT.MAT_ID = SHP.END_MAT_ID(+)
           AND MAT.MAT_ID = WIP.WIP_MAT_ID(+)
           AND MAT.MAT_ID = RES.RES_MAT_ID(+)
           AND NVL(SHP.END_TTL,0) + NVL(WIP.WIP_TTL,0) + NVL(RES.RES_TTL,0) > 0
       ) A
     , (SELECT DECODE(LEVEL, 1, '재공', 2, '실적', 3, '예상실적', 4, '설비대수', 5, 'RUN') AS GUBUN FROM DUAL CONNECT BY LEVEL <= 5 ) B
 GROUP BY A.OPER_GRP, A.OPER_GRP,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ', B.GUBUN 
 ORDER BY DECODE(A.OPER_GRP, 'STOCK', 1, 'LAMI', 2, 'Stealth Saw', 3, 'BG', 4, 'DDS', 5, 'SAW', 6, 'DA', 7, 'DA(Epoxy)', 8, 'DA(DFN)', 9, 'WB', 10, 'C-MOLD', 11, 'SST', 12, 13), A.OPER_GRP,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ', DECODE(B.GUBUN, '재공', 1, '실적', 2, '예상실적', 3, '설비대수', 4, 5) 


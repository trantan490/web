                                                 UNION ALL  
                                                SELECT MAT_ID  
                                                     , SUM(CASE WHEN SHP_QTY_1 > 0 AND WORK_DATE = '20200624' THEN NVL(SHP_QTY_1, 0) ELSE 0 END) AS SHP_TODAY  

                                                                WHEN CALENDAR_ID = 'SE' AND TO_CHAR(TO_DATE('20200423', 'YYYYMMDD'), 'D') = 3 THEN D1_QTY 
                                                                WHEN CALENDAR_ID = 'SE' AND TO_CHAR(TO_DATE('20200423', 'YYYYMMDD'), 'D') = 4 THEN D2_QTY 

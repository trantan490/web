                                                                WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200423', 'YYYYMMDD'), 'D') = 3 THEN D5_QTY 
                                                                WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200423', 'YYYYMMDD'), 'D') = 4 THEN D6_QTY 

                           AND RAS.FACTORY = UPEH.FACTORY(+) 
                           AND RAS.RES_GRP_6 = UPEH.RES_MODEL(+) 
                           AND RAS.RES_STS_2 = UPEH.MAT_ID(+) 
                           AND RAS.RES_STS_8 = UPEH.OPER(+) 
                           AND RAS.RES_STS_2 = WIR.MAT_ID(+) 
                           AND RAS.RES_STS_8 = WIR.OPER(+) 

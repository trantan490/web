SELECT B.MAT_TYPE
     , A.MAT_ID
     , B.MAT_DESC
     , A.OPER
     , SUM(END_QTY) AS TOTAL_QTY
     , SUM(DECODE(WORK_DATE, '20200701', END_QTY)) AS END_QTY_0
     , SUM(DECODE(WORK_DATE, '20200702', END_QTY)) AS END_QTY_1
     , SUM(DECODE(WORK_DATE, '20200703', END_QTY)) AS END_QTY_2
     , SUM(DECODE(WORK_DATE, '20200704', END_QTY)) AS END_QTY_3
     , SUM(DECODE(WORK_DATE, '20200705', END_QTY)) AS END_QTY_4
     , SUM(DECODE(WORK_DATE, '20200706', END_QTY)) AS END_QTY_5
     , SUM(DECODE(WORK_DATE, '20200707', END_QTY)) AS END_QTY_6
     , SUM(DECODE(WORK_DATE, '20200708', END_QTY)) AS END_QTY_7
     , SUM(DECODE(WORK_DATE, '20200709', END_QTY)) AS END_QTY_8
     , SUM(DECODE(WORK_DATE, '20200710', END_QTY)) AS END_QTY_9
     , SUM(DECODE(WORK_DATE, '20200711', END_QTY)) AS END_QTY_10
     , SUM(DECODE(WORK_DATE, '20200712', END_QTY)) AS END_QTY_11
     , SUM(DECODE(WORK_DATE, '20200713', END_QTY)) AS END_QTY_12
     , SUM(DECODE(WORK_DATE, '20200714', END_QTY)) AS END_QTY_13
     , SUM(DECODE(WORK_DATE, '20200715', END_QTY)) AS END_QTY_14
     , SUM(DECODE(WORK_DATE, '20200716', END_QTY)) AS END_QTY_15
     , SUM(DECODE(WORK_DATE, '20200717', END_QTY)) AS END_QTY_16
     , SUM(DECODE(WORK_DATE, '20200718', END_QTY)) AS END_QTY_17
     , SUM(DECODE(WORK_DATE, '20200719', END_QTY)) AS END_QTY_18
     , SUM(DECODE(WORK_DATE, '20200720', END_QTY)) AS END_QTY_19
     , SUM(DECODE(WORK_DATE, '20200721', END_QTY)) AS END_QTY_20
     , SUM(DECODE(WORK_DATE, '20200722', END_QTY)) AS END_QTY_21
     , SUM(DECODE(WORK_DATE, '20200723', END_QTY)) AS END_QTY_22
     , SUM(DECODE(WORK_DATE, '20200724', END_QTY)) AS END_QTY_23
     , SUM(DECODE(WORK_DATE, '20200725', END_QTY)) AS END_QTY_24
     , ROUND(SUM(END_QTY)/25, 1) AS AVG
  FROM (
        SELECT FACTORY, MAT_ID, OPER, WORK_DATE, CM_KEY_3
             , SUM(S1_END_QTY_1+S2_END_QTY_1+S3_END_QTY_1) AS END_QTY
          FROM RSUMWIPMOV
         WHERE 1=1
           AND FACTORY = 'HMVA2'
           AND WORK_DATE BETWEEN '20200701' AND '20200725'
           AND (OPER LIKE 'V%' OR OPER LIKE 'M%')
         GROUP BY FACTORY, MAT_ID, OPER, WORK_DATE, CM_KEY_3 
       ) A
     , MWIPMATDEF B
 WHERE 1=1
   AND A.FACTORY = B.FACTORY
   AND A.MAT_ID = B.MAT_ID
   AND A.END_QTY > 0
   AND A.OPER IN ('M0330')
   AND A.MAT_ID LIKE '%'  
 GROUP BY B.MAT_TYPE, B.MAT_DESC, A.MAT_ID, A.OPER
 ORDER BY B.MAT_TYPE, B.MAT_DESC, A.MAT_ID, A.OPER


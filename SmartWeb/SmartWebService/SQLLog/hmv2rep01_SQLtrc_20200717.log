AND VIEW_TIME BETWEEN TO_CHAR(SYSDATE - 10/1440, 'YYYYMMDDHH24MISS') AND TO_CHAR(SYSDATE + 10/1440, 'YYYYMMDDHH24MISS')
-------------------------------------------------------------
WITH DT AS
(
 SELECT MAT_ID
      , SUM(DECODE(WORK_DATE, '20200627', SHP_QTY, 0)) AS D0_SHP
      , SUM(DECODE(WORK_DATE, '20200628', SHP_QTY, 0)) AS D1_SHP
      , SUM(DECODE(WORK_DATE, '20200629', SHP_QTY, 0)) AS D2_SHP
      , SUM(DECODE(WORK_DATE, '20200630', SHP_QTY, 0)) AS D3_SHP
      , SUM(DECODE(WORK_DATE, '20200701', SHP_QTY, 0)) AS D4_SHP
      , SUM(DECODE(WORK_DATE, '20200702', SHP_QTY, 0)) AS D5_SHP
      , SUM(DECODE(WORK_DATE, '20200703', SHP_QTY, 0)) AS D6_SHP
      , SUM(SHP_QTY) AS WW_SHP
      , SUM(CASE WHEN WORK_DATE <= '20200627' THEN SHP_QTY ELSE 0 END) AS SUM_D0_SHP
      , SUM(CASE WHEN WORK_DATE <= '20200628' THEN SHP_QTY ELSE 0 END) AS SUM_D1_SHP
      , SUM(CASE WHEN WORK_DATE <= '20200629' THEN SHP_QTY ELSE 0 END) AS SUM_D2_SHP
      , SUM(CASE WHEN WORK_DATE <= '20200630' THEN SHP_QTY ELSE 0 END) AS SUM_D3_SHP
      , SUM(CASE WHEN WORK_DATE <= '20200701' THEN SHP_QTY ELSE 0 END) AS SUM_D4_SHP
      , SUM(CASE WHEN WORK_DATE <= '20200702' THEN SHP_QTY ELSE 0 END) AS SUM_D5_SHP
      , SUM(CASE WHEN WORK_DATE <= '20200703' THEN SHP_QTY ELSE 0 END) AS SUM_D6_SHP
   FROM (
         SELECT WORK_DATE, MAT_ID
              , SUM(S1_FAC_OUT_QTY_1+S2_FAC_OUT_QTY_1+S3_FAC_OUT_QTY_1) AS SHP_QTY 
           FROM RSUMFACMOV
          WHERE 1=1
            AND WORK_DATE BETWEEN '20200627' AND '20200703'
            AND LOT_TYPE = 'W' 
            AND CM_KEY_1 = 'HMVA2'
            AND FACTORY NOT IN ('RETURN') 
            AND MAT_ID NOT LIKE 'HX%'
          GROUP BY WORK_DATE, MAT_ID
          UNION ALL
         SELECT WORK_DATE, MAT_ID
              , SUM(S1_FAC_OUT_QTY_1+S2_FAC_OUT_QTY_1+S3_FAC_OUT_QTY_1) AS SHP_QTY 
           FROM CSUMFACMOV
          WHERE 1=1
            AND WORK_DATE BETWEEN '20200627' AND '20200703'
            AND LOT_TYPE = 'W' 
            AND CM_KEY_1 = 'HMVA2'
            AND FACTORY NOT IN ('RETURN') 

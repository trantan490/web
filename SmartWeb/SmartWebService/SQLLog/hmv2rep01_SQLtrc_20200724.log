SELECT CUSTOMER,  ' ', MAT_GRP_2, MAT_GRP_3, MAT_GRP_4,  ' ', MAT_GRP_6,  ' ',  ' ',  ' ', CONV_MAT_ID,  ' ',  ' ',  ' ' 
     , WEEK_PLAN, SHP_WEEK, WEEK_DEF, REMAIN_MOLD, REMAIN_WB, REMAIN_DA, REMAIN_BG, REMAIN_INPUT, REMAIN_WF, DA_CAPA, WB_CAPA
     , D0_PLAN2, D0_DEF, SHP_TODAY, HMK3A, QC_GATE, PVI, BAKE, AVI, DC2, DC1, SIG, SBA, TRIM, MK, TIN, CURE, MOLD, PLASMA, GATE
     , WB9, DA9, WB8, DA8, WB7, DA7, WB6, DA6, WB5, DA5, WB4, DA4, WB3, DA3, WB2, DA2, WB1, DA1, SP, SAW, BG, HMK2A
     , D0_PLAN, D1_PLAN, D2_PLAN, D3_PLAN, D4_PLAN, DA1_CAPA, WB1_CAPA, DA2_CAPA, WB2_CAPA, DA3_CAPA, WB3_CAPA, DA4_CAPA, WB4_CAPA, DA5_CAPA, WB5_CAPA
     , DA6_CAPA, WB6_CAPA, DA7_CAPA, WB7_CAPA, DA8_CAPA, WB8_CAPA, DA9_CAPA, WB9_CAPA, WEEK2_PLAN, WEEK3_PLAN, WEEK4_PLAN, WEEK5_PLAN, ORI_PLAN, SHP_MONTH, MONTH_DEF, REV_PLAN, PER
  FROM(
SELECT (SELECT DATA_1 FROM MGCMTBLDAT WHERE TABLE_NAME = 'H_CUSTOMER' AND KEY_1 = MAT.MAT_GRP_1 AND ROWNUM=1) AS CUSTOMER,  ' ', MAT.MAT_GRP_2, MAT.MAT_GRP_3, MAT.MAT_GRP_4,  ' ', MAT.MAT_GRP_6,  ' ',  ' ',  ' ', MAT.CONV_MAT_ID,  ' ',  ' ',  ' ' 
     , ROUND(SUM(NVL(WEEK1_PLAN, 0)) / 1000, 0) AS WEEK_PLAN
     , ROUND(SUM(NVL(SHP_WEEK, 0)) / 1000, 0) AS SHP_WEEK
     , CASE WHEN SUM(NVL(WEEK1_PLAN, 0) - NVL(SHP_WEEK, 0)) < 0 THEN 0
            ELSE ROUND(SUM(NVL(WEEK1_PLAN, 0) - NVL(SHP_WEEK, 0)) / 1000, 0) END AS WEEK_DEF
     , CASE WHEN SUM(NVL(WEEK1_PLAN, 0) - NVL(SHP_WEEK, 0) - NVL(W_MOLD, 0)) < 0 THEN 0 
            ELSE ROUND(SUM(NVL(WEEK1_PLAN, 0) - NVL(SHP_WEEK, 0) - NVL(W_MOLD, 0)) / 1000, 0) END AS REMAIN_MOLD
     , CASE WHEN SUM(NVL(WEEK1_PLAN, 0) - NVL(SHP_WEEK, 0) - NVL(W_WB, 0)) < 0 THEN 0
            ELSE ROUND(SUM(NVL(WEEK1_PLAN, 0) - NVL(SHP_WEEK, 0) - NVL(W_WB, 0)) / 1000, 0) END AS REMAIN_WB
     , CASE WHEN SUM(NVL(WEEK1_PLAN, 0) - NVL(SHP_WEEK, 0) - NVL(W_DA, 0)) < 0 THEN 0
            ELSE ROUND(SUM(NVL(WEEK1_PLAN, 0) - NVL(SHP_WEEK, 0) - NVL(W_DA, 0)) / 1000, 0) END AS REMAIN_DA
     , CASE WHEN SUM(NVL(WEEK1_PLAN, 0) - NVL(SHP_WEEK, 0) - NVL(W_BG, 0)) < 0 THEN 0
            ELSE ROUND(SUM(NVL(WEEK1_PLAN, 0) - NVL(SHP_WEEK, 0) - NVL(W_BG, 0)) / 1000, 0) END AS REMAIN_BG
     , CASE WHEN SUM(NVL(WEEK1_PLAN, 0) - NVL(SHP_WEEK, 0) - NVL(W_ISSUE, 0)) < 0 THEN 0
            ELSE ROUND(SUM(NVL(WEEK1_PLAN, 0) - NVL(SHP_WEEK, 0) - NVL(W_ISSUE, 0)) / 1000, 0) END AS REMAIN_INPUT
     , CASE WHEN SUM(NVL(WEEK1_PLAN, 0) - NVL(SHP_WEEK, 0) - NVL(TOTAL, 0)) < 0 THEN 0
            ELSE ROUND(SUM(NVL(WEEK1_PLAN, 0) - NVL(SHP_WEEK, 0) - NVL(TOTAL, 0)) / 1000, 0) END AS REMAIN_WF
     , ROUND(SUM(NVL(DA_CAPA, 0)) / 1000, 0) AS DA_CAPA
     , ROUND(SUM(NVL(WB_CAPA, 0)) / 1000, 0) AS WB_CAPA
     , CASE WHEN SUM(NVL(D0_PLAN, 0) + (NVL(WEEK1_TTL, 0) - NVL(SHP_WEEK_TTL, 0))) < 0 THEN 0
            ELSE ROUND(SUM(NVL(D0_PLAN, 0) + (NVL(WEEK1_TTL, 0) - NVL(SHP_WEEK_TTL, 0))) / 1000, 0) END AS D0_PLAN2
     , CASE WHEN SUM(NVL(D0_PLAN, 0) + (NVL(WEEK1_TTL, 0) - NVL(SHP_WEEK_TTL, 0)) - NVL(SHP_TODAY, 0)) < 0 THEN 0
            ELSE ROUND(SUM(NVL(D0_PLAN, 0) + (NVL(WEEK1_TTL, 0) - NVL(SHP_WEEK_TTL, 0)) - NVL(SHP_TODAY, 0)) / 1000, 0) END AS D0_DEF
     , ROUND(SUM(NVL(SHP_TODAY, 0)) / 1000, 0) AS SHP_TODAY
     , ROUND(SUM(NVL(WIP.HMK3A, 0)) / 1000, 0) AS HMK3A
     , ROUND(SUM(NVL(WIP.QC_GATE, 0)) / 1000, 0) AS QC_GATE
     , ROUND(SUM(NVL(WIP.PVI, 0)) / 1000, 0) AS PVI
     , ROUND(SUM(NVL(WIP.BAKE,0)) / 1000, 0) AS BAKE
     , ROUND(SUM(NVL(WIP.AVI,0)) / 1000, 0) AS AVI
     , ROUND(SUM(NVL(WIP.DC2,0)) / 1000, 0) AS DC2
     , ROUND(SUM(NVL(WIP.DC1,0)) / 1000, 0) AS DC1
     , ROUND(SUM(NVL(WIP.SIG,0)) / 1000, 0) AS SIG
     , ROUND(SUM(NVL(WIP.SBA,0)) / 1000, 0) AS SBA
     , ROUND(SUM(NVL(WIP.TIN,0)) / 1000, 0) AS TIN
     , ROUND(SUM(NVL(WIP.TRIM,0)) / 1000, 0) AS TRIM
     , ROUND(SUM(NVL(WIP.MK,0)) / 1000, 0) AS MK
     , ROUND(SUM(NVL(WIP.CURE,0)) / 1000, 0) AS CURE
     , ROUND(SUM(NVL(WIP.MOLD,0)) / 1000, 0) AS MOLD
     , ROUND(SUM(NVL(WIP.PLASMA,0)) / 1000, 0) AS PLASMA
     , ROUND(SUM(NVL(WIP.GATE,0)) / 1000, 0) AS GATE
     , ROUND(SUM(NVL(WIP.WB9,0)) / 1000, 0) AS WB9
     , ROUND(SUM(NVL(WIP.DA9,0)) / 1000, 0) AS DA9
     , ROUND(SUM(NVL(WIP.WB8,0)) / 1000, 0) AS WB8
     , ROUND(SUM(NVL(WIP.DA8,0)) / 1000, 0) AS DA8
     , ROUND(SUM(NVL(WIP.WB7,0)) / 1000, 0) AS WB7
     , ROUND(SUM(NVL(WIP.DA7,0)) / 1000, 0) AS DA7
     , ROUND(SUM(NVL(WIP.WB6,0)) / 1000, 0) AS WB6
     , ROUND(SUM(NVL(WIP.DA6,0)) / 1000, 0) AS DA6
     , ROUND(SUM(NVL(WIP.WB5,0)) / 1000, 0) AS WB5
     , ROUND(SUM(NVL(WIP.DA5,0)) / 1000, 0) AS DA5
     , ROUND(SUM(NVL(WIP.WB4,0)) / 1000, 0) AS WB4
     , ROUND(SUM(NVL(WIP.DA4,0)) / 1000, 0) AS DA4
     , ROUND(SUM(NVL(WIP.WB3,0)) / 1000, 0) AS WB3
     , ROUND(SUM(NVL(WIP.DA3,0)) / 1000, 0) AS DA3
     , ROUND(SUM(NVL(WIP.WB2,0)) / 1000, 0) AS WB2
     , ROUND(SUM(NVL(WIP.DA2,0)) / 1000, 0) AS DA2
     , ROUND(SUM(NVL(WIP.WB1,0)) / 1000, 0) AS WB1
     , ROUND(SUM(NVL(WIP.DA1,0)) / 1000, 0) AS DA1
     , ROUND(SUM(NVL(WIP.SP,0)) / 1000, 0) AS SP
     , ROUND(SUM(NVL(WIP.SAW,0)) / 1000, 0) AS SAW
     , ROUND(SUM(NVL(WIP.BG,0)) / 1000, 0) AS BG
     , ROUND(SUM(NVL(WIP.HMK2A,0)) / 1000, 0) AS HMK2A
     , ROUND(SUM(NVL(D0_PLAN, 0) + (NVL(WEEK1_TTL, 0) - NVL(SHP_WEEK_TTL, 0))) / 1000, 0) AS D0_PLAN
     , ROUND(SUM(NVL(D1_PLAN, 0)) / 1000, 0) AS D1_PLAN
     , ROUND(SUM(NVL(D2_PLAN, 0)) / 1000, 0) AS D2_PLAN
     , ROUND(SUM(NVL(D3_PLAN, 0)) / 1000, 0) AS D3_PLAN
     , ROUND(SUM(NVL(D4_PLAN, 0)) / 1000, 0) AS D4_PLAN
     , ROUND(SUM(NVL(DA1_CAPA, 0)) / 1000, 0) AS DA1_CAPA
     , ROUND(SUM(NVL(WB1_CAPA, 0)) / 1000, 0) AS WB1_CAPA
     , ROUND(SUM(NVL(DA2_CAPA, 0)) / 1000, 0) AS DA2_CAPA
     , ROUND(SUM(NVL(WB2_CAPA, 0)) / 1000, 0) AS WB2_CAPA
     , ROUND(SUM(NVL(DA3_CAPA, 0)) / 1000, 0) AS DA3_CAPA
     , ROUND(SUM(NVL(WB3_CAPA, 0)) / 1000, 0) AS WB3_CAPA
     , ROUND(SUM(NVL(DA4_CAPA, 0)) / 1000, 0) AS DA4_CAPA
     , ROUND(SUM(NVL(WB4_CAPA, 0)) / 1000, 0) AS WB4_CAPA
     , ROUND(SUM(NVL(DA5_CAPA, 0)) / 1000, 0) AS DA5_CAPA
     , ROUND(SUM(NVL(WB5_CAPA, 0)) / 1000, 0) AS WB5_CAPA
     , ROUND(SUM(NVL(DA6_CAPA, 0)) / 1000, 0) AS DA6_CAPA
     , ROUND(SUM(NVL(WB6_CAPA, 0)) / 1000, 0) AS WB6_CAPA
     , ROUND(SUM(NVL(DA7_CAPA, 0)) / 1000, 0) AS DA7_CAPA
     , ROUND(SUM(NVL(WB7_CAPA, 0)) / 1000, 0) AS WB7_CAPA
     , ROUND(SUM(NVL(DA8_CAPA, 0)) / 1000, 0) AS DA8_CAPA
     , ROUND(SUM(NVL(WB8_CAPA, 0)) / 1000, 0) AS WB8_CAPA
     , ROUND(SUM(NVL(DA9_CAPA, 0)) / 1000, 0) AS DA9_CAPA
     , ROUND(SUM(NVL(WB9_CAPA, 0)) / 1000, 0) AS WB9_CAPA
     , ROUND(SUM(NVL(WEEK2_PLAN, 0)) / 1000, 0) AS WEEK2_PLAN
     , ROUND(SUM(NVL(WEEK3_PLAN, 0)) / 1000, 0) AS WEEK3_PLAN
     , ROUND(SUM(NVL(WEEK4_PLAN, 0)) / 1000, 0) AS WEEK4_PLAN
     , ROUND(SUM(NVL(WEEK5_PLAN, 0)) / 1000, 0) AS WEEK5_PLAN
     , ROUND(SUM(NVL(ORI_PLAN, 0)) / 1000, 0) AS ORI_PLAN
     , ROUND(SUM(NVL(SHP_MONTH, 0)) / 1000, 0) AS SHP_MONTH
     , ROUND(SUM(NVL(ORI_PLAN, 0) - NVL(SHP_MONTH, 0)) / 1000, 0) AS MONTH_DEF
     , ROUND(SUM(NVL(REV_PLAN, 0)) / 1000, 0) AS REV_PLAN
     , CASE WHEN SUM(NVL(ORI_PLAN, 0)) = 0 THEN 0
            ELSE ROUND(SUM(NVL(REV_PLAN, 0)) / SUM(NVL(ORI_PLAN, 0)) * 100, 0)
       END PER
     , ROUND(SUM(NVL(W_MOLD, 0)) / 1000, 0) AS W_MOLD 
     , ROUND(SUM(NVL(W_WB, 0)) / 1000, 0) AS W_WB
     , ROUND(SUM(NVL(W_DA, 0)) / 1000, 0) AS W_DA
     , ROUND(SUM(NVL(W_BG, 0)) / 1000, 0) AS W_BG
     , ROUND(SUM(NVL(W_ISSUE, 0)) / 1000, 0) AS W_ISSUE
     , ROUND(SUM(NVL(TOTAL, 0)) / 1000, 0) AS TOTAL
  FROM (
        SELECT MAT_GRP_1, MAT_GRP_2, MAT_GRP_3, MAT_GRP_4, MAT_GRP_5, MAT_GRP_6
             , MAT_GRP_7, MAT_GRP_8, MAT_GRP_9, MAT_GRP_10, MAT_CMF_7, MAT_CMF_10, MAT_ID, MAT_CMF_11
             , CASE WHEN MAT_GRP_9 = 'MEMORY' AND MAT_GRP_1 = 'HX' AND INSTR(MAT_ID, '_') = 0 THEN SUBSTR(MAT_ID, 1, 12) || '%' || SUBSTR(MAT_ID, -3)
                    WHEN MAT_GRP_9 = 'MEMORY' AND MAT_GRP_1 = 'HX' AND INSTR(MAT_ID, '_') > 0 THEN SUBSTR(MAT_ID, 1, INSTR(MAT_ID, '_')) || '%' || SUBSTR(MAT_ID, -3)
                    WHEN MAT_GRP_9 = 'MEMORY' AND MAT_GRP_1 = 'SE' THEN 'SEK_________-___' || SUBSTR(MAT_ID, -3)
                    ELSE MAT_ID
               END CONV_MAT_ID
             , VENDOR_ID AS SAP_CODE
          FROM MWIPMATDEF
         WHERE FACTORY = 'HMVA2'
           AND MAT_TYPE = 'FG' 
           AND DELETE_FLAG = ' ' 
           AND (MAT_GRP_5 IN ('1st','Merge', '-') OR MAT_GRP_5 LIKE 'Middle%') 
           AND MAT_GRP_3 <> 'COB' 
           AND MAT_ID LIKE '%'
       ) MAT 
     , ( 
        SELECT FACTORY, MAT_ID 
             , SUM(CASE WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200724', 'YYYYMMDD'), 'D') = 1 THEN D0_QTY
                        WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200724', 'YYYYMMDD'), 'D') = 2 THEN D0_QTY + D1_QTY
                        WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200724', 'YYYYMMDD'), 'D') = 3 THEN D0_QTY + D1_QTY + D2_QTY
                        WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200724', 'YYYYMMDD'), 'D') = 4 THEN D0_QTY + D1_QTY + D2_QTY + D3_QTY
                        WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200724', 'YYYYMMDD'), 'D') = 5 THEN D0_QTY + D1_QTY + D2_QTY + D3_QTY + D4_QTY
                        WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200724', 'YYYYMMDD'), 'D') = 6 THEN D0_QTY + D1_QTY + D2_QTY + D3_QTY + D4_QTY + D5_QTY
                        WHEN CALENDAR_ID = 'SE' AND TO_CHAR(TO_DATE('20200724', 'YYYYMMDD'), 'D') = 3 THEN D0_QTY 
                        WHEN CALENDAR_ID = 'SE' AND TO_CHAR(TO_DATE('20200724', 'YYYYMMDD'), 'D') = 4 THEN D0_QTY + D1_QTY 
                        WHEN CALENDAR_ID = 'SE' AND TO_CHAR(TO_DATE('20200724', 'YYYYMMDD'), 'D') = 5 THEN D0_QTY + D1_QTY + D2_QTY 
                        WHEN CALENDAR_ID = 'SE' AND TO_CHAR(TO_DATE('20200724', 'YYYYMMDD'), 'D') = 6 THEN D0_QTY + D1_QTY + D2_QTY + D3_QTY 
                        WHEN CALENDAR_ID = 'SE' AND TO_CHAR(TO_DATE('20200724', 'YYYYMMDD'), 'D') = 7 THEN D0_QTY + D1_QTY + D2_QTY + D3_QTY + D4_QTY 
                        WHEN CALENDAR_ID = 'SE' AND TO_CHAR(TO_DATE('20200724', 'YYYYMMDD'), 'D') = 1 THEN D0_QTY + D1_QTY + D2_QTY + D3_QTY + D4_QTY + D5_QTY 
                        ELSE 0
                   END) AS WEEK1_TTL 
             , SUM(W1_QTY) AS WEEK1_PLAN 
             , SUM(W2_QTY) AS WEEK2_PLAN 
             , SUM(W3_QTY) AS WEEK3_PLAN 

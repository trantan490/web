                        WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200724', 'YYYYMMDD'), 'D') = 5 THEN D8_QTY
                        WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200724', 'YYYYMMDD'), 'D') = 6 THEN D9_QTY

        SELECT 'CHIP수' AS GUBUN2, 'BOC' AS GUBUN3 FROM DUAL UNION
        SELECT 'CHIP수' AS GUBUN2, 'Others' AS GUBUN3 FROM DUAL UNION
        SELECT 'Wire수' AS GUBUN2, ' ' AS GUBUN3 FROM DUAL UNION
        SELECT 'PKG수' AS GUBUN2, 'C-Mold' AS GUBUN3 FROM DUAL UNION
        SELECT 'PKG수' AS GUBUN2, 'ORP' AS GUBUN3 FROM DUAL UNION
        SELECT 'PKG수' AS GUBUN2, 'SBA' AS GUBUN3 FROM DUAL UNION
        SELECT 'PKG수' AS GUBUN2, 'SST' AS GUBUN3 FROM DUAL UNION
        SELECT '95mm' AS GUBUN2, 'Mold' AS GUBUN3 FROM DUAL UNION
        SELECT '95mm' AS GUBUN2, 'SBA' AS GUBUN3 FROM DUAL UNION
        SELECT '95mm' AS GUBUN2, 'SST' AS GUBUN3 FROM DUAL
       ) B      
)
SELECT MDL.GUBUN1, MDL.GUBUN2, MDL.GUBUN3
     , ROUND(CASE WHEN MDL.GUBUN1 = '계획' THEN V0 ELSE V0 / TO_CHAR(LAST_DAY(TO_DATE('202005', 'YYYYMM')), 'DD') END / 1, 0) V0
     , ROUND(CASE WHEN MDL.GUBUN1 = '계획' THEN V1 ELSE V1 / TO_CHAR(LAST_DAY(TO_DATE('202006', 'YYYYMM')), 'DD') END / 1, 0) V1
     , ROUND(CASE WHEN MDL.GUBUN1 = '계획' THEN V2 ELSE V2 / TO_CHAR(LAST_DAY(TO_DATE('202007', 'YYYYMM')), 'DD') END / 1, 0) V2
     , ROUND(CASE WHEN MDL.GUBUN1 = '계획' THEN V3 ELSE V3 / TO_CHAR(LAST_DAY(TO_DATE('202008', 'YYYYMM')), 'DD') END / 1, 0) V3
     , ROUND(CASE WHEN MDL.GUBUN1 = '계획' THEN V4 ELSE V4 / TO_CHAR(LAST_DAY(TO_DATE('202009', 'YYYYMM')), 'DD') END / 1, 0) V4
     , ROUND(CASE WHEN MDL.GUBUN1 = '계획' THEN V5 ELSE V5 / TO_CHAR(LAST_DAY(TO_DATE('202010', 'YYYYMM')), 'DD') END / 1, 0) V5
     , ROUND(CASE WHEN MDL.GUBUN1 = '계획' THEN V6 ELSE V6 / TO_CHAR(LAST_DAY(TO_DATE('202011', 'YYYYMM')), 'DD') END / 1, 0) V6
     , ROUND(CASE WHEN MDL.GUBUN1 = '계획' THEN V7 ELSE V7 / TO_CHAR(LAST_DAY(TO_DATE('202012', 'YYYYMM')), 'DD') END / 1, 0) V7
     , ROUND(CASE WHEN MDL.GUBUN1 = '계획' THEN V8 ELSE V8 / TO_CHAR(LAST_DAY(TO_DATE('202101', 'YYYYMM')), 'DD') END / 1, 0) V8
     , ROUND(CASE WHEN MDL.GUBUN1 = '계획' THEN V9 ELSE V9 / TO_CHAR(LAST_DAY(TO_DATE('202102', 'YYYYMM')), 'DD') END / 1, 0) V9
     , ROUND(CASE WHEN MDL.GUBUN1 = '계획' THEN V10 ELSE V10 / TO_CHAR(LAST_DAY(TO_DATE('202103', 'YYYYMM')), 'DD') END / 1, 0) V10
     , ROUND(CASE WHEN MDL.GUBUN1 = '계획' THEN V11 ELSE V11 / TO_CHAR(LAST_DAY(TO_DATE('202104', 'YYYYMM')), 'DD') END / 1, 0) V11
     , ROUND(CASE WHEN MDL.GUBUN1 = '계획' THEN V12 ELSE V12 / TO_CHAR(LAST_DAY(TO_DATE('202105', 'YYYYMM')), 'DD') END / 1, 0) V12
     , ROUND(CASE WHEN MDL.GUBUN1 = '계획' THEN V13 ELSE V13 / TO_CHAR(LAST_DAY(TO_DATE('202106', 'YYYYMM')), 'DD') END / 1, 0) V13
  FROM MD MDL
     , (
        SELECT *
          FROM (
                SELECT '계획' AS GUBUN1 FROM DUAL UNION
                SELECT '일목표' AS GUBUN1 FROM DUAL
               ) A
             , (
                SELECT GUBUN2, GUBUN3
                     , SUM(DECODE(PLAN_DATE, '202005', CONV_QTY, 0)) V0
                     , SUM(DECODE(PLAN_DATE, '202006', CONV_QTY, 0)) V1
                     , SUM(DECODE(PLAN_DATE, '202007', CONV_QTY, 0)) V2
                     , SUM(DECODE(PLAN_DATE, '202008', CONV_QTY, 0)) V3
                     , SUM(DECODE(PLAN_DATE, '202009', CONV_QTY, 0)) V4
                     , SUM(DECODE(PLAN_DATE, '202010', CONV_QTY, 0)) V5
                     , SUM(DECODE(PLAN_DATE, '202011', CONV_QTY, 0)) V6
                     , SUM(DECODE(PLAN_DATE, '202012', CONV_QTY, 0)) V7
                     , SUM(DECODE(PLAN_DATE, '202101', CONV_QTY, 0)) V8
                     , SUM(DECODE(PLAN_DATE, '202102', CONV_QTY, 0)) V9

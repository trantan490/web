     , SUM(DECODE(WORK_DATE, '20200612', END_QTY)) AS END_QTY_11
     , SUM(DECODE(WORK_DATE, '20200613', END_QTY)) AS END_QTY_12
     , SUM(DECODE(WORK_DATE, '20200614', END_QTY)) AS END_QTY_13
     , SUM(DECODE(WORK_DATE, '20200615', END_QTY)) AS END_QTY_14
     , SUM(DECODE(WORK_DATE, '20200616', END_QTY)) AS END_QTY_15
     , ROUND(SUM(END_QTY)/16, 1) AS AVG
  FROM (
        SELECT FACTORY, MAT_ID, OPER, WORK_DATE, CM_KEY_3
             , SUM(S1_END_QTY_1+S2_END_QTY_1+S3_END_QTY_1) AS END_QTY
          FROM RSUMWIPMOV
         WHERE 1=1
           AND FACTORY = 'HMVA2'
           AND WORK_DATE BETWEEN '20200601' AND '20200616'
           AND (OPER LIKE 'V%' OR OPER LIKE 'M%')
         GROUP BY FACTORY, MAT_ID, OPER, WORK_DATE, CM_KEY_3 
       ) A
     , MWIPMATDEF B
 WHERE 1=1
   AND A.FACTORY = B.FACTORY
   AND A.MAT_ID = B.MAT_ID
   AND A.END_QTY > 0
   AND A.OPER LIKE '%'
   AND A.MAT_ID LIKE '%'  
 GROUP BY B.MAT_TYPE, B.MAT_DESC, A.MAT_ID, A.OPER
 ORDER BY B.MAT_TYPE, B.MAT_DESC, A.MAT_ID, A.OPER


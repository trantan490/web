SELECT PLAN_WEEK,PLAN_YEAR,SEQ 
  FROM ( 
       SELECT PLAN_WEEK,PLAN_YEAR,1 AS SEQ 
         FROM MWIPCALDEF  
        WHERE CALENDAR_ID = 'SE' 
          AND SYS_DATE = TO_CHAR(TO_DATE('20200704', 'YYYYMMDD'), 'YYYYMMDD') 
    UNION ALL 
       SELECT PLAN_WEEK,PLAN_YEAR,2 AS SEQ 
         FROM MWIPCALDEF  
        WHERE CALENDAR_ID = 'SE' 
          AND SYS_DATE = TO_CHAR(TO_DATE('20200704', 'YYYYMMDD')+7, 'YYYYMMDD') 
    UNION ALL 
       SELECT PLAN_WEEK,PLAN_YEAR,3 AS SEQ 
         FROM MWIPCALDEF  
        WHERE CALENDAR_ID = 'SE' 
          AND SYS_DATE = TO_CHAR(TO_DATE('20200704', 'YYYYMMDD')+1, 'YYYYMMDD') 
    UNION ALL 
       SELECT PLAN_WEEK,PLAN_YEAR,4 AS SEQ 
         FROM MWIPCALDEF  
        WHERE CALENDAR_ID = 'SE' 
          AND SYS_DATE = TO_CHAR(TO_DATE('20200704', 'YYYYMMDD')-1, 'YYYYMMDD') 
        ) 
ORDER BY SEQ


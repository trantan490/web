SELECT DECODE(A.RES_UP_DOWN_FLAG, 'U', 'UP', 'DOWN') AS STATE
     , A.RES_GRP_6 AS MODEL, A.RES_STS_2 AS MAT_ID, A.RES_ID, A.RES_STS_1 AS CODE
  FROM MRASRESDEF_BOH A
     , (
        SELECT M.* 
             , NVL((SELECT DATA_10 FROM MGCMTBLDAT WHERE TABLE_NAME = 'H_CUSTOMER' AND FACTORY = 'HMVA2' AND  KEY_1 = M.MAT_GRP_1), '-') AS CUST_TYPE 
          FROM MWIPMATDEF M 
         WHERE FACTORY = 'HMVA2' 
           AND DELETE_FLAG = ' ' 
       ) B

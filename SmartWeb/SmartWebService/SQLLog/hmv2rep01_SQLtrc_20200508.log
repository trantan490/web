   AND A.FACTORY = B.FACTORY(+)
   AND A.RES_STS_2 = B.MAT_ID(+)
   AND A.FACTORY = 'HMVA2'
   AND A.RES_CMF_9 = 'Y'
   AND A.RES_CMF_7 = 'Y'
   AND (A.RES_STS_1 NOT IN ('C200', 'B199') OR A.RES_UP_DOWN_FLAG = 'U') 
   AND A.DELETE_FLAG  = ' '
   AND A.RES_TYPE  = 'EQUIPMENT'
   AND A.CUTOFF_DT = '2020040122'
   AND A.RES_GRP_3 = 'SAW'
 ORDER BY STATE, RES_GRP_6


            THEN SUBSTR(TO_CHAR(TO_DATE(TO_DATE('20200508134654', 'YYYYMMDD HH24MISS')),'YYYY'),3,2) || 'Y ' || TO_CHAR(TO_CHAR(TO_DATE('20200508134654', 'YYYYMMDD HH24MISS'), 'Q') - 3) || 'Q' 
            WHEN TO_CHAR(TO_DATE('20200508134654', 'YYYYMMDD HH24MISS'), 'Q') - 3 = 0 
            THEN SUBSTR(TO_CHAR(TO_DATE(TO_DATE('20200508134654', 'YYYYMMDD HH24MISS')),'YYYY'),3,2) - 1 || 'Y ' || '4' || 'Q' 
            WHEN TO_CHAR(TO_DATE('20200508134654', 'YYYYMMDD HH24MISS'), 'Q') - 3 = -1 
            THEN SUBSTR(TO_CHAR(TO_DATE(TO_DATE('20200508134654', 'YYYYMMDD HH24MISS')),'YYYY'),3,2) - 1 || 'Y ' || '3' || 'Q' 
            WHEN TO_CHAR(TO_DATE('20200508134654', 'YYYYMMDD HH24MISS'), 'Q') - 3 = -2 
            THEN SUBSTR(TO_CHAR(TO_DATE(TO_DATE('20200508134654', 'YYYYMMDD HH24MISS')),'YYYY'),3,2) - 1 || 'Y ' || '2' || 'Q' 
       END AS Q4, 
       CASE WHEN TO_CHAR(TO_DATE('20200508134654', 'YYYYMMDD HH24MISS'), 'Q') - 2 > 0 
            THEN SUBSTR(TO_CHAR(TO_DATE(TO_DATE('20200508134654', 'YYYYMMDD HH24MISS')),'YYYY'),3,2) || 'Y ' || TO_CHAR(TO_CHAR(TO_DATE('20200508134654', 'YYYYMMDD HH24MISS'), 'Q') - 2)|| 'Q' 
            WHEN TO_CHAR(TO_DATE('20200508134654', 'YYYYMMDD HH24MISS'), 'Q') - 2 = 0 
            THEN SUBSTR(TO_CHAR(TO_DATE(TO_DATE('20200508134654', 'YYYYMMDD HH24MISS')),'YYYY'),3,2) - 1 || 'Y ' || '4' || 'Q' 
            WHEN TO_CHAR(TO_DATE('20200508134654', 'YYYYMMDD HH24MISS'), 'Q') - 2 = -1 
            THEN SUBSTR(TO_CHAR(TO_DATE(TO_DATE('20200508134654', 'YYYYMMDD HH24MISS')),'YYYY'),3,2) - 1 || 'Y ' || '3' || 'Q' 
       END AS Q3, 
       CASE WHEN TO_CHAR(TO_DATE('20200508134654', 'YYYYMMDD HH24MISS'), 'Q') - 1 > 0 
            THEN SUBSTR(TO_CHAR(TO_DATE(TO_DATE('20200508134654', 'YYYYMMDD HH24MISS')),'YYYY'),3,2) || 'Y ' || TO_CHAR(TO_CHAR(TO_DATE('20200508134654', 'YYYYMMDD HH24MISS'), 'Q') - 1)|| 'Q' 
            WHEN TO_CHAR(TO_DATE('20200508134654', 'YYYYMMDD HH24MISS'), 'Q') - 1 = 0 

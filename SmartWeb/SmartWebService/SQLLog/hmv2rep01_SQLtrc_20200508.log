            THEN SUBSTR(TO_CHAR(TO_DATE(TO_DATE('20200508082213', 'YYYYMMDD HH24MISS')),'YYYY'),3,2) || 'Y ' || TO_CHAR(TO_CHAR(TO_DATE('20200508082213', 'YYYYMMDD HH24MISS'), 'Q') - 2)|| 'Q' 

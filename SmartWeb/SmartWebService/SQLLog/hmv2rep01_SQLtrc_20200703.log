SELECT (SELECT DATA_1 FROM MGCMTBLDAT WHERE TABLE_NAME = 'H_CUSTOMER' AND KEY_1 = A.MAT_GRP_1 AND ROWNUM=1) AS CUSTOMER,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ', A.MAT_ID  
     , A.LOT_ID, A.LOT_CMF_5
     , SUM(NVL(QTY_1,0)) +  SUM(NVL(B.LOSS_QTY,0)) - SUM(NVL(C.BNS_QTY,0)) AS IN_QTY
     , SUM(NVL(QTY_1,0)) AS OUT_QTY
     , ROUND((NVL(QTY_1,0)/(NVL(QTY_1,0) +  SUM(NVL(B.LOSS_QTY,0)) - SUM(NVL(C.BNS_QTY,0)) ))*100,3) AS YIELD 
     , SUM(NVL(B.LOSS_QTY,0)) AS LOSS 
     , SUM(NVL(B."211",0)) AS "211" 
     , SUM(NVL(B."306",0)) AS "306" 
     , SUM(NVL(B."307",0)) AS "307" 
     , SUM(NVL(B."319",0)) AS "319" 
     , SUM(NVL(B."333",0)) AS "333" 
     , SUM(NVL(B."392",0)) AS "392" 
     , SUM(NVL(B."402",0)) AS "402" 
     , SUM(NVL(B."406",0)) AS "406" 
     , SUM(NVL(B."430",0)) AS "430" 
     , SUM(NVL(B."480",0)) AS "480" 
  FROM (  
         SELECT B.MAT_GRP_1, B.MAT_GRP_2, B.MAT_GRP_3, B.MAT_GRP_4, B.MAT_GRP_5, B.MAT_GRP_6, B.MAT_GRP_7, B.MAT_GRP_8, A.MAT_ID 
              , A.FACTORY
              , A.INVOICE_NO
              , A.LOT_ID
              , A.QTY_1 
              , A.TRAN_TIME     
              , A.IN_QTY 
              , A.LOT_CMF_5 
           FROM ( 
                  SELECT FROM_FACTORY AS FACTORY
                       , INVOICE_NO
                       , MAT_ID
                       , LOT_ID
                       , SHIP_QTY_1 AS QTY_1 
                       , IN_QTY 
                       , TRAN_TIME   
                       , LOT_CMF_5   
                    FROM VWIPSHPLOT   
                   WHERE 1=1   
                     AND OWNER_CODE = 'PROD' 
                     AND LOT_TYPE = 'W' 
                     AND FROM_FACTORY  = 'HMVA2' 
                     AND TO_FACTORY IN ('CUSTOMER', 'HMKT1') 
                     AND TRAN_TIME BETWEEN '20200701220000' AND '20200702215959' 
                     AND FROM_OPER IN ('AZ010','AZ009','TZ010','EZ010','F0000')
                ) A 
                , MWIPMATDEF B 
            WHERE 1=1 
              AND A.MAT_ID = B.MAT_ID(+) 
              AND A.FACTORY = B.FACTORY(+) 
              AND B.MAT_VER(+) = 1 
       ) A 
     , (  
         SELECT LOT_ID 
              , SUM(NVL(LOSS_QTY,0)) AS LOSS_QTY 
              , SUM(DECODE(LOSS_CODE, '211', LOSS_QTY, 0))AS "211" 
              , SUM(DECODE(LOSS_CODE, '306', LOSS_QTY, 0))AS "306" 
              , SUM(DECODE(LOSS_CODE, '307', LOSS_QTY, 0))AS "307" 
              , SUM(DECODE(LOSS_CODE, '319', LOSS_QTY, 0))AS "319" 
              , SUM(DECODE(LOSS_CODE, '333', LOSS_QTY, 0))AS "333" 
              , SUM(DECODE(LOSS_CODE, '392', LOSS_QTY, 0))AS "392" 
              , SUM(DECODE(LOSS_CODE, '402', LOSS_QTY, 0))AS "402" 
              , SUM(DECODE(LOSS_CODE, '406', LOSS_QTY, 0))AS "406" 
              , SUM(DECODE(LOSS_CODE, '430', LOSS_QTY, 0))AS "430" 
              , SUM(DECODE(LOSS_CODE, '480', LOSS_QTY, 0))AS "480" 
           FROM RWIPLOTLSM
          WHERE HIST_DEL_FLAG = ' '
            AND FACTORY  = 'HMVA2' 
            AND LOT_ID IN ( 
                            SELECT LOT_ID
                              FROM VWIPSHPLOT   
                             WHERE 1=1   
                               AND FROM_FACTORY  = 'HMVA2' 
                               AND OWNER_CODE = 'PROD' 
                               AND TO_FACTORY IN ('CUSTOMER', 'HMKT1') 
                               AND LOT_TYPE = 'W' 
                               AND FROM_OPER IN ('AZ010','AZ009','TZ010','EZ010', 'F0000')
                               AND TRAN_TIME BETWEEN '20200701220000' AND '20200702215959' 
                          ) 
          GROUP BY LOT_ID 
       ) B
     , ( 
         SELECT LOT_ID , SUM(NVL(TOTAL_BONUS_QTY,0)) AS BNS_QTY
           FROM RWIPLOTBNS
          WHERE 1=1
            AND FACTORY  = 'HMVA2' 
            AND HIST_DEL_FLAG = ' '
            AND TRAN_TIME BETWEEN '20200701220000' AND '20200702215959'
          GROUP BY FACTORY, LOT_ID 
       ) C                     
 WHERE 1 = 1  
   AND A.LOT_ID = B.LOT_ID(+)                           
   AND A.LOT_ID = C.LOT_ID(+)        
 GROUP BY A.MAT_GRP_1,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ', A.MAT_ID, A.LOT_ID, QTY_1, LOT_CMF_5    
 ORDER BY A.MAT_GRP_1,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ', A.MAT_ID, A.LOT_ID, OUT_QTY     


     , SUM(NVL(B."953",0)) AS "953" 
  FROM (  
         SELECT B.MAT_GRP_1, B.MAT_GRP_2, B.MAT_GRP_3, B.MAT_GRP_4, B.MAT_GRP_5, B.MAT_GRP_6, B.MAT_GRP_7, B.MAT_GRP_8, A.MAT_ID 

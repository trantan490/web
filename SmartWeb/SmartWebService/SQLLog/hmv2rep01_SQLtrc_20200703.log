SELECT COUNT(*)
FROM RWEBFUNLOG
WHERE FACTORY = :FACTORY  
AND USER_ID = :USER_ID 
AND FUNC_NAME = :FUNC_NAME 
AND FUNC_GROUP = :FUNC_GROUP
AND VIEW_TIME BETWEEN TO_CHAR(SYSDATE - 10/1440, 'YYYYMMDDHH24MISS') AND TO_CHAR(SYSDATE + 10/1440, 'YYYYMMDDHH24MISS')
-------------------------------------------------------------
SELECT (SELECT DATA_1 FROM MGCMTBLDAT WHERE TABLE_NAME = 'H_CUSTOMER' AND KEY_1 = MAT.MAT_GRP_1 AND ROWNUM=1) AS CUSTOMER,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ', MAT.CONV_MAT_ID
     , DAT.OPER
     , ROUND(SUM(DECODE(DAT.WORK_DATE, '20200703', DAT.PLN_QTY,0)) / 1, 0) AS PLN_QTY_0
     , ROUND(SUM(DECODE(DAT.WORK_DATE, '20200703', DAT.END_QTY,0)) / 1, 0) AS END_QTY_0
     , ROUND(SUM(DECODE(DAT.WORK_DATE, '20200704', DAT.PLN_QTY,0)) / 1, 0) AS PLN_QTY_1
     , ROUND(SUM(DECODE(DAT.WORK_DATE, '20200704', DAT.END_QTY,0)) / 1, 0) AS END_QTY_1
     , ROUND(SUM(DECODE(DAT.WORK_DATE, '20200705', DAT.PLN_QTY,0)) / 1, 0) AS PLN_QTY_2
     , ROUND(SUM(DECODE(DAT.WORK_DATE, '20200705', DAT.END_QTY,0)) / 1, 0) AS END_QTY_2
     , ROUND(SUM(DECODE(DAT.WORK_DATE, '20200706', DAT.PLN_QTY,0)) / 1, 0) AS PLN_QTY_3
     , ROUND(SUM(DECODE(DAT.WORK_DATE, '20200706', DAT.END_QTY,0)) / 1, 0) AS END_QTY_3
     , ROUND(SUM(DECODE(DAT.WORK_DATE, '20200707', DAT.PLN_QTY,0)) / 1, 0) AS PLN_QTY_4
     , ROUND(SUM(DECODE(DAT.WORK_DATE, '20200707', DAT.END_QTY,0)) / 1, 0) AS END_QTY_4
     , ROUND(SUM(DECODE(DAT.WORK_DATE, '20200708', DAT.PLN_QTY,0)) / 1, 0) AS PLN_QTY_5
     , ROUND(SUM(DECODE(DAT.WORK_DATE, '20200708', DAT.END_QTY,0)) / 1, 0) AS END_QTY_5
     , ROUND(SUM(DECODE(DAT.WORK_DATE, '20200709', DAT.PLN_QTY,0)) / 1, 0) AS PLN_QTY_6
     , ROUND(SUM(DECODE(DAT.WORK_DATE, '20200709', DAT.END_QTY,0)) / 1, 0) AS END_QTY_6
     , ROUND(SUM(DAT.PLN_QTY) / 1, 0) AS PLN_TTL
     , ROUND(SUM(DAT.END_QTY) / 1, 0) AS SHP_TTL
  FROM (
        SELECT CASE WHEN MAT_GRP_1 = 'HX' THEN MAT_CMF_10 ELSE MAT_ID END CONV_MAT_ID

SELECT  ' ',  ' ', A.MAT_GRP_3, A.MAT_GRP_6,  ' ',  ' ',  ' ',  ' '
      , LOSS_DESC, LOSS_CODE, A.LOSS_QTY
      , ROUND((A.LOSS_QTY/(C.LOSS_QTY+QTY)) * 1000000)
      , LOSS_OPER_DESC
   FROM(
SELECT  ' ',  ' ', MAT_GRP_3, MAT_GRP_6,  ' ',  ' ',  ' ',  ' '
      , LOSS_DESC, LOSS_CODE, LOSS_QTY
      , LOSS_OPER_DESC
  FROM (
        SELECT  ' ',  ' ', MAT_GRP_3, MAT_GRP_6,  ' ',  ' ',  ' ',  ' ', ROW_NUMBER() OVER(PARTITION BY  ' ',  ' ', MAT_GRP_3, MAT_GRP_6,  ' ',  ' ',  ' ',  ' ' ORDER BY MAT_GRP_3 DESC, MAT_GRP_6 DESC, LOSS_QTY DESC, A.LOSS_CODE ASC) AS NUM, B.LOSS_DESC,A.LOSS_CODE, A.LOSS_QTY, B.LOSS_OPER_DESC
            FROM
               (
               SELECT  ' ',  ' ', MAT_GRP_3, MAT_GRP_6,  ' ',  ' ',  ' ',  ' '
                     , LOSS_CODE, SUM(NVL(LOSS_QTY,0)) AS LOSS_QTY
                 FROM RWIPLOTLSM A,
                      (
                      SELECT * 
                        FROM VWIPSHPLOT LOT, MWIPMATDEF MAT
                       WHERE FROM_FACTORY = 'HMVA2'
                         AND LOT.MAT_ID = MAT.MAT_ID
                         AND MAT.FACTORY ='HMVA2'
                         AND LOT_TYPE = 'W'
                         AND TRAN_TIME BETWEEN '20200620220000' AND '20200702215959' 
                         AND (LOT.MAT_ID LIKE 'DR%'
                         AND LOT.MAT_ID NOT LIKE '%2JU')
                         AND FROM_OPER = 'AZ010'
                       ) B
                WHERE A.LOT_ID(+) = B.LOT_ID
                  AND A.FACTORY = 'HMVA2'
                GROUP BY  ' ',  ' ', MAT_GRP_3, MAT_GRP_6,  ' ',  ' ',  ' ',  ' ', LOSS_CODE 
                ORDER BY  ' ',  ' ', MAT_GRP_3, TO_NUMBER(MAT_GRP_6) DESC,  ' ',  ' ',  ' ',  ' ', LOSS_QTY DESC, LOSS_CODE ASC 
                ) A,
                (
                SELECT KEY_1 AS LOSS_CODE
                     , DATA_1 AS LOSS_DESC
                     , DATA_5 AS LOSS_OPER_DESC
                  FROM MGCMTBLDAT
                 WHERE FACTORY = 'HMVA2'
                   AND TABLE_NAME = 'LOSS_CODE'
                ) B
            WHERE A.LOSS_CODE = B.LOSS_CODE
)
) A,
(SELECT MAT_GRP_3, MAT_GRP_6, SUM(SHIP_QTY_1) AS QTY
   FROM VWIPSHPLOT LOT, MWIPMATDEF MAT
  WHERE FROM_FACTORY = 'HMVA2'
    AND LOT.MAT_ID = MAT.MAT_ID
    AND MAT.FACTORY ='HMVA2'
    AND LOT_TYPE = 'W'
    AND TRAN_TIME BETWEEN '20200620220000' AND '20200702215959' 
    AND (LOT.MAT_ID LIKE 'DR%'
    AND LOT.MAT_ID NOT LIKE '%2JU')
    AND FROM_OPER = 'AZ010'
    GROUP BY  ' ',  ' ', MAT_GRP_3, MAT_GRP_6,  ' ',  ' ',  ' ',  ' '
) B ,
( 
 SELECT  ' ',  ' ', MAT_GRP_3, MAT_GRP_6,  ' ',  ' ',  ' ',  ' '
      , SUM(NVL(LOSS_QTY,0)) AS LOSS_QTY
   FROM RWIPLOTLSM A,
       (
       SELECT * 
         FROM VWIPSHPLOT LOT, MWIPMATDEF MAT
        WHERE FROM_FACTORY = 'HMVA2'
          AND LOT.MAT_ID = MAT.MAT_ID
          AND MAT.FACTORY ='HMVA2'
          AND LOT_TYPE = 'W'
          AND TRAN_TIME BETWEEN '20200620220000' AND '20200702215959' 
          AND (LOT.MAT_ID LIKE 'DR%'
          AND LOT.MAT_ID NOT LIKE '%2JU')
                         AND FROM_OPER = 'AZ010'
                       ) B
                WHERE A.LOT_ID(+) = B.LOT_ID
                  AND A.FACTORY = 'HMVA2'
                GROUP BY  ' ',  ' ', MAT_GRP_3, MAT_GRP_6,  ' ',  ' ',  ' ',  ' '
               ) C 
WHERE A.MAT_GRP_3 = B.MAT_GRP_3
  AND A.MAT_GRP_6 = B.MAT_GRP_6
  AND A.MAT_GRP_3 = C.MAT_GRP_3
  AND A.MAT_GRP_6 = C.MAT_GRP_6
  ORDER BY  ' ',  ' ', MAT_GRP_3, TO_NUMBER(MAT_GRP_6) DESC,  ' ',  ' ',  ' ',  ' ', LOSS_QTY DESC, LOSS_CODE


         SELECT LOT_ID , SUM(NVL(TOTAL_BONUS_QTY,0)) AS BNS_QTY
           FROM RWIPLOTBNS
          WHERE 1=1
            AND FACTORY  = 'HMVA2' 
            AND HIST_DEL_FLAG = ' '
            AND TRAN_TIME BETWEEN '20200620220000' AND '20200702215959'
          GROUP BY FACTORY, LOT_ID 

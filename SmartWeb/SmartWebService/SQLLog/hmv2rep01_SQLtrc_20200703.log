                                                                WHEN CALENDAR_ID = 'SE' AND TO_CHAR(TO_DATE('20200630', 'YYYYMMDD'), 'D') = 3 THEN D3_QTY 
                                                                WHEN CALENDAR_ID = 'SE' AND TO_CHAR(TO_DATE('20200630', 'YYYYMMDD'), 'D') = 4 THEN D4_QTY 

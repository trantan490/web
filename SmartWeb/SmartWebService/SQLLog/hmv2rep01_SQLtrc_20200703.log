                       , LOT_ID
                       , SHIP_QTY_1 AS QTY_1 
                       , IN_QTY 
                       , TRAN_TIME   
                       , LOT_CMF_5   
                    FROM VWIPSHPLOT   
                   WHERE 1=1   
                     AND OWNER_CODE = 'PROD' 
                     AND LOT_TYPE = 'W' 
                     AND FROM_FACTORY  = 'HMVA2' 
                     AND TO_FACTORY IN ('CUSTOMER', 'HMKT1') 
                     AND TRAN_TIME BETWEEN '20200620220000' AND '20200702215959' 
                     AND FROM_OPER IN ('AZ010','AZ009','TZ010','EZ010','F0000')
                ) A 
                , MWIPMATDEF B 
            WHERE 1=1 
              AND A.MAT_ID = B.MAT_ID(+) 
              AND A.FACTORY = B.FACTORY(+) 
              AND B.MAT_VER(+) = 1 
       ) A 
     , (  
         SELECT LOT_ID 
              , SUM(NVL(LOSS_QTY,0)) AS LOSS_QTY 
              , SUM(DECODE(LOSS_CODE, '106', LOSS_QTY, 0))AS "106" 
              , SUM(DECODE(LOSS_CODE, '112', LOSS_QTY, 0))AS "112" 
              , SUM(DECODE(LOSS_CODE, '123', LOSS_QTY, 0))AS "123" 
              , SUM(DECODE(LOSS_CODE, '209', LOSS_QTY, 0))AS "209" 
              , SUM(DECODE(LOSS_CODE, '211', LOSS_QTY, 0))AS "211" 
              , SUM(DECODE(LOSS_CODE, '302', LOSS_QTY, 0))AS "302" 
              , SUM(DECODE(LOSS_CODE, '306', LOSS_QTY, 0))AS "306" 
              , SUM(DECODE(LOSS_CODE, '307', LOSS_QTY, 0))AS "307" 
              , SUM(DECODE(LOSS_CODE, '312', LOSS_QTY, 0))AS "312" 
              , SUM(DECODE(LOSS_CODE, '319', LOSS_QTY, 0))AS "319" 
              , SUM(DECODE(LOSS_CODE, '326', LOSS_QTY, 0))AS "326" 
              , SUM(DECODE(LOSS_CODE, '333', LOSS_QTY, 0))AS "333" 
              , SUM(DECODE(LOSS_CODE, '392', LOSS_QTY, 0))AS "392" 
              , SUM(DECODE(LOSS_CODE, '402', LOSS_QTY, 0))AS "402" 
              , SUM(DECODE(LOSS_CODE, '404', LOSS_QTY, 0))AS "404" 
              , SUM(DECODE(LOSS_CODE, '406', LOSS_QTY, 0))AS "406" 
              , SUM(DECODE(LOSS_CODE, '414', LOSS_QTY, 0))AS "414" 
              , SUM(DECODE(LOSS_CODE, '418', LOSS_QTY, 0))AS "418" 
              , SUM(DECODE(LOSS_CODE, '430', LOSS_QTY, 0))AS "430" 
              , SUM(DECODE(LOSS_CODE, '479', LOSS_QTY, 0))AS "479" 
              , SUM(DECODE(LOSS_CODE, '480', LOSS_QTY, 0))AS "480" 
              , SUM(DECODE(LOSS_CODE, '481', LOSS_QTY, 0))AS "481" 
              , SUM(DECODE(LOSS_CODE, '610', LOSS_QTY, 0))AS "610" 
              , SUM(DECODE(LOSS_CODE, '718', LOSS_QTY, 0))AS "718" 
              , SUM(DECODE(LOSS_CODE, '80', LOSS_QTY, 0))AS "80" 
              , SUM(DECODE(LOSS_CODE, '924', LOSS_QTY, 0))AS "924" 
              , SUM(DECODE(LOSS_CODE, '953', LOSS_QTY, 0))AS "953" 
           FROM RWIPLOTLSM
          WHERE HIST_DEL_FLAG = ' '

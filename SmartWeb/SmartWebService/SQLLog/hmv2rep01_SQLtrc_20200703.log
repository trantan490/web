SELECT B.MAT_TYPE
     , A.MAT_ID
     , B.MAT_DESC
     , A.OPER
     , SUM(END_QTY) AS TOTAL_QTY
     , SUM(DECODE(WORK_DATE, '20200620', END_QTY)) AS END_QTY_0
     , SUM(DECODE(WORK_DATE, '20200621', END_QTY)) AS END_QTY_1
     , SUM(DECODE(WORK_DATE, '20200622', END_QTY)) AS END_QTY_2
     , SUM(DECODE(WORK_DATE, '20200623', END_QTY)) AS END_QTY_3
     , SUM(DECODE(WORK_DATE, '20200624', END_QTY)) AS END_QTY_4
     , SUM(DECODE(WORK_DATE, '20200625', END_QTY)) AS END_QTY_5
     , SUM(DECODE(WORK_DATE, '20200626', END_QTY)) AS END_QTY_6
     , SUM(DECODE(WORK_DATE, '20200627', END_QTY)) AS END_QTY_7
     , SUM(DECODE(WORK_DATE, '20200628', END_QTY)) AS END_QTY_8
     , SUM(DECODE(WORK_DATE, '20200629', END_QTY)) AS END_QTY_9
     , SUM(DECODE(WORK_DATE, '20200630', END_QTY)) AS END_QTY_10
     , SUM(DECODE(WORK_DATE, '20200701', END_QTY)) AS END_QTY_11
     , SUM(DECODE(WORK_DATE, '20200702', END_QTY)) AS END_QTY_12
     , SUM(DECODE(WORK_DATE, '20200703', END_QTY)) AS END_QTY_13
     , ROUND(SUM(END_QTY)/14, 1) AS AVG
  FROM (
        SELECT FACTORY, MAT_ID, OPER, WORK_DATE, CM_KEY_3
             , SUM(S1_END_QTY_1+S2_END_QTY_1+S3_END_QTY_1) AS END_QTY
          FROM RSUMWIPMOV
         WHERE 1=1
           AND FACTORY = 'HMVA2'
           AND WORK_DATE BETWEEN '20200620' AND '20200703'
           AND (OPER LIKE 'V%' OR OPER LIKE 'M%')
         GROUP BY FACTORY, MAT_ID, OPER, WORK_DATE, CM_KEY_3 
       ) A
     , MWIPMATDEF B
 WHERE 1=1
   AND A.FACTORY = B.FACTORY
   AND A.MAT_ID = B.MAT_ID
   AND A.END_QTY > 0
   AND A.OPER IN ('M0330', 'M0336', 'M0337')
   AND A.MAT_ID LIKE '%'  
 GROUP BY B.MAT_TYPE, B.MAT_DESC, A.MAT_ID, A.OPER
 ORDER BY B.MAT_TYPE, B.MAT_DESC, A.MAT_ID, A.OPER


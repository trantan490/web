              , A.LOT_ID
              , A.LOT_CMF_5 AS LOT_TYPE
              , A.IN_TIME 
              , A.OUT_TIME
              , A.END_RES_ID 
              , A.TRAN_USER_ID
              , NVL(B.TOTAL_LOSS,0) + A.OUT_QTY AS IN_QTY
              , A.OUT_QTY 
              , B.LOSS_CODE
              , B.LOSS_QTY  
              , NVL(B.TOTAL_LOSS,0) AS TOTAL_LOSS  
              , A.PGM AS PGM 
           FROM (  
                 SELECT OLD_OPER AS OPER, MAT_ID, LOT_ID 
                      , LOT_CMF_5, OLD_OPER_IN_TIME AS IN_TIME, TRAN_TIME AS OUT_TIME 

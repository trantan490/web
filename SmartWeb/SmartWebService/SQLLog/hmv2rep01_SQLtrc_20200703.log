SELECT  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ', MAT_ID 
     , OPER, RUN_ID, LOT_ID, LOT_TYPE, PGM 
     , IN_TIME, OUT_TIME, END_RES_ID 
     , (SELECT USER_DESC FROM RWEBUSRDEF WHERE USER_ID = TRAN_USER_ID) || '(' || TRAN_USER_ID || ')' AS USER_ID 
     , IN_QTY, OUT_QTY, TOTAL_LOSS 
     , ROUND(OUT_QTY / IN_QTY * 100, 2) AS YIELD 
     , SUM(DECODE(LOSS_CODE, '209', LOSS_QTY, 0)) AS "209" 
     , SUM(DECODE(LOSS_CODE, '211', LOSS_QTY, 0)) AS "211" 
     , SUM(DECODE(LOSS_CODE, '302', LOSS_QTY, 0)) AS "302" 
     , SUM(DECODE(LOSS_CODE, '306', LOSS_QTY, 0)) AS "306" 
     , SUM(DECODE(LOSS_CODE, '307', LOSS_QTY, 0)) AS "307" 
     , SUM(DECODE(LOSS_CODE, '319', LOSS_QTY, 0)) AS "319" 
     , SUM(DECODE(LOSS_CODE, '320', LOSS_QTY, 0)) AS "320" 
     , SUM(DECODE(LOSS_CODE, '326', LOSS_QTY, 0)) AS "326" 
     , SUM(DECODE(LOSS_CODE, '333', LOSS_QTY, 0)) AS "333" 
     , SUM(DECODE(LOSS_CODE, '392', LOSS_QTY, 0)) AS "392" 
     , SUM(DECODE(LOSS_CODE, '402', LOSS_QTY, 0)) AS "402" 
     , SUM(DECODE(LOSS_CODE, '404', LOSS_QTY, 0)) AS "404" 
     , SUM(DECODE(LOSS_CODE, '406', LOSS_QTY, 0)) AS "406" 
     , SUM(DECODE(LOSS_CODE, '414', LOSS_QTY, 0)) AS "414" 
     , SUM(DECODE(LOSS_CODE, '418', LOSS_QTY, 0)) AS "418" 
     , SUM(DECODE(LOSS_CODE, '430', LOSS_QTY, 0)) AS "430" 
     , SUM(DECODE(LOSS_CODE, '480', LOSS_QTY, 0)) AS "480" 
     , SUM(DECODE(LOSS_CODE, '481', LOSS_QTY, 0)) AS "481" 
     , SUM(DECODE(LOSS_CODE, '610', LOSS_QTY, 0)) AS "610" 
  FROM ( 
         SELECT  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ', A.MAT_ID 
              , A.OPER
              , A.RUN_ID
              , A.LOT_ID
              , A.LOT_CMF_5 AS LOT_TYPE
              , A.IN_TIME 
              , A.OUT_TIME
              , A.END_RES_ID 
              , A.TRAN_USER_ID
              , NVL(B.TOTAL_LOSS,0) + A.OUT_QTY AS IN_QTY
              , A.OUT_QTY 
              , B.LOSS_CODE
              , B.LOSS_QTY  
              , NVL(B.TOTAL_LOSS,0) AS TOTAL_LOSS  
              , A.PGM AS PGM 
           FROM (  
                 SELECT OLD_OPER AS OPER, MAT_ID, LOT_ID 
                      , LOT_CMF_5, OLD_OPER_IN_TIME AS IN_TIME, TRAN_TIME AS OUT_TIME 
                      , END_RES_ID, TRAN_USER_ID, QTY_1 AS OUT_QTY 
                      , TRAN_CMF_3 AS PGM 
                      , LOT_CMF_4 AS RUN_ID 
                   FROM RWIPLOTHIS  
                  WHERE OLD_FACTORY  = 'HMVA2' 
                    AND MAT_VER = 1  
                    AND OLD_OPER IN ('A1000', 'A055A', 'A060A', 'A1040', 'A1070', 'A1020', 'A1100', 'A1110', 'A1120', 'A1130', 'A1140', 'A1150')
                    AND HIST_DEL_FLAG = ' ' 
                    AND TRAN_CODE IN ('END','SHIP') 
                    AND TRAN_TIME BETWEEN '20200620220000' AND '20200703215959'
                ) A 
              , (         
                 SELECT LOT_ID, OPER, LOSS_CODE, LOSS_QTY, SUM(LOSS_QTY) OVER(PARTITION BY LOT_ID, OPER) AS TOTAL_LOSS 
                   FROM RWIPLOTLSM  
                  WHERE FACTORY   = 'HMVA2' 
                    AND MAT_VER = 1  
                    AND OPER IN ('A1000', 'A055A', 'A060A', 'A1040', 'A1070', 'A1020', 'A1100', 'A1110', 'A1120', 'A1130', 'A1140', 'A1150')
                    AND HIST_DEL_FLAG = ' '   
                ) B 
              , (   
                 SELECT ATTR_KEY,ATTR_VALUE  
                   FROM MATRNAMSTS  
                  WHERE FACTORY  = 'HMVA2' 
                    AND ATTR_TYPE = 'MAT_TEST_PGM' 
                    AND ATTR_NAME = 'TEST_PROGRAM'  
                ) C 
              , MWIPMATDEF D 
          WHERE 1 = 1  
            AND A.LOT_ID = B.LOT_ID 
            AND A.OPER = B.OPER 
            AND A.MAT_ID = C.ATTR_KEY(+) 
            AND A.MAT_ID = D.MAT_ID  
            AND D.FACTORY  = 'HMVA2' 
            AND LOSS_CODE LIKE '%'
       ) DAT
 GROUP BY  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ', MAT_ID, OPER, RUN_ID, LOT_ID, LOT_TYPE, PGM, IN_TIME, OUT_TIME, END_RES_ID, TRAN_USER_ID, IN_QTY, OUT_QTY, TOTAL_LOSS 
 ORDER BY  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ', MAT_ID, OPER, RUN_ID, LOT_ID, LOT_TYPE, PGM, IN_TIME, OUT_TIME, END_RES_ID, TRAN_USER_ID, IN_QTY, OUT_QTY, TOTAL_LOSS 


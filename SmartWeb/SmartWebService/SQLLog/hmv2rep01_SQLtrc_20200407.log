WITH DT AS
(
    SELECT TRUNC(TO_DATE('20200407171603','YYYYMMDDHH24MISS') - TO_DATE(A.OPER_IN_TIME,'YYYYMMDDHH24MISS'), 2) AS TAT 
         , CASE WHEN A.FACTORY = 'HMKA1' AND B.OPER_GRP_1 = 'HMK2A' THEN 'STOCK'
                WHEN A.FACTORY = 'HMKA1' AND B.OPER_GRP_1 IN ('B/G','SAW', 'Back Side Coating') THEN 'DP'
                WHEN A.FACTORY = 'HMKA1' AND B.OPER_GRP_1 IN ('D/A','SMT', 'S/P') THEN 'DA'
                WHEN A.FACTORY = 'HMKA1' AND B.OPER_GRP_1 IN ('W/B','GATE') THEN 'WB'
                WHEN A.FACTORY = 'HMKA1' AND B.OPER_GRP_1 IN ('MOLD','CURE') THEN 'MOLD'
                WHEN A.FACTORY = 'HMKA1' THEN 'FINISH'
                WHEN A.FACTORY = 'HMKT1' AND B.OPER_GRP_1 = 'HMK3T' THEN 'STOCK'
                WHEN A.FACTORY = 'HMKT1' AND B.OPER_GRP_1 IN ('TEST', 'CAS', 'OS') THEN 'TEST'
                WHEN A.FACTORY = 'HMKT1' THEN 'MVP'
                WHEN A.FACTORY = 'FGS' AND B.OPER = 'F0000' THEN 'STOCK'
                WHEN A.FACTORY = 'HMKE1' AND B.OPER_GRP_1 = 'HMKE1' THEN 'STOCK'
                WHEN A.FACTORY = 'HMKE1' AND B.OPER_GRP_1 = 'TEST' THEN 'TEST'
                WHEN A.FACTORY = 'HMKE1' AND B.OPER_GRP_1 IN ('BAKE', 'QA', 'P/K', 'HMKE2') THEN 'MVP'
                WHEN A.FACTORY = 'HMKB1' AND B.OPER_GRP_1 IN ('HMK2B','IQC', 'I-STOCK') THEN 'Incoming'
                WHEN A.FACTORY = 'HMKB1' AND B.OPER_GRP_1 = 'RCF_PHOTO' THEN 'RCF'
                WHEN A.FACTORY = 'HMKB1' AND B.OPER_GRP_1 IN ('RDL1_SPUTTER','RDL1_PHOTO', 'RDL1_PLAT', 'RDL1_ETCH') THEN 'RDL1'
                WHEN A.FACTORY = 'HMKB1' AND B.OPER_GRP_1 = 'PSV1_PHOTO' THEN 'PSV1'
                WHEN A.FACTORY = 'HMKB1' AND B.OPER_GRP_1 IN ('RDL2_SPUTTER','RDL2_PHOTO', 'RDL2_PLAT', 'RDL2_ETCH') THEN 'RDL2'
                WHEN A.FACTORY = 'HMKB1' AND B.OPER_GRP_1 = 'PSV2_PHOTO' THEN 'PSV2'
                WHEN A.FACTORY = 'HMKB1' AND B.OPER_GRP_1 IN ('RDL3_SPUTTER','RDL3_PHOTO', 'RDL3_PLAT', 'RDL3_ETCH') THEN 'RDL3'
                WHEN A.FACTORY = 'HMKB1' AND B.OPER_GRP_1 = 'PSV3_PHOTO' THEN 'PSV3'
                WHEN A.FACTORY = 'HMKB1' AND B.OPER_GRP_1 IN ('BUMP_SPUTTER','BUMP_PHOTO','BUMP_CU_PLAT','BUMP_SNAG_PLAT','BUMP_ETCH','BUMP_BALL_MOUNT','BUMP_REFLOW','FINAL_INSP') THEN 'BUMP'
                WHEN A.FACTORY = 'HMKB1' AND B.OPER_GRP_1 IN ('SORT','AVI') THEN 'AVI'
                WHEN A.FACTORY = 'HMKB1' AND B.OPER_GRP_1 = 'OGI' THEN 'OQC'
                WHEN A.FACTORY = 'HMKB1' AND B.OPER_GRP_1 IN ('PACKING','HMK3B') THEN 'PACKING'
                ELSE ''
           END AS OPER_GRP 
         , CASE WHEN A.FACTORY = 'HMKA1' AND A.MAT_ID LIKE 'SEK%' THEN '삼성메모리'
                WHEN A.FACTORY = 'HMKA1' AND A.MAT_ID LIKE 'SES%' THEN 'S-LSI'
                WHEN A.FACTORY = 'HMKA1' AND A.MAT_ID LIKE 'HX%' THEN 'HYNIX'
                WHEN A.FACTORY = 'HMKA1' THEN 'FABLESS'
                WHEN A.FACTORY = 'HMKT1' AND A.MAT_ID LIKE 'SES%' THEN 'S-LSI'
                WHEN A.FACTORY = 'HMKT1' AND A.MAT_ID LIKE 'IM%' THEN 'IML'
                WHEN A.FACTORY = 'HMKT1' AND A.MAT_ID LIKE 'FC%' THEN 'FCI'
                WHEN A.FACTORY = 'HMKT1' THEN '기타업체'
                WHEN A.FACTORY IN ('FGS', 'HMKE1') THEN 'FABLESS'
                WHEN A.FACTORY = 'HMKB1' AND A.MAT_ID LIKE 'SEK%' THEN '삼성메모리'
                WHEN A.FACTORY = 'HMKB1' AND A.MAT_ID LIKE 'SES%' THEN 'S-LSI'
                WHEN A.FACTORY = 'HMKB1' AND A.MAT_ID LIKE 'HX%' THEN 'HYNIX'
                WHEN A.FACTORY = 'HMKB1' THEN 'FABLESS'
                ELSE ''
           END AS CUST_TYPE
         , A.* 
         , C.MAT_GRP_1, C.MAT_GRP_2, C.MAT_GRP_3, C.MAT_GRP_4, C.MAT_GRP_5
         , C.MAT_GRP_6, C.MAT_GRP_7, C.MAT_GRP_8, C.MAT_GRP_9, C.MAT_GRP_10
         , C.MAT_CMF_10, C.MAT_CMF_11, C.MAT_CMF_13
      FROM RWIPLOTSTS A
         , MWIPOPRDEF B
         , MWIPMATDEF C
     WHERE 1=1
       AND A.FACTORY = B.FACTORY
       AND A.FACTORY = C.FACTORY
       AND A.OPER = B.OPER
       AND A.MAT_ID = C.MAT_ID
       AND A.FACTORY = 'HMVA2'
       AND A.LOT_DEL_FLAG = ' '
       AND A.LOT_TYPE = 'W'
       AND A.MAT_ID LIKE '%'
)
SELECT A.OPER_GRP, A.CUST_TYPE,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ' 
     , SUM(LOT_OVER_0) AS LOT_OVER_0
     , SUM(QTY_OVER_0) AS QTY_OVER_0
     , SUM(LOT_OVER_1) AS LOT_OVER_1
     , SUM(QTY_OVER_1) AS QTY_OVER_1
     , SUM(LOT_OVER_2) AS LOT_OVER_2
     , SUM(QTY_OVER_2) AS QTY_OVER_2
     , SUM(LOT_OVER_3) AS LOT_OVER_3
     , SUM(QTY_OVER_3) AS QTY_OVER_3
     , SUM(LOT_OVER_7) AS LOT_OVER_7
     , SUM(QTY_OVER_7) AS QTY_OVER_7
     , SUM(LOT_TTL) AS LOT_TTL
     , SUM(QTY_TTL) AS QTY_TTL
     , DECODE(SUM(WIP_TTL), 0, 0, ROUND(SUM(QTY_HOLD) / SUM(WIP_TTL) * 100, 1)) AS HOLD_RATE
     , SUM(QTY_HOLD) AS QTY_HOLD
     , SUM(WIP_TTL) AS WIP_TTL
  FROM (
        SELECT OPER_GRP, CUST_TYPE, MAT_ID
             , SUM(CASE WHEN TAT < 1 THEN 1 ELSE 0 END) AS LOT_OVER_0
             , SUM(CASE WHEN TAT < 1 THEN QTY_1 ELSE 0 END) AS QTY_OVER_0
             , SUM(CASE WHEN 1 <= TAT AND TAT < 2 THEN 1 ELSE 0 END) AS LOT_OVER_1
             , SUM(CASE WHEN 1 <= TAT AND TAT < 2 THEN QTY_1 ELSE 0 END) AS QTY_OVER_1
             , SUM(CASE WHEN 2 <= TAT AND TAT < 3 THEN 1 ELSE 0 END) AS LOT_OVER_2
             , SUM(CASE WHEN 2 <= TAT AND TAT < 3 THEN QTY_1 ELSE 0 END) AS QTY_OVER_2
             , SUM(CASE WHEN 3 <= TAT AND TAT < 7 THEN 1 ELSE 0 END) AS LOT_OVER_3
             , SUM(CASE WHEN 3 <= TAT AND TAT < 7 THEN QTY_1 ELSE 0 END) AS QTY_OVER_3
             , SUM(CASE WHEN 7 <= TAT THEN 1 ELSE 0 END) AS LOT_OVER_7
             , SUM(CASE WHEN 7 <= TAT THEN QTY_1 ELSE 0 END) AS QTY_OVER_7
             , SUM(CASE WHEN 0 <= TAT THEN 1 ELSE 0 END) AS LOT_TTL
             , SUM(CASE WHEN 0 <= TAT THEN QTY_1 ELSE 0 END) AS QTY_TTL
             , SUM(CASE WHEN HOLD_FLAG = 'Y' THEN QTY_1 ELSE 0 END) AS QTY_HOLD 
          FROM DT
         WHERE 1=1
           AND TAT >= 0
         GROUP BY OPER_GRP, CUST_TYPE, MAT_ID
       ) A
     , (
        SELECT OPER_GRP, CUST_TYPE, MAT_ID, SUM(QTY_1) AS WIP_TTL FROM DT GROUP BY OPER_GRP, CUST_TYPE, MAT_ID
       ) B
     , MWIPMATDEF C
 WHERE A.OPER_GRP = B.OPER_GRP(+)
   AND A.CUST_TYPE = B.CUST_TYPE(+)
   AND A.MAT_ID = B.MAT_ID(+)
   AND A.MAT_ID = C.MAT_ID
   AND C.FACTORY = 'HMVA2'
 GROUP BY A.OPER_GRP, A.CUST_TYPE,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ' 
 ORDER BY DECODE(A.OPER_GRP, 'STOCK', 1, 'DP', 2, 'DA', 3, 'WB', 4, 'GATE', 5, 'MOLD', 6, 'TEST', 7, 9), DECODE(A.CUST_TYPE, '삼성메모리', 1, 'S-LSI', 2, 'HYNIX', 3, 'IML', 4, 'FCI', 5, 9),  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ' 


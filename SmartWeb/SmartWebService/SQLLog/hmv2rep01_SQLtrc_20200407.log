SELECT CUST_TYPE, NVL((SELECT DATA_1 FROM MGCMTBLDAT WHERE TABLE_NAME = 'H_CUSTOMER' AND KEY_1 = MAT_GRP_1 AND ROWNUM=1), '-') AS CUSTOMER, MAT_GRP_9 AS MAJOR_CODE, MAT_GRP_2 AS FAMILY,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ' 
     , ROUND(SUM(W0 * CONVERT_QTY) /1000000,0) AS W0
     , ROUND(SUM(W1 * CONVERT_QTY) /1000000,0) AS W1
     , ROUND(SUM(W2 * CONVERT_QTY) /1000000,0) AS W2
     , ROUND(SUM(W3 * CONVERT_QTY) /1000000,0) AS W3
     , ROUND(SUM(W4 * CONVERT_QTY) /1000000,0) AS W4
     , ROUND(SUM(W5 * CONVERT_QTY) /1000000,0) AS W5
     , ROUND(SUM(W6 * CONVERT_QTY) /1000000,0) AS W6
     , ROUND(SUM(W7 * CONVERT_QTY) /1000000,0) AS W7
     , ROUND(SUM(W8 * CONVERT_QTY) /1000000,0) AS W8
  FROM (
        SELECT MAT.*
             , NVL((SELECT DATA_10 FROM MGCMTBLDAT WHERE TABLE_NAME = 'H_CUSTOMER' AND FACTORY = 'HMKA1' AND  KEY_1 = MAT_GRP_1), '-') AS CUST_TYPE
             , CASE WHEN MAT.MAT_GRP_3 IN ('COB', 'BGN') THEN W0 / TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)) ELSE W0 END AS W0
             , CASE WHEN MAT.MAT_GRP_3 IN ('COB', 'BGN') THEN W1 / TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)) ELSE W1 END AS W1
             , CASE WHEN MAT.MAT_GRP_3 IN ('COB', 'BGN') THEN W2 / TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)) ELSE W2 END AS W2
             , CASE WHEN MAT.MAT_GRP_3 IN ('COB', 'BGN') THEN W3 / TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)) ELSE W3 END AS W3
             , CASE WHEN MAT.MAT_GRP_3 IN ('COB', 'BGN') THEN W4 / TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)) ELSE W4 END AS W4
             , CASE WHEN MAT.MAT_GRP_3 IN ('COB', 'BGN') THEN W5 / TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)) ELSE W5 END AS W5
             , CASE WHEN MAT.MAT_GRP_3 IN ('COB', 'BGN') THEN W6 / TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)) ELSE W6 END AS W6
             , CASE WHEN MAT.MAT_GRP_3 IN ('COB', 'BGN') THEN W7 / TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)) ELSE W7 END AS W7
             , CASE WHEN MAT.MAT_GRP_3 IN ('COB', 'BGN') THEN W8 / TO_NUMBER(DECODE(MAT.MAT_CMF_13,' ',1,MAT.MAT_CMF_13)) ELSE W8 END AS W8
             , CONVERT_QTY
          FROM MWIPMATDEF MAT
             , (
                SELECT MAT_ID
                     , SUM(DECODE(PLAN_WEEK, '202007', WW_QTY, 0)) AS W0
                     , SUM(DECODE(PLAN_WEEK, '202008', WW_QTY, 0)) AS W1
                     , SUM(DECODE(PLAN_WEEK, '202009', WW_QTY, 0)) AS W2
                     , SUM(DECODE(PLAN_WEEK, '202010', WW_QTY, 0)) AS W3
                     , SUM(DECODE(PLAN_WEEK, '202011', WW_QTY, 0)) AS W4
                     , SUM(DECODE(PLAN_WEEK, '202012', WW_QTY, 0)) AS W5
                     , SUM(DECODE(PLAN_WEEK, '202013', WW_QTY, 0)) AS W6
                     , SUM(DECODE(PLAN_WEEK, '202014', WW_QTY, 0)) AS W7
                     , SUM(DECODE(PLAN_WEEK, '202015', WW_QTY, 0)) AS W8
                  FROM RWIPPLNWEK
                 WHERE 1=1
                   AND FACTORY = 'HMVA2'
                   AND GUBUN = '3' 
                   AND PLAN_WEEK BETWEEN '202007' AND '202015'
                 GROUP BY MAT_ID
               ) PLN
             , (
                SELECT PRODUCT AS MAT_ID, PRICE AS CONVERT_QTY
                  FROM RPRIMATDAT
                 WHERE 1=1
                   AND SUBSTR(ITEM_CD, 10, 2) = 'A0'
               ) COV
         WHERE 1=1
           AND MAT.MAT_ID = COV.MAT_ID(+)
           AND MAT.MAT_ID = PLN.MAT_ID
           AND MAT.FACTORY = 'HMVA2'
           AND MAT.DELETE_FLAG = ' '
           AND MAT.MAT_TYPE = 'FG'
           AND MAT.MAT_ID LIKE '%'
       )
 GROUP BY CUST_TYPE, MAT_GRP_1, MAT_GRP_9, MAT_GRP_2,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ' 
 ORDER BY DECODE(CUST_TYPE, 'SEC', 1, 'Hynix', 2, 'Fabless', 3, 4), MAT_GRP_1, MAT_GRP_9, MAT_GRP_2,  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ',  ' ' 


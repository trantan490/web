SELECT CUST, MAT_GRP_1, MAT_GRP_10, MAT_CMF_11, MAT_GRP_5, CHIP
     , ROUND(CAPA / 1000, 0) AS CAPA
     , ROUND(MAIN_DA / 1000, 0) AS MAIN_DA
     , ROUND(MAIN_WB / 1000, 0) AS MAIN_WB
     , ROUND((WIP_DA + WIP_MERGE) / 1000, 0) AS WIP_DA
     , ROUND(WIP_2GROUP / 1000, 0) AS WIP_2GROUP
     , ROUND((WIP_2GROUP - MAX(WIP_2GROUP) OVER(PARTITION BY MAT_GRP_1, MAT_GRP_10, MAT_CMF_11)) / 1000, 0) AS DA_WIP_KIT
     , ROUND(WIP_SAW / 1000, 0) AS WIP_SAW
     , ROUND(T_OUT_SAW / 1000, 0) AS T_OUT_SAW
     , ROUND(Y_OUT_SAW / 1000, 0) AS Y_OUT_SAW
     , ROUND((-(WIP_2GROUP - MAX(WIP_2GROUP) OVER(PARTITION BY MAT_GRP_1, MAT_GRP_10, MAT_CMF_11)) - WIP_SAW) / 1000, 0) AS NEED_BG
     , ROUND(WIP_BG / 1000, 0) AS WIP_BG
     , ROUND(T_OUT_BG / 1000, 0) AS T_OUT_BG
     , ROUND(Y_OUT_BG / 1000, 0) AS Y_OUT_BG
     , ROUND((-(WIP_2GROUP - MAX(WIP_2GROUP) OVER(PARTITION BY MAT_GRP_1, MAT_GRP_10, MAT_CMF_11)) - WIP_SAW - WIP_BG) / 1000, 0) AS NEED_INPUT
     , ROUND(WIP_STOCK / 1000, 0) AS WIP_STOCK
     , ROUND(T_OUT_STOCK / 1000, 0) AS T_OUT_STOCK
     , ROUND((PLN_WEEK1 - (WIP_FRONT + WIP_BACK + WIP_2GROUP + AO_WEEK)) / 1000, 0) AS T_WEEK_IN_DEF
     , ROUND(((PLN_WEEK1 + PLN_WEEK2) - (WIP_FRONT + WIP_BACK + WIP_2GROUP + AO_WEEK)) / 1000, 0) AS N_WEEK_IN_DEF
     , ROUND(((PLN_WEEK1 + PLN_WEEK2 + PLN_WEEK3) - (WIP_FRONT + WIP_BACK + WIP_2GROUP + AO_WEEK)) / 1000, 0) AS NN_WEEK_IN_DEF
     , ROUND(((PLN_WEEK1 + PLN_WEEK2 + PLN_WEEK3 + PLN_WEEK4) - (WIP_FRONT + WIP_BACK + WIP_2GROUP + AO_WEEK)) / 1000, 0) AS NNN_WEEK_IN_DEF
     , ROUND((PLN_MONTH - (WIP_FRONT + WIP_BACK + WIP_2GROUP + AO_MONTH)) / 1000, 0) AS MONTH_IN_DEF
     , ROUND((((PLN_WEEK1 - (WIP_FRONT + WIP_BACK + WIP_2GROUP + AO_WEEK)) - WIP_STOCK)) / 1000, 0) AS T_WEEK_IN_WF
     , ROUND(((((PLN_WEEK1 + PLN_WEEK2) - (WIP_FRONT + WIP_BACK + WIP_2GROUP + AO_WEEK)) - WIP_STOCK)) / 1000, 0) AS N_WEEK_IN_WF
     , ROUND(((((PLN_WEEK1 + PLN_WEEK2 + PLN_WEEK3) - (WIP_FRONT + WIP_BACK + WIP_2GROUP + AO_WEEK)) - WIP_STOCK)) / 1000, 0) AS NN_WEEK_IN_WF
     , ROUND(((((PLN_WEEK1 + PLN_WEEK2 + PLN_WEEK3 + PLN_WEEK4) - (WIP_FRONT + WIP_BACK + WIP_2GROUP + AO_WEEK)) - WIP_STOCK)) / 1000, 0) AS NNN_WEEK_IN_WF
     , ROUND((((PLN_MONTH - (WIP_FRONT + WIP_BACK + WIP_2GROUP + AO_MONTH)) - WIP_STOCK)) / 1000, 0) AS MONTH_IN_WF
     , OPER_STEALTH
     , ROUND(UPH_STEALTH / 1000, 0) AS UPH_STEALTH
     , ROUND(WIP_STEALTH / 1000, 0) AS WIP_STEALTH
     , ROUND(WIP_PREBG / 1000, 0) AS WIP_PREBG
     , ROUND(WIP_LAMI / 1000, 0) AS WIP_LAMI
     , ROUND(WIP_STOCK / 1000, 0) AS WIP_STOCK
     , ROUND((WIP_STOCK + WIP_LAMI + WIP_PREBG + WIP_STEALTH) / 1000, 0) AS WIP_TTL
     , CASE WHEN OPER_STEALTH = 'O' THEN ROUND((PLN_WEEK1 - (WIP_FRONT + WIP_BACK + WIP_2GROUP + AO_WEEK) + WIP_STEALTH + WIP_PREBG + WIP_LAMI) / 1000, 0)
            ELSE 0
       END AS T_WEEK_STEALTH_IN_DEF
     , CASE WHEN OPER_STEALTH = 'O' THEN ROUND(((PLN_WEEK1 + PLN_WEEK2) - (WIP_FRONT + WIP_BACK + WIP_2GROUP + AO_WEEK) + WIP_STEALTH + WIP_PREBG + WIP_LAMI) / 1000, 0)
            ELSE 0
       END  AS N_WEEK_STEALTH_IN_DEF
     , CASE WHEN OPER_STEALTH = 'O' THEN ROUND((PLN_MONTH - (WIP_FRONT + WIP_BACK + WIP_2GROUP + AO_MONTH) + WIP_STEALTH + WIP_PREBG + WIP_LAMI) / 1000, 0)
            ELSE 0
       END AS MONTH_STEALTH_IN_DEF
     , BG_1, WM_TYPE1, NET_DIE, COMP_CNT, WIRE_CNT
  FROM (
        SELECT A.MAT_GRP_1
             , A.MAT_GRP_10
             , A.MAT_CMF_11
             , A.MAT_GRP_5
             , A.CUST
             , A.CHIP
             , A.NET_DIE
             , NVL(D.CAPA,0) AS CAPA
             , A.WIP_DA_WB, B.WIP_MID_MER, B.WIP_MID_MER_3, B.WIP_MID_MER_4, B.WIP_MID_MER_5
             , CASE WHEN A.MAT_GRP_5 IN ('-', '1st') THEN NVL(A.WIP_DA_WB,0) + NVL(B.WIP_MID_MER,0)
                    WHEN A.MAT_GRP_5 = '2nd' THEN NVL(A.WIP_DA_WB,0) + NVL(B.WIP_MID_MER,0)
                    WHEN A.MAT_GRP_5 = '3rd' THEN NVL(A.WIP_DA_WB,0) + NVL(B.WIP_MID_MER_3,0)
                    WHEN A.MAT_GRP_5 = '4th' THEN NVL(A.WIP_DA_WB,0) + NVL(B.WIP_MID_MER_4,0)
                    WHEN A.MAT_GRP_5 = '5th' THEN NVL(A.WIP_DA_WB,0) + NVL(B.WIP_MID_MER_5,0)
                    WHEN A.MAT_GRP_5 = '6th' THEN NVL(A.WIP_DA_WB,0) + NVL(B.WIP_MID_MER_6,0)
                    WHEN A.MAT_GRP_5 = '7th' THEN NVL(A.WIP_DA_WB,0) + NVL(B.WIP_MID_MER_7,0)
                    WHEN A.MAT_GRP_5 = '8th' THEN NVL(A.WIP_DA_WB,0) + NVL(B.WIP_MID_MER_8,0)
                    WHEN A.MAT_GRP_5 = '9th' THEN NVL(A.WIP_DA_WB,0) + NVL(B.WIP_MID_MER_9,0)
                    ELSE 0
               END WIP_2GROUP
             , NVL(A.WIP_SAW,0) AS WIP_SAW
             , NVL(A.Y_OUT_SAW,0) AS Y_OUT_SAW
             , NVL(A.T_OUT_SAW,0) AS T_OUT_SAW
             , NVL(A.WIP_BG,0) AS WIP_BG
             , NVL(A.Y_OUT_BG,0) AS Y_OUT_BG
             , NVL(A.T_OUT_BG,0) AS T_OUT_BG
             , NVL(A.WIP_STOCK,0) AS WIP_STOCK
             , NVL(A.T_OUT_STOCK,0) AS T_OUT_STOCK
             , NVL(E.PLN_WEEK1,0) AS PLN_WEEK1
             , NVL(E.PLN_WEEK2,0) AS PLN_WEEK2
             , NVL(E.PLN_WEEK3,0) AS PLN_WEEK3
             , NVL(E.PLN_WEEK4,0) AS PLN_WEEK4
             , NVL(E.PLN_MONTH,0) AS PLN_MONTH
             , NVL(A.WIP_FRONT,0) AS WIP_FRONT
             , NVL(B.WIP_BACK,0) AS WIP_BACK
             , NVL(C.AO_WEEK,0) AS AO_WEEK
             , NVL(C.AO_MONTH,0) AS AO_MONTH
             , NVL(A.WIP_DA,0) AS WIP_DA
             , NVL(F.WIP_MERGE,0) AS WIP_MERGE
             , NVL(G.MAIN_DA,0) AS MAIN_DA
             , NVL(G.MAIN_WB,0) AS MAIN_WB
             , A.OPER_STEALTH
             , NVL(A.UPH_STEALTH,0) AS UPH_STEALTH
             , NVL(A.WIP_LAMI,0) AS WIP_LAMI
             , NVL(A.WIP_PREBG,0) AS WIP_PREBG
             , NVL(A.WIP_STEALTH,0) AS WIP_STEALTH
             , A.BG_1
             , A.WM_TYPE1
             , A.COMP_CNT
             , A.WIRE_CNT
          FROM (
                SELECT MAT_GRP_1, MAT_GRP_10, MAT_CMF_11, MAT_GRP_5, MAX(NET_DIE) AS NET_DIE
                     , MAX(CASE WHEN MAT.MAT_ID LIKE 'SEK%' THEN 'SEK'
                                WHEN MAT.MAT_ID LIKE 'SES%' THEN 'SES'
                                WHEN MAT.MAT_ID LIKE 'HX%' THEN 'HX'
                                ELSE 'ETC'
                           END) CUST
                     , MAX(CASE WHEN MAT.MAT_ID LIKE 'SEKS3%' THEN SUBSTR(MAT.MAT_ID, INSTR(MAT.MAT_ID, '-')-3, 3)
                                WHEN MAT.MAT_ID LIKE 'SEK9%' THEN 'N'
                                WHEN MAT.MAT_ID LIKE 'SEKY%' THEN 'S'
                                WHEN MAT.MAT_ID LIKE 'SEK%' THEN 'D'
                                ELSE ' '
                           END) CHIP
                     , SUM(WIP_STOCK / COMP_CNT) AS WIP_STOCK
                     , SUM(WIP_BG / COMP_CNT) AS WIP_BG
                     , SUM(WIP_SAW / COMP_CNT) AS WIP_SAW
                     , SUM(WIP_LAMI / COMP_CNT) AS WIP_LAMI
                     , SUM(WIP_PREBG / COMP_CNT) AS WIP_PREBG
                     , SUM(WIP_STEALTH / COMP_CNT) AS WIP_STEALTH
                     , SUM(WIP_DA_WB) AS WIP_DA_WB
                     , SUM(WIP_FRONT1 / COMP_CNT) + SUM(WIP_FRONT2) AS WIP_FRONT
                     , CASE WHEN MAT_GRP_5 = '1st' THEN SUM(WIP_DA1 / COMP_CNT) + SUM(WIP_DA2)
                            ELSE SUM(WIP_DA1 / COMP_CNT) + SUM(WIP_DA2) + SUM(WIP_DA3)
                       END AS WIP_DA
                     , SUM(T_OUT_STOCK / COMP_CNT) AS T_OUT_STOCK
                     , SUM(Y_OUT_BG / COMP_CNT) AS Y_OUT_BG
                     , SUM(T_OUT_BG / COMP_CNT) AS T_OUT_BG
                     , SUM(Y_OUT_SAW / COMP_CNT) AS Y_OUT_SAW
                     , SUM(T_OUT_SAW / COMP_CNT) AS T_OUT_SAW
                     , MAX(OPER_STEALTH) AS OPER_STEALTH
                     , AVG(UPH_STEALTH) AS UPH_STEALTH
                     , MAX(BG_1) AS BG_1
                     , MAX(WM_TYPE1) AS WM_TYPE1
                     , MAX(COMP_CNT) AS COMP_CNT
                     , MAX(WIRE_CNT) AS WIRE_CNT
                  FROM (
                        SELECT A.*
                             , (SELECT DECODE(OPER, 'A0033', 'O') FROM MWIPFLWOPR@RPTTOMES WHERE FACTORY = A.FACTORY AND FLOW = A.FIRST_FLOW AND OPER = 'A0033') AS OPER_STEALTH
                             , (SELECT AVG(UPEH) FROM CRASUPHDEF WHERE FACTORY = A.FACTORY AND MAT_ID = A.MAT_ID AND OPER = 'A0033') AS UPH_STEALTH
                             , B.BG_1
                             , B.WM_TYPE1
                             , (SELECT MAX(TCD_CMF_2) FROM CWIPTCDDEF@RPTTOMES WHERE FACTORY = 'HMKA1' AND OPER LIKE 'A060%' AND SET_FLAG = 'Y' AND TCD_CMF_2 <> ' ' AND MAT_ID = A.MAT_ID) AS WIRE_CNT
                          FROM VWIPMATDEF A
                             , CLOTCRDDAT@RPTTOMES B
                         WHERE 1=1
                           AND A.FACTORY = B.FACTORY(+)
                           AND A.MAT_ID = B.MAT_ID(+)
                           AND A.FACTORY = 'HMKA1'
                           AND A.MAT_TYPE = 'FG'
                           AND A.DELETE_FLAG = ' '
                           AND A.MAT_GRP_5 <> 'Merge'
                           AND A.MAT_GRP_5 NOT LIKE 'Middle%'
                           AND A.MAT_ID LIKE 'SEK%'
                       ) MAT
                     , (
                        SELECT MAT_ID
                             , SUM(DECODE(OPER, 'A0000', QTY_1, 0)) AS WIP_STOCK
                             , SUM(CASE WHEN OPER BETWEEN 'A0001' AND 'A0040' THEN QTY_1 ELSE 0 END) AS WIP_BG
                             , SUM(CASE WHEN OPER BETWEEN 'A0041' AND 'A0300' AND OPER <> 'A0250' THEN QTY_1 ELSE 0 END) AS WIP_SAW
                             , SUM(CASE WHEN OPER BETWEEN 'A0400' AND 'A0809' THEN QTY_1 ELSE 0 END) AS WIP_DA_WB
                             , SUM(CASE WHEN OPER BETWEEN 'A0020' AND 'A0395' THEN QTY_1 ELSE 0 END) AS WIP_FRONT1
                             , SUM(CASE WHEN OPER BETWEEN 'A0396' AND 'A0399' THEN QTY_1 ELSE 0 END) AS WIP_FRONT2
                             , SUM(CASE WHEN OPER BETWEEN 'A0301' AND 'A0395' OR OPER = 'A0250' THEN QTY_1 ELSE 0 END) AS WIP_DA1
                             , SUM(CASE WHEN OPER BETWEEN 'A0396' AND 'A0401' THEN QTY_1 ELSE 0 END) AS WIP_DA2
                             , SUM(CASE WHEN OPER BETWEEN 'A0402' AND 'A0409' THEN QTY_1 ELSE 0 END) AS WIP_DA3
                             , SUM(CASE WHEN OPER BETWEEN 'A0020' AND 'A0029' THEN QTY_1 ELSE 0 END) AS WIP_LAMI
                             , SUM(DECODE(OPER, 'A0030', QTY_1, 0)) AS WIP_PREBG
                             , SUM(DECODE(OPER, 'A0033', QTY_1, 0)) AS WIP_STEALTH
                          FROM RWIPLOTSTS 
                         WHERE 1=1  
                           AND LOT_CMF_5 LIKE 'P%'
                           AND FACTORY = 'HMKA1'
                           AND LOT_DEL_FLAG = ' '
                           AND LOT_TYPE = 'W'
                         GROUP BY MAT_ID
                       ) WIP
                     , (
                        SELECT MAT_ID
                             , SUM(CASE WHEN WORK_DATE = '20200406' AND OPER = 'A0040' THEN END_QTY ELSE 0 END) AS Y_OUT_BG
                             , SUM(CASE WHEN WORK_DATE = '20200407' AND OPER = 'A0040' THEN END_QTY ELSE 0 END) AS T_OUT_BG
                             , SUM(CASE WHEN WORK_DATE = '20200406' AND OPER IN ('A0200', 'A0230') THEN END_QTY ELSE 0 END) AS Y_OUT_SAW
                             , SUM(CASE WHEN WORK_DATE = '20200407' AND OPER IN ('A0200', 'A0230') THEN END_QTY ELSE 0 END) AS T_OUT_SAW
                          FROM (
                                SELECT WORK_DATE, MAT_ID, OPER, B.FLOW
                                     , CASE WHEN B.FLOW IS NOT NULL AND OPER = 'A0200' THEN 0
                                            ELSE (S1_END_QTY_1+S2_END_QTY_1+S3_END_QTY_1)
                                       END AS END_QTY 
                                  FROM RSUMWIPMOV A
                                     , (
                                        SELECT FLOW, COUNT(*) AS CNT
                                          FROM MWIPFLWOPR@RPTTOMES
                                         WHERE FACTORY = 'HMKA1'
                                           AND OPER IN ('A0200', 'A0230')
                                         GROUP BY FLOW
                                        HAVING COUNT(*) > 1
                                       ) B
                                 WHERE 1=1
                                   AND A.FLOW = B.FLOW(+)
                                   AND FACTORY = 'HMKA1'
                                   AND WORK_DATE IN ('20200406', '20200407')
                                   AND LOT_TYPE = 'W'
                                   AND OPER IN ('A0040', 'A0200', 'A0230')
                                   AND CM_KEY_3 LIKE 'P%'
                               )
                         GROUP BY MAT_ID
                       ) SHP   
                     , (
                        SELECT MAT_ID
                             , SUM(RCV_QTY_1) AS T_OUT_STOCK
                          FROM VSUMWIPRCV 
                         WHERE 1=1
                           AND WORK_DATE = '20200407'
                           AND LOT_TYPE = 'W'
                           AND FACTORY = 'HMKA1'
                           AND CM_KEY_2 = 'PROD'   
                           AND CM_KEY_3 LIKE 'P%'
                         GROUP BY MAT_ID
                       ) RCV 
                 WHERE 1=1
                   AND MAT.MAT_ID = WIP.MAT_ID(+)
                   AND MAT.MAT_ID = SHP.MAT_ID(+)
                   AND MAT.MAT_ID = RCV.MAT_ID(+)
                 GROUP BY MAT_GRP_1, MAT_GRP_10, MAT_CMF_11, MAT_GRP_5
               ) A
             , (
                SELECT A.MAT_GRP_1, A.MAT_GRP_10, A.MAT_CMF_11 
                     , MAX(CASE WHEN A.MAT_ID LIKE 'SEK%' THEN 'SEK'
                                WHEN A.MAT_ID LIKE 'SES%' THEN 'SES'
                                WHEN A.MAT_ID LIKE 'HX%' THEN 'HX'
                                ELSE 'ETC'
                           END) CUST
                     , SUM(CASE WHEN OPER BETWEEN 'A0400' AND 'A0609' AND MAT_GRP_5 NOT IN ('-') THEN QTY_1 ELSE 0 END) AS WIP_MID_MER
                     , SUM(CASE WHEN OPER BETWEEN 'A0400' AND 'A0609' AND MAT_GRP_5 NOT IN ('-', 'Middle') THEN QTY_1 ELSE 0 END) AS WIP_MID_MER_3
                     , SUM(CASE WHEN OPER BETWEEN 'A0400' AND 'A0609' AND MAT_GRP_5 NOT IN ('-', 'Middle', 'Middle 1') THEN QTY_1 ELSE 0 END) AS WIP_MID_MER_4
                     , SUM(CASE WHEN OPER BETWEEN 'A0400' AND 'A0609' AND MAT_GRP_5 NOT IN ('-', 'Middle', 'Middle 1', 'Middle 2') THEN QTY_1 ELSE 0 END) AS WIP_MID_MER_5
                     , SUM(CASE WHEN OPER BETWEEN 'A0400' AND 'A0609' AND MAT_GRP_5 NOT IN ('-', 'Middle', 'Middle 1', 'Middle 2', 'Middle 3') THEN QTY_1 ELSE 0 END) AS WIP_MID_MER_6
                     , SUM(CASE WHEN OPER BETWEEN 'A0400' AND 'A0609' AND MAT_GRP_5 NOT IN ('-', 'Middle', 'Middle 1', 'Middle 2', 'Middle 3', 'Middle 4') THEN QTY_1 ELSE 0 END) AS WIP_MID_MER_7
                     , SUM(CASE WHEN OPER BETWEEN 'A0400' AND 'A0609' AND MAT_GRP_5 NOT IN ('-', 'Middle', 'Middle 1', 'Middle 2', 'Middle 3', 'Middle 4', 'Middle 5') THEN QTY_1 ELSE 0 END) AS WIP_MID_MER_8
                     , SUM(CASE WHEN OPER BETWEEN 'A0400' AND 'A0609' AND MAT_GRP_5 NOT IN ('-', 'Middle', 'Middle 1', 'Middle 2', 'Middle 3', 'Middle 4', 'Middle 5', 'Middle 6') THEN QTY_1 ELSE 0 END) AS WIP_MID_MER_9
                     , SUM(CASE WHEN OPER BETWEEN 'A0400' AND 'A0609' THEN 0
                                WHEN OPER = 'A0800' THEN 0
                                WHEN OPER BETWEEN 'A0801' AND 'A0809' AND SUBSTR(MAT_GRP_4, -1) = SUBSTR(OPER, -1) THEN QTY_1
                                ELSE QTY_1
                           END) AS WIP_BACK
                  FROM MWIPMATDEF A
                     , RWIPLOTSTS B
                 WHERE 1=1  
                   AND A.FACTORY = B.FACTORY
                   AND A.MAT_ID = B.MAT_ID
                   AND B.FACTORY = 'HMKA1'
                   AND B.OPER BETWEEN 'A0400' AND 'AZ010'
                   AND B.LOT_TYPE = 'W'
                   AND B.LOT_DEL_FLAG = ' '
                   AND A.DELETE_FLAG = ' '
                   AND REGEXP_LIKE(A.MAT_GRP_5, 'Middle*|Merge|-')
                   AND B.LOT_CMF_5 LIKE 'P%'
                 GROUP BY A.MAT_GRP_1, A.MAT_GRP_10, A.MAT_CMF_11 
               ) B
             , (
                SELECT A.MAT_GRP_1, A.MAT_GRP_10, A.MAT_CMF_11 
                     , MAX(CASE WHEN A.MAT_ID LIKE 'SEK%' THEN 'SEK'
                                WHEN A.MAT_ID LIKE 'SES%' THEN 'SES'
                                WHEN A.MAT_ID LIKE 'HX%' THEN 'HX'
                                ELSE 'ETC'
                           END) CUST
                     , SUM(CASE WHEN WORK_DATE BETWEEN '20200404' AND '20200407' THEN (S1_FAC_OUT_QTY_1+S2_FAC_OUT_QTY_1+S3_FAC_OUT_QTY_1) ELSE 0 END) AS AO_WEEK
                     , SUM(CASE WHEN WORK_DATE BETWEEN '20200401' AND '20200407' THEN (S1_FAC_OUT_QTY_1+S2_FAC_OUT_QTY_1+S3_FAC_OUT_QTY_1) ELSE 0 END) AS AO_MONTH
                  FROM MWIPMATDEF A
                     , RSUMFACMOV B
                 WHERE 1=1
                   AND A.FACTORY = B.CM_KEY_1
                   AND A.MAT_ID = B.MAT_ID
                   AND B.WORK_DATE BETWEEN '20200401' AND '20200407'
                   AND B.FACTORY <> 'RETURN'
                   AND B.CM_KEY_1 = 'HMKA1'
                   AND B.LOT_TYPE = 'W'   
                   AND B.CM_KEY_3 LIKE 'P%'
                   AND A.DELETE_FLAG = ' '
                 GROUP BY A.MAT_GRP_1, A.MAT_GRP_10, A.MAT_CMF_11 
               ) C
             , (                
                SELECT MAT_GRP_1, MAT_GRP_10, MAT_CMF_11
                     , MAX(CASE WHEN RES.MAT_ID LIKE 'SEK%' THEN 'SEK'
                                WHEN RES.MAT_ID LIKE 'SES%' THEN 'SES'
                                WHEN RES.MAT_ID LIKE 'HX%' THEN 'HX'
                                ELSE 'ETC'
                           END) CUST
                     , STACK
                     , SUM(TRUNC(NVL(NVL(UPH.UPEH,0) * 24 * 0.7 * RES.RAS_CNT, 0))) AS CAPA
                  FROM ( 
                        SELECT B.FACTORY, B.RES_STS_2 AS MAT_ID
                             , A.MAT_GRP_1, A.MAT_GRP_10, A.MAT_CMF_11
                             , B.RES_STS_8 AS OPER
                             , CASE WHEN B.RES_STS_8 IN ('A0400','A0401') THEN '1st'
                                    WHEN B.RES_STS_8 = 'A0402' THEN '2nd'
                                    WHEN B.RES_STS_8 = 'A0403' THEN '3rd'
                                    WHEN B.RES_STS_8 = 'A0404' THEN '4th'
                                    WHEN B.RES_STS_8 = 'A0405' THEN '5th'
                                    WHEN B.RES_STS_8 = 'A0406' THEN '6th'
                                    WHEN B.RES_STS_8 = 'A0407' THEN '7th'
                                    WHEN B.RES_STS_8 = 'A0408' THEN '8th'
                                    WHEN B.RES_STS_8 = 'A0409' THEN '9th'
                                    ELSE ''
                               END STACK
                             , B.RES_GRP_6 AS RES_MODEL, B.RES_GRP_7 AS UPEH_GRP, COUNT(B.RES_ID) AS RAS_CNT 
                          FROM MWIPMATDEF A
                             , MRASRESDEF B  
                         WHERE 1=1  
                           AND A.FACTORY = B.FACTORY 
                           AND A.MAT_ID = B.RES_STS_2 
                           AND B.FACTORY = 'HMKA1' 
                           AND B.RES_CMF_9 = 'Y' 
                           AND B.RES_CMF_7 = 'Y' 
                           AND B.DELETE_FLAG = ' '
                           AND B.RES_STS_8 LIKE 'A04%'
                         GROUP BY B.FACTORY, B.RES_STS_2, A.MAT_GRP_1, A.MAT_GRP_10, A.MAT_CMF_11, B.RES_STS_8, B.RES_GRP_6, B.RES_GRP_7 
                       ) RES 
                     , CRASUPHDEF UPH
                 WHERE 1=1 
                   AND RES.FACTORY = UPH.FACTORY(+) 
                   AND RES.OPER = UPH.OPER(+) 
                   AND RES.RES_MODEL = UPH.RES_MODEL(+) 
                   AND RES.UPEH_GRP = UPH.UPEH_GRP(+) 
                   AND RES.MAT_ID = UPH.MAT_ID(+)
                 GROUP BY MAT_GRP_1, MAT_GRP_10, MAT_CMF_11, STACK
               ) D
             , (  
                SELECT A.MAT_GRP_1, A.MAT_GRP_10, A.MAT_CMF_11
                     , MAX(CASE WHEN A.MAT_ID LIKE 'SEK%' THEN 'SEK'
                                WHEN A.MAT_ID LIKE 'SES%' THEN 'SES'
                                WHEN A.MAT_ID LIKE 'HX%' THEN 'HX'
                                ELSE 'ETC'
                           END) CUST
                     , SUM(PLN_WEEK1) AS PLN_WEEK1
                     , SUM(PLN_WEEK2) AS PLN_WEEK2
                     , SUM(PLN_WEEK3) AS PLN_WEEK3
                     , SUM(PLN_WEEK4) AS PLN_WEEK4
                     , SUM(PLN_MONTH) AS PLN_MONTH
                  FROM MWIPMATDEF A 
                     , (
                        SELECT MAT_ID
                             , DECODE(PLAN_WEEK, '202015', WW_QTY, 0) AS PLN_WEEK1
                             , DECODE(PLAN_WEEK, '202016', WW_QTY, 0) AS PLN_WEEK2
                             , DECODE(PLAN_WEEK, '202017', WW_QTY, 0) AS PLN_WEEK3
                             , DECODE(PLAN_WEEK, '202018', WW_QTY, 0) AS PLN_WEEK4
                             , 0 AS PLN_MONTH
                          FROM RWIPPLNWEK
                         WHERE 1=1
                           AND FACTORY = 'HMKA1'
                           AND PLAN_WEEK IN ('202015','202016','202017','202018')
                           AND GUBUN = '3' 
                         UNION ALL
                        SELECT MAT_ID
                             , 0 AS PLN_WEEK1, 0 AS PLN_WEEK2, 0 AS PLN_WEEK3, 0 AS PLN_WEEK4
                             , TO_NUMBER(RESV_FIELD1) AS PLN_MONTH
                          FROM CWIPPLNMON
                         WHERE 1=1
                           AND FACTORY = 'HMKA1'
                           AND PLAN_MONTH = '202004'
                           AND DECODE(RESV_FIELD1, ' ', 0, RESV_FIELD1) > 0
                       ) B
                 WHERE 1=1
                   AND A.MAT_ID = B.MAT_ID
                   AND A.FACTORY = 'HMKA1'
                   AND A.DELETE_FLAG = ' '
                 GROUP BY A.MAT_GRP_1, A.MAT_GRP_10, A.MAT_CMF_11
               ) E
             , (
                SELECT A.MAT_GRP_1, A.MAT_GRP_10, A.MAT_CMF_11 
                     , MAX(CASE WHEN A.MAT_ID LIKE 'SEK%' THEN 'SEK'
                                WHEN A.MAT_ID LIKE 'SES%' THEN 'SES'
                                WHEN A.MAT_ID LIKE 'HX%' THEN 'HX'
                                ELSE 'ETC'
                           END) CUST
                     , CASE WHEN OPER = 'A0402' THEN '2nd'
                            WHEN OPER = 'A0403' THEN '3rd'
                            WHEN OPER = 'A0404' THEN '4th'
                            WHEN OPER = 'A0405' THEN '5th'
                            WHEN OPER = 'A0406' THEN '6th'
                            WHEN OPER = 'A0407' THEN '7th'
                            WHEN OPER = 'A0408' THEN '8th'
                            WHEN OPER = 'A0409' THEN '9th'
                            ELSE ''
                       END MAT_GRP_5
                     , SUM(CASE WHEN OPER = 'A0402' AND MAT_GRP_5 IN ('Middle','Merge') THEN QTY_1
                                WHEN OPER = 'A0403' AND MAT_GRP_5 IN ('Middle 1','Merge') THEN QTY_1
                                WHEN OPER = 'A0404' AND MAT_GRP_5 IN ('Middle 2','Merge') THEN QTY_1
                                WHEN OPER = 'A0405' AND MAT_GRP_5 IN ('Middle 3','Merge') THEN QTY_1
                                WHEN OPER = 'A0406' AND MAT_GRP_5 IN ('Middle 4','Merge') THEN QTY_1
                                WHEN OPER = 'A0407' AND MAT_GRP_5 IN ('Middle 5','Merge') THEN QTY_1
                                WHEN OPER = 'A0408' AND MAT_GRP_5 IN ('Middle 6','Merge') THEN QTY_1
                                WHEN OPER = 'A0409' AND MAT_GRP_5 IN ('Middle 7','Merge') THEN QTY_1
                                ELSE 0 
                           END) AS WIP_MERGE 
                  FROM MWIPMATDEF A
                     , RWIPLOTSTS B
                 WHERE 1=1  
                   AND A.FACTORY = B.FACTORY
                   AND A.MAT_ID = B.MAT_ID
                   AND B.FACTORY = 'HMKA1'
                   AND B.OPER BETWEEN 'A0402' AND 'A0409'
                   AND B.LOT_TYPE = 'W'
                   AND B.LOT_DEL_FLAG = ' '
                   AND A.DELETE_FLAG = ' '
                   AND REGEXP_LIKE(A.MAT_GRP_5, 'Middle*|Merge')
                   AND B.LOT_CMF_5 LIKE 'P%'
                 GROUP BY A.MAT_GRP_1, A.MAT_GRP_10, A.MAT_CMF_11, B.OPER 
               ) F
             , (
                SELECT MAT_GRP_1, MAT_GRP_10, MAT_CMF_11, CUST, MAT_GRP_5 
                     , SUM(MAIN_DA) AS MAIN_DA
                     , SUM(MAIN_WB) AS MAIN_WB
                  FROM (
                        SELECT B.MAT_GRP_1, B.MAT_GRP_10, B.MAT_CMF_11, OPER
                             , CASE WHEN A.MAT_ID LIKE 'SEK%' THEN 'SEK'
                                    WHEN A.MAT_ID LIKE 'SES%' THEN 'SES'
                                    WHEN A.MAT_ID LIKE 'HX%' THEN 'HX'
                                    ELSE 'ETC'
                               END CUST
                             , CASE WHEN OPER IN ('A0400', 'A0401', 'A0250', 'A0333', 'A0340', 'A0370', 'A0380', 'A0310') THEN '1st' 
                                    WHEN OPER IN ('A0550', 'A0551', 'A0600','A0601', 'A0500', 'A0501', 'A0530', 'A0531') THEN '1st' 
                                    WHEN OPER IN ('A0402') THEN '2nd' 
                                    WHEN OPER = 'A0801' AND MAT_GRP_1 = 'SE' AND MAT_GRP_5 <> 'Merge' THEN '2nd' 
                                    WHEN OPER = 'A0801' AND MAT_GRP_1 <> 'SE' AND SUBSTR(MAT_GRP_4,-1) <> SUBSTR(OPER, -1) THEN '2nd' 
                                    WHEN OPER IN ('A0552', 'A0602', 'A0502', 'A0532') THEN '2nd' 
                                    WHEN OPER IN ('A0403') THEN '3rd' 
                                    WHEN OPER = 'A0802' AND MAT_GRP_1 = 'SE' AND MAT_GRP_5 <> 'Merge' THEN '3rd' 
                                    WHEN OPER = 'A0802' AND MAT_GRP_1 <> 'SE' AND SUBSTR(MAT_GRP_4,-1) <> SUBSTR(OPER, -1) THEN '3rd' 
                                    WHEN OPER IN ('A0553', 'A0603', 'A0503', 'A0533') THEN '3rd' 
                                    WHEN OPER IN ('A0404') THEN '4th' 
                                    WHEN OPER = 'A0803' AND MAT_GRP_1 = 'SE' AND MAT_GRP_5 <> 'Merge' THEN '4th' 
                                    WHEN OPER = 'A0803' AND MAT_GRP_1 <> 'SE' AND SUBSTR(MAT_GRP_4,-1) <> SUBSTR(OPER, -1) THEN '4th' 
                                    WHEN OPER IN ('A0554', 'A0604', 'A0504', 'A0534') THEN '4th' 
                                    WHEN OPER IN ('A0405') THEN '5th' 
                                    WHEN OPER = 'A0804' AND MAT_GRP_1 = 'SE' AND MAT_GRP_5 <> 'Merge' THEN '5th' 
                                    WHEN OPER = 'A0804' AND MAT_GRP_1 <> 'SE' AND SUBSTR(MAT_GRP_4,-1) <> SUBSTR(OPER, -1) THEN '5th'
                                    WHEN OPER IN ('A0555', 'A0605', 'A0505', 'A0535') THEN '5th'
                                    WHEN OPER IN ('A0406') THEN '6th' 
                                    WHEN OPER = 'A0805' AND MAT_GRP_1 = 'SE' AND MAT_GRP_5 <> 'Merge' THEN '6th' 
                                    WHEN OPER = 'A0805' AND MAT_GRP_1 <> 'SE' AND SUBSTR(MAT_GRP_4,-1) <> SUBSTR(OPER, -1) THEN '6th'
                                    WHEN OPER IN ('A0556', 'A0606', 'A0506', 'A0536') THEN '6th'
                                    WHEN OPER IN ('A0407') THEN '7th' 
                                    WHEN OPER = 'A0806' AND MAT_GRP_1 = 'SE' AND MAT_GRP_5 <> 'Merge' THEN '7th' 
                                    WHEN OPER = 'A0806' AND MAT_GRP_1 <> 'SE' AND SUBSTR(MAT_GRP_4,-1) <> SUBSTR(OPER, -1) THEN '7th'
                                    WHEN OPER IN ('A0557', 'A0607', 'A0507', 'A0537') THEN '7th'
                                    WHEN OPER IN ('A0408') THEN '8th' 
                                    WHEN OPER = 'A0807' AND MAT_GRP_1 = 'SE' AND MAT_GRP_5 <> 'Merge' THEN '8th' 
                                    WHEN OPER = 'A0807' AND MAT_GRP_1 <> 'SE' AND SUBSTR(MAT_GRP_4,-1) <> SUBSTR(OPER, -1) THEN '8th'
                                    WHEN OPER IN ('A0558', 'A0608', 'A0508', 'A0538') THEN '8th'
                                    WHEN OPER IN ('A0409') THEN '9th' 
                                    WHEN OPER = 'A0808' AND MAT_GRP_1 = 'SE' AND MAT_GRP_5 <> 'Merge' THEN '9th' 
                                    WHEN OPER = 'A0808' AND MAT_GRP_1 <> 'SE' AND SUBSTR(MAT_GRP_4,-1) <> SUBSTR(OPER, -1) THEN '9th'
                                    WHEN OPER IN ('A0559', 'A0609', 'A0509', 'A0539') THEN '9th'
                                    ELSE ' ' 
                                END MAT_GRP_5 
                             , CASE WHEN OPER NOT LIKE 'A05%' AND OPER NOT LIKE 'A06%' THEN (
                                         CASE WHEN MAT_GRP_1 = 'HX' AND HX_COMP_MIN IS NOT NULL
                                                   THEN (CASE WHEN HX_COMP_MIN <> HX_COMP_MAX AND OPER > HX_COMP_MIN AND OPER <= HX_COMP_MAX THEN QTY_1 / NVL(COMP_CNT / 2,1)
                                                              WHEN OPER <= HX_COMP_MAX THEN QTY_1 / NVL(COMP_CNT,1)
                                                              ELSE QTY_1 END)
                                              WHEN OPER <= 'A0395' THEN QTY_1 / NVL(COMP_CNT,1) 
                                              ELSE QTY_1 
                                         END 
                                        ) 
                                    ELSE 0 
                               END AS MAIN_DA 
                             , CASE WHEN OPER LIKE 'A05%' OR OPER LIKE 'A06%' THEN QTY_1 ELSE 0 END AS MAIN_WB 
                          FROM RWIPLOTSTS A
                             , VWIPMATDEF B 
                         WHERE 1=1  
                           AND A.FACTORY = B.FACTORY 
                           AND A.MAT_ID = B.MAT_ID 
                           AND A.FACTORY = 'HMKA1' 
                           AND A.LOT_TYPE = 'W'  
                           AND A.LOT_CMF_5 LIKE 'P%'
                           AND A.LOT_DEL_FLAG = ' '  
                           AND B.MAT_GRP_2 <> '-' 
                           AND B.DELETE_FLAG = ' '  
                           AND REGEXP_LIKE(B.MAT_GRP_5, '1st|Middle*|Merge') 
                           AND A.LOT_CMF_5 LIKE 'P%'
                       ) 
                 WHERE MAT_GRP_5 <> ' '
                 GROUP BY MAT_GRP_1, MAT_GRP_10, MAT_CMF_11, CUST, MAT_GRP_5
               ) G
         WHERE 1=1
           AND A.MAT_GRP_1 = B.MAT_GRP_1(+)
           AND A.MAT_GRP_1 = C.MAT_GRP_1(+)
           AND A.MAT_GRP_1 = D.MAT_GRP_1(+)
           AND A.MAT_GRP_1 = E.MAT_GRP_1(+)
           AND A.MAT_GRP_1 = F.MAT_GRP_1(+)
           AND A.MAT_GRP_1 = G.MAT_GRP_1(+)
           AND A.MAT_GRP_10 = B.MAT_GRP_10(+)
           AND A.MAT_GRP_10 = C.MAT_GRP_10(+)
           AND A.MAT_GRP_10 = D.MAT_GRP_10(+)
           AND A.MAT_GRP_10 = E.MAT_GRP_10(+)
           AND A.MAT_GRP_10 = F.MAT_GRP_10(+)
           AND A.MAT_GRP_10 = G.MAT_GRP_10(+)
           AND A.MAT_CMF_11 = B.MAT_CMF_11(+)
           AND A.MAT_CMF_11 = C.MAT_CMF_11(+)
           AND A.MAT_CMF_11 = D.MAT_CMF_11(+)
           AND A.MAT_CMF_11 = E.MAT_CMF_11(+)
           AND A.MAT_CMF_11 = F.MAT_CMF_11(+)
           AND A.MAT_CMF_11 = G.MAT_CMF_11(+)
           AND A.CUST = B.CUST(+)
           AND A.CUST = C.CUST(+)
           AND A.CUST = D.CUST(+)
           AND A.CUST = E.CUST(+)
           AND A.CUST = F.CUST(+)
           AND A.CUST = G.CUST(+)
           AND A.MAT_GRP_5 = D.STACK(+)
           AND A.MAT_GRP_5 = F.MAT_GRP_5(+)
           AND A.MAT_GRP_5 = G.MAT_GRP_5(+)
           AND A.CUST = 'SEK'
           AND NVL(D.CAPA,0) + NVL(B.WIP_MID_MER,0) + NVL(E.PLN_WEEK1,0) + NVL(E.PLN_WEEK2,0) + NVL(E.PLN_WEEK3,0) + NVL(E.PLN_WEEK4,0) + NVL(E.PLN_MONTH,0) +
               NVL(WIP_STOCK,0) + NVL(WIP_BG,0) + NVL(WIP_SAW,0) + NVL(WIP_LAMI,0) + NVL(WIP_PREBG,0) + NVL(WIP_STEALTH,0) + NVL(WIP_DA_WB,0) + NVL(WIP_FRONT,0) + NVL(B.WIP_BACK,0) +
               NVL(T_OUT_STOCK,0) + NVL(Y_OUT_BG,0) + NVL(T_OUT_BG,0) + NVL(Y_OUT_SAW,0) + NVL(T_OUT_SAW,0) + NVL(C.AO_WEEK,0) + NVL(C.AO_MONTH,0) + NVL(G.MAIN_DA,0) + NVL(G.MAIN_WB,0) > 0
       )
 ORDER BY CUST, MAT_GRP_1, MAT_GRP_10, MAT_CMF_11, MAT_GRP_5


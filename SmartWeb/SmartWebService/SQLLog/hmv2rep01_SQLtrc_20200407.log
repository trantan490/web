     , ROUND(SUM(REV_PLN * CONVERT_QTY) /1000000,0) AS REV_PLN
     , ROUND(SUM(SOP_PLN * CONVERT_QTY) /1000000,0) AS SOP_PLN
     , ROUND(SUM(W0 * CONVERT_QTY) /1000000,0) AS W0

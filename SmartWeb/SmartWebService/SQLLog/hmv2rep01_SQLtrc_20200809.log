                     , SUM(DECODE(WORK_DATE, '20200829', RES_CNT, 0)) AS RES_28
                     , SUM(DECODE(WORK_DATE, '20200830', RES_CNT, 0)) AS RES_29
                     , SUM(DECODE(WORK_DATE, '20200831', RES_CNT, 0)) AS RES_30
                     , SUM(DECODE(WORK_DATE, '20200901', RES_CNT, 0)) AS RES_31
                     , SUM(DECODE(WORK_DATE, '20200801', RUN_CNT, 0)) AS RUN_0
                     , SUM(DECODE(WORK_DATE, '20200802', RUN_CNT, 0)) AS RUN_1
                     , SUM(DECODE(WORK_DATE, '20200803', RUN_CNT, 0)) AS RUN_2
                     , SUM(DECODE(WORK_DATE, '20200804', RUN_CNT, 0)) AS RUN_3
                     , SUM(DECODE(WORK_DATE, '20200805', RUN_CNT, 0)) AS RUN_4
                     , SUM(DECODE(WORK_DATE, '20200806', RUN_CNT, 0)) AS RUN_5

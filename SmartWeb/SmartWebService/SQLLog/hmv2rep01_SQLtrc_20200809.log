                     , SUM(DECODE(WORK_DATE, '20200808', RUN_CNT, 0)) AS RUN_7
                     , SUM(DECODE(WORK_DATE, '20200809', RUN_CNT, 0)) AS RUN_8
                     , SUM(DECODE(WORK_DATE, '20200810', RUN_CNT, 0)) AS RUN_9
                     , SUM(DECODE(WORK_DATE, '20200811', RUN_CNT, 0)) AS RUN_10

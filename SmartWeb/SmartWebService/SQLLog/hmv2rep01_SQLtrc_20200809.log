     , ROUND(SUM(WB6)/1,0) AS WB6 
     , ROUND(SUM(DA6)/1,0) AS DA6 
     , ROUND(SUM(WB5)/1,0) AS WB5 
     , ROUND(SUM(DA5)/1,0) AS DA5 
     , ROUND(SUM(WB4)/1,0) AS WB4 
     , ROUND(SUM(DA4)/1,0) AS DA4 

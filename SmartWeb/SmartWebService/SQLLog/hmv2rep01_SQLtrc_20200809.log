                     , SUM(DECODE(WORK_DATE, '20200805', RES_CNT, 0)) AS RES_4
                     , SUM(DECODE(WORK_DATE, '20200806', RES_CNT, 0)) AS RES_5
                     , SUM(DECODE(WORK_DATE, '20200807', RES_CNT, 0)) AS RES_6
                     , SUM(DECODE(WORK_DATE, '20200808', RES_CNT, 0)) AS RES_7
                     , SUM(DECODE(WORK_DATE, '20200809', RES_CNT, 0)) AS RES_8
                     , SUM(DECODE(WORK_DATE, '20200810', RES_CNT, 0)) AS RES_9
                     , SUM(DECODE(WORK_DATE, '20200811', RES_CNT, 0)) AS RES_10
                     , SUM(DECODE(WORK_DATE, '20200812', RES_CNT, 0)) AS RES_11
                     , SUM(DECODE(WORK_DATE, '20200813', RES_CNT, 0)) AS RES_12

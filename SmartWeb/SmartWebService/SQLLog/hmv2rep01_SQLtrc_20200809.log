                                                             , SUM(DECODE(PLAN_WEEK, '202033', D2_QTY, 0)) AS D9_QTY  
                                                             , SUM(DECODE(PLAN_WEEK, '202033', D3_QTY, 0)) AS D10_QTY  
                                                             , SUM(DECODE(PLAN_WEEK, '202033', D4_QTY, 0)) AS D11_QTY  

                     , SUM(DECODE(WORK_DATE, '20200815', RES_CNT, 0)) AS RES_14
                     , SUM(DECODE(WORK_DATE, '20200816', RES_CNT, 0)) AS RES_15
                     , SUM(DECODE(WORK_DATE, '20200817', RES_CNT, 0)) AS RES_16
                     , SUM(DECODE(WORK_DATE, '20200818', RES_CNT, 0)) AS RES_17
                     , SUM(DECODE(WORK_DATE, '20200819', RES_CNT, 0)) AS RES_18
                     , SUM(DECODE(WORK_DATE, '20200820', RES_CNT, 0)) AS RES_19
                     , SUM(DECODE(WORK_DATE, '20200821', RES_CNT, 0)) AS RES_20
                     , SUM(DECODE(WORK_DATE, '20200822', RES_CNT, 0)) AS RES_21
                     , SUM(DECODE(WORK_DATE, '20200823', RES_CNT, 0)) AS RES_22
                     , SUM(DECODE(WORK_DATE, '20200824', RES_CNT, 0)) AS RES_23
                     , SUM(DECODE(WORK_DATE, '20200825', RES_CNT, 0)) AS RES_24

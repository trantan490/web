     , SUM(DECODE(WORK_DATE, '20200512', HLD.QTY_1, 0)) A9
     , SUM(DECODE(WORK_DATE, '20200513', HLD.QTY_1, 0)) A10
     , SUM(DECODE(WORK_DATE, '20200514', HLD.QTY_1, 0)) A11

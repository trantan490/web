SELECT DWH.TRAN_DATE 
     , DECODE(DWH.RES, 'FLX01A', 'IFLEX RF', 'FLX02A', 'IFLEX RF', 'FLX03A', 'IFLEX RF', 'FLX04A', 'IFLEX RF', 'T2K01A', 'T2000', 'V9301A', 'VERIGY') AS MODEL 
     , LOT.DEVICE
     , DWH.RES
     , LOT.LOT_QTY
     , 24 AS STD
     , ROUND((24 - (MATL + HANDLER + SETUP + ENGINEER + PM_CAM + NO_WORK + TESTER + NOSCHD + OTHER)),2) AS ACT
     , ROUND((24 - (MATL + HANDLER + SETUP + ENGINEER + PM_CAM + NO_WORK + TESTER + NOSCHD + OTHER)) / 24 * 100, 2) AS PER
     , ROUND(RETEST,2) AS RETEST
     , ROUND(QA_TEST,2) AS QA_TEST
     , ROUND(MATL,2) AS MATL
     , ROUND(HANDLER,2) AS HANDLER
     , ROUND(SETUP,2) AS SETUP
     , ROUND(ENGINEER,2) AS ENGINEER
     , ROUND(PM_CAM,2) AS PM_CAM
     , ROUND(NO_WORK,2) AS NO_WORK
     , ROUND(TESTER,2) AS TESTER
     , ROUND(NOSCHD,2) AS NOSCHD
     , ROUND(OTHER,2) AS OTHER
     , ROUND(MATL + HANDLER + SETUP + ENGINEER + PM_CAM + NO_WORK + TESTER + NOSCHD + OTHER, 2) AS TOTAL
  FROM (
        SELECT DOWN_DATE AS TRAN_DATE, RES
             , SUM(DECODE(UP_CODE,'H100',DOWN_TIME,0)) + SUM(DECODE(SUBSTR(UP_CODE,1,2),'H9',DOWN_TIME,0)) AS RETEST
             , SUM(DECODE(UP_CODE,'E300',DOWN_TIME,'H800',DOWN_TIME,0)) AS QA_TEST
             , SUM(DECODE(SUBSTR(UP_CODE,1,2),'H1',DECODE(SUBSTR(UP_CODE,4,1),'0',0,DOWN_TIME),'H2',DECODE(SUBSTR(UP_CODE,4,1),'0',0,DOWN_TIME),'H3',DECODE(SUBSTR(UP_CODE,4,1),'0',0,DOWN_TIME),'H4',DECODE(SUBSTR(UP_CODE,4,1),'0',0,DOWN_TIME),'H6',DOWN_TIME,'H7',DOWN_TIME,0)) AS MATL
             , SUM(DECODE(SUBSTR(UP_CODE,1,2),'A4',DOWN_TIME,0)) AS HANDLER
             , SUM(DECODE(SUBSTR(UP_CODE,1,1),'D',DOWN_TIME,0)) AS SETUP
             , SUM(DECODE(UP_CODE,'E900',DOWN_TIME,0)) AS ENGINEER
             , SUM(DECODE(SUBSTR(UP_CODE,1,1),'C',DOWN_TIME,0)) AS PM_CAM
             , SUM(DECODE(SUBSTR(UP_CODE,1,1),'B',DOWN_TIME,0)) AS NO_WORK
             , SUM(DECODE(SUBSTR(UP_CODE,1,2),'A5',DOWN_TIME,0)) AS TESTER
             , SUM(DECODE(UP_CODE,'E200',DOWN_TIME,0)) AS NOSCHD
             , SUM(DECODE(SUBSTR(UP_CODE,1,1),'F',DOWN_TIME,'G',DOWN_TIME,0)) + SUM(DECODE(SUBSTR(UP_CODE,1,2),'H2',DECODE(SUBSTR(UP_CODE,4,1),'0',DOWN_TIME,0),'H3',DECODE(SUBSTR(UP_CODE,4,1),'0',DOWN_TIME,0),'H4',DECODE(SUBSTR(UP_CODE,4,1),'0',DOWN_TIME,0),'H5',DOWN_TIME,0)) + SUM(DECODE(UP_CODE,'E100',DOWN_TIME,'E400',DOWN_TIME,'E500',DOWN_TIME,'E600',DOWN_TIME,0)) AS OTHER
          FROM (
                SELECT TO_CHAR(TO_DATE(DOWN_TRAN_TIME, 'YYYYMMDDHH24MISS') + 2/24, 'YYYYMMDD') AS DOWN_DATE
                     , RES_ID AS RES
                     , UP_NEW_STS_1 AS UP_CODE
                     , CASE WHEN UP_TRAN_TIME <> ' '
                            THEN ROUND(TO_CHAR(TO_DATE(UP_TRAN_TIME, 'YYYYMMDDHH24MISS') - TO_DATE(DOWN_TRAN_TIME, 'YYYYMMDDHH24MISS'))* 24,3)
                            ELSE ROUND(TO_CHAR(SYSDATE - TO_DATE(DOWN_TRAN_TIME, 'YYYYMMDDHH24MISS')),3)
                       END AS DOWN_TIME
                  FROM CRASRESDWH
                 WHERE 1=1
                   AND FACTORY = 'HMKT1'
                   AND RES_ID IN ('FLX01A','FLX02A','FLX03A','FLX04A','T2K01A','V9301A')
                   AND DOWN_TRAN_TIME BETWEEN '20200331220000' AND '20200414215959'
               )
         GROUP BY DOWN_DATE, RES
       ) DWH
     , (
        SELECT TRAN_DATE, END_RES_ID, DEVICE, SUM(LOT_QTY) AS LOT_QTY
          FROM (
                SELECT TO_CHAR(TO_DATE(TRAN_TIME, 'YYYYMMDDHH24MISS') + 2/24, 'YYYYMMDD') AS TRAN_DATE
                     , LOT.END_RES_ID
                     , MAT.MAT_CMF_7 AS DEVICE
                     , LOT.START_QTY_1 AS LOT_QTY
                  FROM CWIPLOTEND LOT
                     , MWIPMATDEF MAT
                 WHERE 1=1
                   AND LOT.FACTORY=MAT.FACTORY
                   AND LOT.MAT_ID=MAT.MAT_ID
                   AND LOT.FACTORY='HMKT1'
                   AND LOT.OLD_OPER = 'T0100'
                   AND LOT.END_RES_ID IN ('FLX01A','FLX02A','FLX03A','FLX04A','T2K01A','V9301A')
                   AND LOT.TRAN_TIME BETWEEN '20200331220000' AND '20200414215959'
               )
         GROUP BY TRAN_DATE, END_RES_ID, DEVICE
       ) LOT
 WHERE 1=1
   AND DWH.TRAN_DATE=LOT.TRAN_DATE(+)
   AND DWH.RES = LOT.END_RES_ID(+)
 ORDER BY TRAN_DATE, DWH.RES


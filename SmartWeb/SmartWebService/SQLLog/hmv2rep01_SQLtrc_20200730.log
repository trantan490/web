     , SUM(DECODE(WORK_DATE, '20200630', END_QTY)) AS END_QTY_5
     , SUM(DECODE(WORK_DATE, '20200701', END_QTY)) AS END_QTY_6
     , SUM(DECODE(WORK_DATE, '20200702', END_QTY)) AS END_QTY_7
     , SUM(DECODE(WORK_DATE, '20200703', END_QTY)) AS END_QTY_8
     , SUM(DECODE(WORK_DATE, '20200704', END_QTY)) AS END_QTY_9

SELECT REPLACE(A.MAT_ID, '-O', '') AS MAT_ID 
     , (SELECT DATA_1 FROM MGCMTBLDAT WHERE FACTORY = 'HMVA2' AND TABLE_NAME = 'VENDOR' AND KEY_1 = VENDOR_CODE) AS VENDOR_CODE 
     , ROUND(SUM(NVL(C.QTY_TTL,0))/1,0) AS L_IN  
         , ROUND(SUM(CASE  
           WHEN TO_CHAR(ADD_MONTHS(SYSDATE,3), 'YYYYMMDDHH24MISS') < C.LOT_CMF_16 THEN C.QTY_TTL ELSE 0 
            END)/1,0) AS MONTHS4  
         , ROUND(SUM(CASE  
               WHEN C.LOT_CMF_16 <= TO_CHAR(ADD_MONTHS(SYSDATE,3), 'YYYYMMDDHH24MISS') AND TO_CHAR(ADD_MONTHS(SYSDATE,2), 'YYYYMMDDHH24MISS') < C.LOT_CMF_16 THEN C.QTY_TTL ELSE 0 
                END)/1,0) AS MONTHS3  
         , ROUND(SUM(CASE  
               WHEN C.LOT_CMF_16 <= TO_CHAR(ADD_MONTHS(SYSDATE,2), 'YYYYMMDDHH24MISS') AND TO_CHAR(ADD_MONTHS(SYSDATE,1), 'YYYYMMDDHH24MISS') < C.LOT_CMF_16 THEN C.QTY_TTL ELSE 0 
                END)/1,0) AS MONTHS2  
         , ROUND(SUM(CASE  
               WHEN C.LOT_CMF_16 <= TO_CHAR(ADD_MONTHS(SYSDATE,1), 'YYYYMMDDHH24MISS') AND TO_CHAR(SYSDATE, 'YYYYMMDDHH24MISS') < C.LOT_CMF_16 THEN C.QTY_TTL ELSE 0 
                END)/1,0) AS MONTHS1  
         , ROUND(SUM(CASE  
               WHEN C.LOT_CMF_16 <= TO_CHAR(SYSDATE, 'YYYYMMDDHH24MISS') THEN C.QTY_TTL ELSE 0 
                END)/1,0) AS MONTHS0  
  FROM MWIPMATDEF A  
     , (  
        SELECT MAT_ID, LOT_CMF_16, LOT_CMF_20 AS VENDOR_CODE 
             , SUM(CASE WHEN OPER LIKE 'A%' OR OPER > 'M0330' THEN QTY_1 ELSE 0 END) AS QTY_TTL 
          FROM RWIPLOTSTS A  
             , CRASRESMAT B  
         WHERE 1=1   
           AND A.FACTORY = B.FACTORY(+)  
           AND A.LOT_ID = B.LOT_ID(+)  
           AND A.FACTORY = 'HMVA2' 
           AND A.LOT_TYPE != 'W' 
           AND A.LOT_DEL_FLAG = ' ' 
           AND A.LOT_CMF_2 = '-'  
           AND A.LOT_CMF_9 != ' '  
           AND A.QTY_1 > 0  
           AND A.OPER NOT IN  ('00001', '00002', 'V0000')  
         GROUP BY A.MAT_ID, LOT_CMF_16, LOT_CMF_20 
       ) C  
 WHERE 1=1 
   AND A.MAT_ID = C.MAT_ID(+) 
   AND A.FACTORY = 'HMVA2' 
   AND (A.MAT_ID = 'Total' OR  A.MAT_ID = 'Total-O') 
 GROUP BY REPLACE(A.MAT_ID, '-O', ''), VENDOR_CODE 


     , SUM(DECODE(WORK_DATE, '20200707', END_QTY)) AS END_QTY_12
     , SUM(DECODE(WORK_DATE, '20200708', END_QTY)) AS END_QTY_13
     , SUM(DECODE(WORK_DATE, '20200709', END_QTY)) AS END_QTY_14
     , SUM(DECODE(WORK_DATE, '20200710', END_QTY)) AS END_QTY_15
     , SUM(DECODE(WORK_DATE, '20200711', END_QTY)) AS END_QTY_16
     , SUM(DECODE(WORK_DATE, '20200712', END_QTY)) AS END_QTY_17
     , SUM(DECODE(WORK_DATE, '20200713', END_QTY)) AS END_QTY_18
     , SUM(DECODE(WORK_DATE, '20200714', END_QTY)) AS END_QTY_19
     , SUM(DECODE(WORK_DATE, '20200715', END_QTY)) AS END_QTY_20
     , SUM(DECODE(WORK_DATE, '20200716', END_QTY)) AS END_QTY_21
     , SUM(DECODE(WORK_DATE, '20200717', END_QTY)) AS END_QTY_22
     , SUM(DECODE(WORK_DATE, '20200718', END_QTY)) AS END_QTY_23
     , SUM(DECODE(WORK_DATE, '20200719', END_QTY)) AS END_QTY_24
     , SUM(DECODE(WORK_DATE, '20200720', END_QTY)) AS END_QTY_25
     , SUM(DECODE(WORK_DATE, '20200721', END_QTY)) AS END_QTY_26
     , SUM(DECODE(WORK_DATE, '20200722', END_QTY)) AS END_QTY_27
     , SUM(DECODE(WORK_DATE, '20200723', END_QTY)) AS END_QTY_28
     , SUM(DECODE(WORK_DATE, '20200724', END_QTY)) AS END_QTY_29

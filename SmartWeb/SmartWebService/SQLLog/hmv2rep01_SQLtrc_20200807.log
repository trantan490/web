SELECT A.MAT_ID, A.LOT_ID, B.MAT_DESC, A.OPER, A.QTY_1, A.LOT_STATUS, A.LOT_CMF_5 
     , (SELECT DATA_1 FROM MGCMTBLDAT WHERE TABLE_NAME = 'VENDOR' AND FACTORY = A.FACTORY AND KEY_1 = A.LOT_CMF_20) AS VENDOR_NAME 
     , A.LOT_CMF_20 AS VENDOR_CODE 
     , TRUNC(SYSDATE-TO_DATE(A.LAST_TRAN_TIME,'YYYYMMDDHH24MISS')) || '일 ' || 
       TRUNC(MOD((SYSDATE-TO_DATE(A.LAST_TRAN_TIME,'YYYYMMDDHH24MISS')),1)*24)|| '시간 ' || 
       TRUNC(MOD((SYSDATE-TO_DATE(A.LAST_TRAN_TIME,'YYYYMMDDHH24MISS'))*24,1)*60)|| '분 ' 
       AS TRAN_INTERVAL 
     , TRUNC(SYSDATE-TO_DATE(A.CREATE_TIME,'YYYYMMDDHH24MISS')) || '일 ' || 
       TRUNC(MOD((SYSDATE-TO_DATE(A.CREATE_TIME,'YYYYMMDDHH24MISS')),1)*24)|| '시간 ' || 
       TRUNC(MOD((SYSDATE-TO_DATE(A.CREATE_TIME,'YYYYMMDDHH24MISS'))*24,1)*60)|| '분 ' 
       AS CREATE_INTERVAL 
     , A.HOLD_FLAG, A.HOLD_CODE 
  FROM RWIPLOTSTS A 
     , MWIPMATDEF B 
 WHERE 1=1 
   AND A.FACTORY = B.FACTORY
   AND A.MAT_ID = B.MAT_ID
   AND A.FACTORY = 'HMVA2'
   AND A.LOT_TYPE = 'P'
   AND A.LOT_DEL_FLAG = ' '
   AND A.LOT_CMF_2 = '-'
   AND A.LOT_CMF_9 != ' '
   AND A.QTY_1 > 0
   AND A.OPER <>  'V0000'
   AND (A.OPER LIKE 'M%' OR A.OPER LIKE 'V%')
   AND B.DELETE_FLAG = ' '
   AND A.MAT_ID LIKE '%'
   AND A.OPER > 'M0330'
 ORDER BY A.MAT_ID, A.OPER, A.LOT_ID


                                                                WHEN CALENDAR_ID = 'OTD' AND TO_CHAR(TO_DATE('20200807', 'YYYYMMDD'), 'D') = 6 THEN D0_QTY + D1_QTY + D2_QTY + D3_QTY + D4_QTY + D5_QTY 
                                                                WHEN CALENDAR_ID = 'SE' AND TO_CHAR(TO_DATE('20200807', 'YYYYMMDD'), 'D') = 3 THEN D0_QTY 

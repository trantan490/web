         WHERE 1=1 
           AND TRAN_TIME BETWEEN '20200806220000' AND '20200807215959' 
           AND OLD_FACTORY = 'HMVA2' 
           AND TRAN_CODE IN ('END','SHIP') 
           AND LOT_TYPE = 'W' 

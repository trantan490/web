     , ROUND(SUM(RCV_5 * PRICE) / 1, 0)  AS RCV_5
     , ROUND(SUM(RCV_6 * PRICE) / 1, 0)  AS RCV_6
     , ROUND(SUM(RCV_7 * PRICE) / 1, 0)  AS RCV_7

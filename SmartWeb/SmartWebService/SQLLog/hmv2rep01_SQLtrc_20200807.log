     , SUM(D5_TIME) AS D5_TIME
     , SUM(D6_TIME) AS D6_TIME
     , SUM(D7_TIME) AS D7_TIME
     , SUM(D8_TIME) AS D8_TIME
     , SUM(D9_TIME) AS D9_TIME
     , SUM(D1_WAIT_TIME) AS D1_WAIT_TIME
     , SUM(D2_WAIT_TIME) AS D2_WAIT_TIME
     , SUM(D3_WAIT_TIME) AS D3_WAIT_TIME
     , SUM(D4_WAIT_TIME) AS D4_WAIT_TIME
     , SUM(D5_WAIT_TIME) AS D5_WAIT_TIME
     , SUM(D6_WAIT_TIME) AS D6_WAIT_TIME
     , SUM(D7_WAIT_TIME) AS D7_WAIT_TIME
     , SUM(D8_WAIT_TIME) AS D8_WAIT_TIME
     , SUM(D9_WAIT_TIME) AS D9_WAIT_TIME
  FROM (
        SELECT WORK_DATE
             , (SELECT DATA_1 FROM MGCMTBLDAT WHERE FACTORY = A.FACTORY AND TABLE_NAME='H_DEPARTMENT' AND KEY_1 = A.RES_GRP_1) AS DEPART
             , (SELECT DATA_1 FROM MGCMTBLDAT WHERE FACTORY = A.FACTORY AND TABLE_NAME='H_DEPARTMENT' AND KEY_1 = A.RES_GRP_2) AS PART
             , RES_GRP_3
             , RES_ID
             , SUM(DECODE(MAINT_STEP, 'D_WAIT', FREQUENCY, 0)) AS CNT
             , ROUND(SUM(TIME_SUM) / 60, 0) AS TTL_TIME
             , ROUND(SUM(DECODE(MAINT_STEP, 'D_WAIT', TIME_SUM, 0)) / 60, 0) AS WAIT_TIME 
             , SUM(CASE WHEN GROUP_CODE = 'D1' AND MAINT_STEP = 'D_WAIT' THEN FREQUENCY ELSE 0 END) AS D1_CNT 
             , SUM(CASE WHEN GROUP_CODE = 'D2' AND MAINT_STEP = 'D_WAIT' THEN FREQUENCY ELSE 0 END) AS D2_CNT 
             , SUM(CASE WHEN GROUP_CODE = 'D3' AND MAINT_STEP = 'D_WAIT' THEN FREQUENCY ELSE 0 END) AS D3_CNT 
             , SUM(CASE WHEN GROUP_CODE = 'D4' AND MAINT_STEP = 'D_WAIT' THEN FREQUENCY ELSE 0 END) AS D4_CNT 
             , SUM(CASE WHEN GROUP_CODE = 'D5' AND MAINT_STEP = 'D_WAIT' THEN FREQUENCY ELSE 0 END) AS D5_CNT 
             , SUM(CASE WHEN GROUP_CODE = 'D6' AND MAINT_STEP = 'D_WAIT' THEN FREQUENCY ELSE 0 END) AS D6_CNT 
             , SUM(CASE WHEN GROUP_CODE = 'D7' AND MAINT_STEP = 'D_WAIT' THEN FREQUENCY ELSE 0 END) AS D7_CNT 
             , SUM(CASE WHEN GROUP_CODE = 'D8' AND MAINT_STEP = 'D_WAIT' THEN FREQUENCY ELSE 0 END) AS D8_CNT 
             , SUM(CASE WHEN GROUP_CODE = 'D9' AND MAINT_STEP = 'D_WAIT' THEN FREQUENCY ELSE 0 END) AS D9_CNT 
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D1' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D1_TIME 
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D2' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D2_TIME 
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D3' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D3_TIME 
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D4' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D4_TIME 
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D5' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D5_TIME 
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D6' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D6_TIME 
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D7' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D7_TIME 
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D8' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D8_TIME 
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D9' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D9_TIME 
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D1' AND MAINT_STEP = 'D_WAIT' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D1_WAIT_TIME 
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D2' AND MAINT_STEP = 'D_WAIT' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D2_WAIT_TIME 
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D3' AND MAINT_STEP = 'D_WAIT' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D3_WAIT_TIME  
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D4' AND MAINT_STEP = 'D_WAIT' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D4_WAIT_TIME  
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D5' AND MAINT_STEP = 'D_WAIT' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D5_WAIT_TIME  
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D6' AND MAINT_STEP = 'D_WAIT' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D6_WAIT_TIME  
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D7' AND MAINT_STEP = 'D_WAIT' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D7_WAIT_TIME  
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D8' AND MAINT_STEP = 'D_WAIT' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D8_WAIT_TIME  
             , ROUND(SUM(CASE WHEN GROUP_CODE = 'D9' AND MAINT_STEP = 'D_WAIT' THEN TIME_SUM ELSE 0 END) / 60, 0) AS D9_WAIT_TIME  
          FROM (
                SELECT MNT.*, RES.RES_GRP_1, RES.RES_GRP_2, RES.RES_GRP_3
                     , SUBSTR(MNT.MAINT_CODE, 1, 2) AS GROUP_CODE
                  FROM CSUMRESMNT@RPTTOMES MNT
                     , MRASRESDEF RES
                 WHERE 1=1
                   AND MNT.FACTORY = RES.FACTORY
                   AND MNT.RES_ID = RES.RES_ID
                   AND RES.RES_CMF_20 < '20200805220000'
                   AND WORK_DATE BETWEEN '20200806' AND '20200807'
                   AND MNT.FACTORY = 'HMVA2' 
                   AND MAINT_CODE LIKE 'D%'
               ) A
         GROUP BY WORK_DATE, FACTORY, RES_GRP_1, RES_GRP_2, RES_GRP_3, RES_ID
       )
 GROUP BY WORK_DATE, DEPART, PART, RES_GRP_3, RES_ID
 ORDER BY WORK_DATE, DEPART, PART, RES_GRP_3, RES_ID


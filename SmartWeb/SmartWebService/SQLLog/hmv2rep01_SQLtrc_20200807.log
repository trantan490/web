             , CASE WHEN MAT_GRP_3 IN ('COB', 'BGN') THEN ROUND(PLN.REV_PLAN/DECODE(MAT_CMF_13,' ', 1, '-', 1, MAT_CMF_13),0) ELSE PLN.REV_PLAN END AS REV_PLAN

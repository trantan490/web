                                                                WHEN CALENDAR_ID = 'SE' AND TO_CHAR(TO_DATE('20200807', 'YYYYMMDD'), 'D') = 2 THEN D0_QTY 
                                                                WHEN CALENDAR_ID = 'SE' AND TO_CHAR(TO_DATE('20200807', 'YYYYMMDD'), 'D') = 3 THEN D1_QTY 
